#  speaker 
#  warnings: 0 
#: sc/0172:0 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0172:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0172:1 
msgid "きよし"
msgstr "Kiyoshi"

#  text 
#  warnings: 0 
#: sc/0172:1 
msgid "あれ？　今、バディがいた気が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:2 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:2 
msgid "気のせいだよね。\nバディはうちにいるはずだし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:3 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:3 
msgid "……でも、最近バディは突然どこかに\n走り出したりするんだよな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:4 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:4 
msgid "前に見つけた時は……\n何か変なことしてたし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:5 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:5 
msgid "もしかしてこれから\nまたあんなことをしに行くつもりじゃ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:6 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:6 
msgid "それとも、全く別のことをするために\nどこかに行くところだったかも？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:7 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:7 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:8 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:8 
msgid "よし、行ってみよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:9 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:9 
msgid "バディ、バディ―。\nどこにいるんだ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:10 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:10 
msgid "見つからない……\n一体どこに行ったんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:11 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:11 
msgid "あっ、あの人に聞いてみよう。\nすみませ―ん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:12 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:12 
msgid "あの―、さっきこの辺りに\n犬が走ってきませんでした？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:13 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0172:13 
msgid "え？　犬ですか？\nう〜ん、見ていませんねぇ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:14 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:14 
msgid "そうですか……\nありがとうございました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:15 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:15 
msgid "（次はあっちの方に行ってみようかな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:16 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:16 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:17 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:17 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:18 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:18 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:19 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:19 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:20 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:20 
msgid "（！？\n　なんで僕の後をついてきてるの！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:21 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:21 
msgid "（ハッ！？　ま、まさか……！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:22 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:22 
msgid "……どうかしましたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:23 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:23 
msgid "う、う……うおおおおおお！！\n先手必勝―――！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0172:24 
msgid "えええ――！？\nなんか急に襲ってきた――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0172:25 
msgid "こ、これはオヤジ狩りか！？\nこの子供、この歳でなんてワルなんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0172:26 
msgid "くそぉ！　もうダメだ――！！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0172:27 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0172:27 
msgid "うおあああああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:28 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:28 
msgid "あの―、さっきこの辺りに\n犬がいたはずなんだけど見ませんでした？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:29 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0172:29 
msgid "はい？　犬？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:30 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:30 
msgid "僕んちの犬なんですけど、さっきこっちに\n走っていったのが見えた気がして……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:31 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:31 
msgid "でも、消えちゃったみたいに\n姿が見えなくなって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:32 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:32 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0172:33 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:33 
msgid "もしかしたら、あれは幻だったのかも。\nだから本当に消えちゃって……なんて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:34 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:34 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:35 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:35 
msgid "声をかけてごめんなさい。\n僕、行きますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:36 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:36 
msgid "あっ。待ちなさい。\n私はその犬のことを知ってますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:37 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:37 
msgid "えっ？　本当！？\nバディはどこに！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:38 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:38 
msgid "今ここに、私たちの目の前にいます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:39 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:39 
msgid "え？　どこにもいないよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:40 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:40 
msgid "それはこの犬が霊体だからです。\nあなたが心配であの世から見に来たそうです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:41 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:41 
msgid "……えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:42 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:42 
msgid "私はイタコをやっているので\nこの犬の霊を降ろし、話をさせましょう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:43 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:43 
msgid "な、何を言って弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:44 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:44 
msgid "うう……ご主人様と喋れる日が来るなんて\n夢のようだワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:45 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:45 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0172:46 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:46 
msgid "元気にしてたワンか？\n風邪とか引いてないワンか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:47 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:47 
msgid "いや、何してるの？　いくら僕が子供でも\nそんな小芝居に佶されないよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:48 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:48 
msgid "小芝居じゃないワン。イタコさんが霊の僕に\n体を貸して話をさせてくれてるんだワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:49 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:49 
msgid "ふ―ん。じゃあお前の名前は？\n本物なら言えるよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:50 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:50 
msgid "もちろんだワン。\n僕の名前はバディだワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:51 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:51 
msgid "（くっ！　さっきちょっと言ったのを\n　しっかり覚えている！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:52 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:52 
msgid "そ、そうだ。もう１つ質問。\nうちで飼ってるもう１匹の名前は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:53 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:53 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:54 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:54 
msgid "本当にバディなら\nこれくらいわかるよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:55 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:55 
msgid "……カバディ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:56 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:56 
msgid "惜しい！　でも間違い！\n正解はコバディだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:57 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:57 
msgid "知ってるワン。だからさっき\nコバディって言ったワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:58 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:58 
msgid "えっ？　だって、今、カバディって弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:59 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:59 
msgid "聞き間違いだワン。\nコバディって言ったワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:60 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:60 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:61 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:61 
msgid "これで僕のこと、\nバディだって信じてくれたワン？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:62 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:62 
msgid "くっ……なんて汚い大人なんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:63 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:63 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:64 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:64 
msgid "あっ、バディ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:65 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:65 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:66 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:66 
msgid "おまえ、どこ行ってたんだ。\n……でも、良い所に来てくれたよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:67 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:67 
msgid "イタコさん、これが本物のバディだ！\n小芝居だってバレた弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:68 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:69 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:69 
msgid "消えた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:70 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:70 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:71 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:71 
msgid "まさか、幽霊……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:72 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:72 
msgid "お喋りできるようになって嬉しいワン。\nこのイタコさんに感謝だワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:73 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:73 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:74 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:74 
msgid "（イタコ……聞いたことがある。\n　死んだ人の霊を呼ぶ人だっけ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:75 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:75 
msgid "（でもバディは死んでないし……\n　この人、何してるんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:76 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:76 
msgid "今日はご主人様が心配で見に来たけど、\n立派に成長しててうれしいワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:77 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:77 
msgid "僕はこれで安心して成仏できるワン。\nイタコさんに感謝するワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:78 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:78 
msgid "あの、ちょっといい？\nその犬の霊だけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:79 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:79 
msgid "ああぁ……ふぅ。\n動物霊を降ろすのは疲れる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:80 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:80 
msgid "犬と話ができたようで良かった。\nそれじゃあお代、５千円もらいます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:81 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:81 
msgid "えっ！？　ごごごご、５千円！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:82 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:82 
msgid "無理だよ！　というか、嫌だよ！\nどう見てもインチキじゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:83 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:83 
msgid "ご主人様。インチキなんて言っちゃ駄目だワン。\nそれにお金を払わないのもよくないワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:84 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:84 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:85 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:85 
msgid "……５千円。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:86 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:86 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:87 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:87 
msgid "あの、何してるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:88 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:88 
msgid "ご主人様はイタコを知らないのかワン？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:89 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:89 
msgid "一応、聞いたことはあるけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:90 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:90 
msgid "僕は今このイタコさんの体を借りて\n話をしているんだワン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0172:91 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0172:91 
msgid "はぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
