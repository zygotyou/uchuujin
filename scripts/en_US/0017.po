#  speaker 
#  warnings: 0 
#: sc/0017:53 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:53 
msgid "あの……じ、実は、その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:54 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:54 
msgid "ずっと好きだったんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:55 
msgid "？\n"
