msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0017:0 
msgid "２０、２１、２２……\n何回数えても２２枚だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:1 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:1 
msgid "原稿は全部で２３ペ―ジ。\nだからあと１枚あれば応募できる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:2 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:2 
msgid "タイムリミットも迫ってきたし、\n最悪、描き直しというのもアリだけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:3 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:3 
msgid "や、でもその前にこの辺りを調べよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:4 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:4 
msgid "ここでゆっことぶつかったから……\n多分、あるとしたらこの辺りに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:5 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0017:5 
msgid "む？　そこにいるのは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:6 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:6 
msgid "え？　こ、この声……笹原先輩！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:7 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:7 
msgid "いつぞやの娘だな。\nその節は世話になった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:8 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:8 
msgid "い、い、いえ、そ、そんな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:9 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:9 
msgid "（どうしよう〜！\n　笹原先輩とこんなところで会うなんて！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:10 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:10 
msgid "（こ、これはお話しするチャンスかも……\n　で、でも、今は原稿も大事だし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:11 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0017:11 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0017:12 
#, fuzzy
msgid "コジロウ"
msgstr "Koujirou"

#  text 
#  warnings: 0 
#: sc/0017:12 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0017:13 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:13 
msgid "む、コジロウ、どうしたのだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:14 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:14 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:15 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:15 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:16 
msgid "みお＆笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:16 
msgid "また、鹿が！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:17 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:17 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:18 
msgid "みお＆笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:18 
msgid "犬まで！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:19 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:19 
msgid "……あれ、この犬？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:20 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:20 
msgid "知っておるのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:21 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:21 
msgid "あ、はい。\n最近よく見かける犬なんですけど弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0017:22 
msgid "ああっ、鹿とヤギが激突した―！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0017:23 
msgid "……犬も巻き込まれた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:24 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:24 
msgid "（どうしよう……どうしよう……\n　どうし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:25 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:25 
msgid "よ……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:26 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:26 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:27 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:27 
msgid "（え―――――――――――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:28 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:28 
msgid "む？　どうしたのだ、急に珍妙な顔をして。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:29 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:29 
msgid "あっ……\nい、いえ！　なんでもないです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:30 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:30 
msgid "（あのヤギ、私の原稿食べてる……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:31 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:31 
msgid "（最後の原稿、見つかったけど……\n　今まさに食べられてなくなる寸前……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:32 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:32 
msgid "（取り返さないと！　ヤギを投げ飛ばしてでも\n　取り返さないと！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:33 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:33 
msgid "（あ、でも、そんなことしたら\n　笹原先輩に嫌われちゃうかも……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:34 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:34 
msgid "（ああっ！　今、端っこちょっと食べた！？\n　このままじゃ絶対全部食べられる……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:35 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:35 
msgid "（どうする……？　どうする……！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:36 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:36 
msgid "さ、笹原先輩！！\nヤ、ヤギが落ちてる紙を食べてますよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:37 
msgid "？？\n…………ああ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:38 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:38 
msgid "お腹壊しちゃダメですから、取りますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:39 
msgid "うむ、すまないな娘よ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:40 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:40 
msgid "いえ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:41 
msgid "（やった―！\n　怪しまれずに原稿を奪取できた―！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:42 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:42 
msgid "何やらマンガの一枚のようでな。\n私がモデルとして描かれていたのだが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:43 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:43 
msgid "…………！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:44 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:44 
msgid "……み、見たんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:45 
msgid "うむ。何とも面妖な話で弌弌ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:46 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:46 
msgid "しょらっ！！\n"
