msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:61 
msgid "ふむ…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:62 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:62 
msgid "このコジロウは、\n私の兄弟も同然の存在なのだが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:63 
msgid "よかろう。\n娘、そなたの想い、確かに受け取った。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:64 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:64 
msgid "さあ、コジロウ。\n娘のもとへ行くが良い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:65 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:66 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:66 
msgid "娘よ。コジロウを……\n大切にしてやってくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:67 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:67 
msgid "は、はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:68 
msgid "（良かった〜。\n　やっぱり思いきって言ってみるもんだね）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:69 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:69 
msgid "（人間、何事もど―んと\n　ぶつかっていかなきゃ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:70 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:70 
msgid "…………ゲップ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:71 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:71 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:72 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:72 
msgid "？　どうした、顔色が悪いぞ。\n体調がすぐれんのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:73 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:73 
msgid "え、あ、いや、その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:74 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:74 
msgid "身体もふるえているではないか。\n一体どうしたというのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:75 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:75 
msgid "うぅ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:76 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:76 
msgid "（私なんかの心配をしてくれて……\n　やっぱり先輩は優しい……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:77 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:77 
msgid "（なのに、私は笹原先輩を使って\n　マンガなんか描いて……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:78 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:78 
msgid "うう……ぐすっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:79 
msgid "みお＆笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:79 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:80 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:80 
msgid "笹原！　あんた、\nなに１年の子いじめてるのよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:81 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:81 
msgid "それは誤解だ、立花みさと。\n私はイジメていたわけではない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:82 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:82 
msgid "この者の体調がすぐれぬようなので、\n心配して弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:83 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:83 
msgid "なによ！　あんたなんかに心配されたら\n治るものも治らないわよ！　ねっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:84 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:84 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:85 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:85 
msgid "（原稿……黒こげ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:86 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:86 
msgid "ほら見なさい！\n落ち込んじゃったじゃない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:87 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:87 
msgid "……この場は退いた方が良さそうだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:88 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:88 
msgid "そこの娘よ、すまなかったな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:89 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:89 
msgid "立花みさとも悪気があったわけではないのだ。\n許してやってくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:90 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:90 
msgid "あ、ちょっと笹原待ちなさい！\n話はまだ終わってないわよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:91 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:91 
msgid "…………。\n"
msgstr ""
