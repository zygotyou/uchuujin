msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0017:51 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0017:51 
msgid "あ、あの、笹原先輩、\n大事な話があるんです！\n"
