#  speaker 
#  warnings: 1 
#: sc/0034:0 
msgid "大工"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:0 
msgid "いや―、囲碁サッカ―って中楽しいな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:1 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0034:1 
msgid "そうでしょう？\nこれほど奥が深いスポ―ツ他にありませんよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:2 
msgid "関口"
msgstr "Sekiguchi"

#  text 
#  warnings: 0 
#: sc/0034:2 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0034:3 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:3 
msgid "いや―、でも今日の練習は残念だった。\n盛り上がってきた所で下校時間なんてな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:4 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:4 
msgid "オレも高崎先生に教わったこと、\nもっと試したかったんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:5 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:5 
msgid "お、桜井くん、あそこに公園があるぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:6 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:6 
msgid "……やります？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:7 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:7 
msgid "ああ！\n関口もやるよな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:8 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:8 
msgid "私はいいです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:9 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:9 
msgid "そっか……じゃあ関口はマネ―ジャ―だな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:10 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:10 
msgid "それも嫌です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:11 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:11 
msgid "も―、そういうこと言うなって。\nとりあえずオレたちは練習してくから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:12 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:12 
msgid "とりあえずリフティングからやりましょうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:13 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:13 
msgid "よっ、は……っ！\nここで碁石を……っと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:14 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:14 
msgid "よしっ、ここで高く上げて……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:15 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:15 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:16 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:16 
msgid "ここでシュ―ト！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:17 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:17 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0034:18 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:18 
msgid "あっ、危ない！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:19 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:19 
msgid "だ、誰か、当たった！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:20 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:20 
msgid "うわっ、校長先生ですよ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:21 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:21 
msgid "だ、大丈夫ですか……？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:22 
msgid "アンテナが折れた―！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:23 
msgid "くそっ、あの球体はこの星の兵器なのか！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:24 
msgid "いや、誰がどう見てもサッカ―ボ―ルだろ！\nただの遊び道具だ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:25 
msgid "そんな、じゃあアンテナの\n強度に問題があったとでも！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:26 
msgid "どう考えてもそうだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:27 
msgid "なんせ、ザ・ズボラと呼ばれる俺が\n組み立てたんだからな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:28 
msgid "（そんなヤツに任せるなよ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:29 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:29 
msgid "ふぅ……これくらいにしとこうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:30 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:30 
msgid "そうですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:31 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:31 
msgid "……お疲れ様です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:32 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:32 
msgid "何だ関口、待っててくれたのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:33 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:33 
msgid "これを。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:34 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:34 
msgid "あ、タオルとジュ―スだ。\nありがとうございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:35 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:35 
msgid "部長も、どうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:36 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:36 
msgid "なんだ、用意いいじゃないか関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:37 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:37 
msgid "待ってる間、暇だったので。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:38 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:38 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:39 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:39 
msgid "なあ、関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:40 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:40 
msgid "やっぱりお前、マネ―ジャ―向いてるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:41 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:41 
msgid "……言うと思いました。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0034:42 
msgid "大工"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0034:42 
msgid "ってことはつまり、\n実はやる気満だったんだな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:43 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:43 
msgid "いえ、やる気は無いです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:44 
msgid "なんだよそれ―、変なヤツだな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:45 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:45 
msgid "……そうだ、部長！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:46 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:46 
msgid "どうした桜井くん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:47 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:47 
msgid "だったら、これはどうですか？\n……ごにょごにょ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:48 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:48 
msgid "……なるほど、それだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:49 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:49 
msgid "嫌な予感がするので先に断ってもいいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:50 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:50 
msgid "いいから聞け。\n関口はマネ―ジャ―が嫌なんだよな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:51 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:51 
msgid "いえ、マネ―ジャ―がと言うわけでは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:52 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:52 
msgid "今日からお前は\n時定高校囲碁サッカ―部の監督だ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:53 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:53 
msgid "監督？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:54 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:54 
msgid "そう！　高崎先生はあくまで臨時顧問！\nつまりうちの部には監督が居ないんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:55 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:55 
msgid "そこで関口さんが監督になってくれれば\n全部解決するんですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:56 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:56 
msgid "全然解決してないような……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:57 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:57 
msgid "おいおい何言ってんだよ監督！\n明日、就任の挨拶をしてもらうからな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:58 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:58 
msgid "これで必要な役職が全部揃いましたね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:59 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:59 
msgid "ああ、これでうちの部も安泰だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:60 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:61 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:61 
msgid "やっぱマネ―ジャ―に向いてるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:62 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:62 
msgid "なりませんから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:63 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:63 
msgid "わかってる！\nもうマネ―ジャ―になれとは言わないって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:64 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:64 
msgid "その代わり、\n俺にマネ―ジャ―のやり方を教えてくれ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:65 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:65 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:66 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:66 
msgid "えぇっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:67 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:67 
msgid "部長兼マネ―ジャ―とか凄くないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:68 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:68 
msgid "ついでに囲碁サッカ―自体もちょっとやるから\nなんとプレイングマネ―ジャ―だぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:69 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:69 
msgid "え、え―っと……プレイングマネ―ジャ―って\nそういう意味でしたっけ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:70 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:70 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:71 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:71 
msgid "そうだろ？\nだから関口、俺にマネ―ジャ―道を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:72 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:72 
msgid "私がやります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:73 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:73 
msgid "いや、関口今やらないって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:74 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:74 
msgid "私がやりますから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:75 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:75 
msgid "そうか、ならお前に任せるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:76 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:76 
msgid "よ―し、マネ―ジャ―も決まったことだし、\nそろそろ帰ろうぜ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:77 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:77 
msgid "……はぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:78 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:78 
msgid "……お疲れ様です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:79 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:79 
msgid "やっぱりマネ―ジャ―、やってくれないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:80 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:80 
msgid "無理ですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:81 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:81 
msgid "そう言わずにさ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:82 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:82 
msgid "そう言われても……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:83 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:83 
msgid "めんどくさい仕事押し付けないからさ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:84 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:84 
msgid "めんどくさいからってわけでは\nないんですが……やはりちょっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:85 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:85 
msgid "どうしても？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:86 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:86 
msgid "どうしてもです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:87 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:87 
msgid "なってくれたら\n言うこと何でも聞くって言っても？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:88 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:88 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:89 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:89 
msgid "なりません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:90 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:90 
msgid "（今一瞬悩んだ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:91 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:91 
msgid "それじゃしょうがないな！\nさ、帰ろうぜ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:92 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:92 
msgid "はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0034:93 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0034:93 
msgid "（いいの！？）\n"
msgstr ""
