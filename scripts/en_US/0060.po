#  speaker 
#  warnings: 0 
#: sc/0060:0 
msgid "女子生徒（女子生徒１）"
msgstr "Female student #1"

#  text 
#  warnings: 0 
#: sc/0060:0 
msgid "弌弌でね、電車で１時間もかかっちゃって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:1 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0060:1 
msgid "うわ―、大変だったんですね〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:2 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:2 
msgid "……あれ、休み時間終わっちゃった。\nそれじゃ、遅れないように急いでくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:3 
msgid "女子生徒（女子生徒１）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:3 
msgid "は―い！\nじゃあね、先生。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:4 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:4 
msgid "授業、しっかり受けてくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:5 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0060:6 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:6 
msgid "さてと。この時間は授業は無いし、\n職員室でゆっくりしてようかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:7 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:7 
msgid "……それとも\n校内の見回りでもした方がいいかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:8 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:8 
msgid "あ、でも授業中だし……大丈夫かな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:9 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:9 
msgid "ううん、授業中でも\n何かあるかもしれないもの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:10 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:10 
msgid "もしも、なにか問題があったら、\n生活指導担当としてビシッと対応しないと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:11 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:11 
msgid "よ―し弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0060:12 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:13 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:13 
msgid "あっ……ワンちゃんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:14 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:14 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:15 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:15 
msgid "わ〜、可愛い。\nな、撫でてもいいかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:16 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:16 
msgid "……あれ？\n何か、折れたような音が……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0060:17 
msgid "是非とも俺を撫でてくれ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0060:18 
msgid "いや待て！　俺が先だ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0060:19 
msgid "落ち着けお前ら！\n一番は俺に決まってるだろ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0060:20 
msgid "何だと！　若造は引っ込んでろ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0060:21 
msgid "そうだそうだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0060:22 
msgid "もう！　後で俺が撫でてやるから！\n今の状況を理解しろぉぉお！\n"
msgstr ""

#  speaker 
#  warnings: 3 
#: sc/0060:23 
msgid "＆＆"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0060:23 
msgid "……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:24 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0060:24 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0060:25 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:25 
msgid "あれ、相生さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:26 
msgid "あっ、桜井先生。どうしたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:27 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:27 
msgid "ちょっと時間が空いたから\n校内の見回りでもしようかな―って思って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:28 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:28 
msgid "それで、相生さんは何してるんですか？\nもう授業始まってますよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:29 
msgid "あはは―、恥ずかしながら、\n宿題を忘れて廊下に立たされちゃって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:30 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:30 
msgid "そうなんですか……\nダメですよ、宿題はちゃんとしてこないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:31 
msgid "いやあ、やろうやろうとは思うんですけど、\nなかなかやる気になれなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:32 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0060:32 
msgid "こら！　何喋ってるんだ！\n授業中は静かにしないか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:33 
msgid "あっ！　ス、スイマセン！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:34 
msgid "――――！！！！？？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:35 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:35 
msgid "さ、桜井先生！？　な、何故ここに！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:36 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:36 
msgid "あ、あの……校内の見回りをしていて……\nそしたら相生さんがいて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:37 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:37 
msgid "そ、そうそう。それで、宿題してないから\n怒られてただけで、私語じゃないですよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:38 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:38 
msgid "なっ！？　ほ、本当ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:39 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:39 
msgid "は、はい……スイマセン！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:40 
msgid "いえ！　見回り、ご苦労様です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:41 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:41 
msgid "更に相生をしかってくれて……\nなのに俺ってやつは失礼にも怒鳴ったりして！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:42 
msgid "申し訳ありませんでした！\n高崎学、廊下に立っています！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:43 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0060:44 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:44 
msgid "そ、それなら私も廊下に立ってます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:45 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:45 
msgid "いや、桜井先生が立つことはないでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:46 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:46 
msgid "そうです！　桜井先生にはまだ\n校内の見回りという大役が残っています！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:47 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:47 
msgid "で、でも……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:48 
msgid "わかりました！　では桜井先生の分も俺が！\n相生、バケツに水入れて持って来い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:49 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:49 
msgid "えっ？　あ、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:50 
msgid "は、はい、高崎先生っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:51 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:51 
msgid "よしっ！　ぬおお！！\nこの重さが桜井先生の分……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:52 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:52 
msgid "相生さん！\n私にも同じもの持って来てください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:53 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:53 
msgid "えぇっ！？　は、は、は、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:54 
msgid "はぁ、はぁ、はぁ！\nも、持って来ました！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:55 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:55 
msgid "うぅぅ……お、重い……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:56 
msgid "桜井先生、頑張って！\n私も頑張ります！　どりゃ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:57 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:57 
msgid "ううう―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:58 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:58 
msgid "うおおお―――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:59 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0060:59 
msgid "（授業しろよ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:60 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:60 
msgid "あ、あの……\n高崎先生がそこまでしなくても……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:61 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:61 
msgid "いえ！　そう言うわけにはいきません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:62 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:63 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:63 
msgid "高崎先生―、\n授業やらないんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:64 
msgid "今それどころじゃない！\n国語の授業なんか、後回しだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:65 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:65 
msgid "……これは……いったい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:66 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:66 
msgid "これは……その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:67 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:67 
msgid "俺は……俺はなんてバカなことを……\n注意してくれた桜井先生にどなるなんて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:68 
msgid "ゆっこ＆みお"
msgstr "Yuuko & Mio"

#  text 
#  warnings: 0 
#: sc/0060:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:69 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:69 
msgid "……え、えっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:70 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:70 
msgid "それじゃあ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:71 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:71 
msgid "はい、みんな席についてくださ〜い。\nそれでは、これから英語の授業をしますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:72 
msgid "ゆっこ＆みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:72 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:73 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:73 
msgid "あ、あの……高崎先生が廊下に立ってると\n授業が進まないですから……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:74 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:74 
msgid "それなら大丈夫です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:75 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:75 
msgid "おいお前たち、後は自習だ！\nプリントでもやってろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:76 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:76 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:77 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:77 
msgid "これで問題ありません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:78 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:78 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:79 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:79 
msgid "授業が終わるまでずっと立ってます！\nですので、桜井先生は引き続き見回りを！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:80 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:81 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:81 
msgid "は、はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:82 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:82 
msgid "あっ、授業が終わった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:83 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:83 
msgid "……見回りも無事に終わったから、\nちょうど良かったかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:84 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:84 
msgid "あ、そういえば、高崎先生は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:85 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:85 
msgid "ぬおおおお！！\nバケツに……バケツに負けてたまるか―っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0060:86 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0060:86 
msgid "え――っ！？\n"
msgstr ""
