#  speaker 
#  warnings: 0 
#: sc/0171:0 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0171:0 
msgid "さあいらっしゃい、白いよ―、丸いよ―。\nその上黒くて甘いよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:1 
msgid "はかせ"
msgstr "Professor"

#  text 
#  warnings: 0 
#: sc/0171:1 
msgid "あっ、やたいだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:2 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:2 
msgid "ははは、お嬢ちゃん。\n屋台じゃなくて大福屋さんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:3 
msgid "あっ、ホントだ！\n大福がいっぱい売ってる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:4 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:4 
msgid "ああ、大福屋さんだからね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:5 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:5 
msgid "すごいすごい！　大福屋さんだって！\nこれはなのに教えてあげなきゃ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:6 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:6 
msgid "お友達かな？\nどんどん教えてあげておくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:7 
msgid "うん、帰ったら教えなくちゃ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:8 
msgid "お嬢ちゃんは大福好きかい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:9 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:9 
msgid "うん。でもケ―キの方がすきだけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:10 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:11 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:11 
msgid "大福をなめるな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:12 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:12 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0171:13 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:13 
msgid "阪本！　大福仞めちゃダメ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:14 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0171:14 
msgid "えええ―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:15 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:15 
msgid "何！？\n大福を仞めたのか、この猫は！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:16 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:16 
msgid "えええ―――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0171:17 
msgid "とばっちりでアンテナがっ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0171:18 
msgid "勘弁してくれ――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:19 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:19 
msgid "（くっ、やっぱり子供はケ―キが好きなのか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:20 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:20 
msgid "（やはりシックで落ち着いた外見の魅力は\n　大人にしかわからないのか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:21 
msgid "（いや、違う、違うぞ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:22 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:22 
msgid "（これは子供を大福に目覚めさせる\n　チャンスじゃないか！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:23 
msgid "（そうだ、ここで諦めていては\n　大福ブ―ムなど夢のまた夢！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:24 
msgid "お嬢ちゃん、大福もおいしいんだよ。\nほら、１つ食べてみなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:25 
msgid "あっ、くれるの！？　いただきま―す！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:26 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:26 
msgid "あむあむ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:27 
msgid "おいしい！！\nすごい、大福おいしいです！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:28 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:28 
msgid "そうだろうそうだろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:29 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:29 
msgid "さ、気に入ったならどんどん食べなさい！\n今お茶も亞れてあげよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:30 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:30 
msgid "ほらほら！　どんどん食べなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:31 
msgid "わわっ、凄い凄い！\n甘い！　おいしい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:32 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:32 
msgid "まだまだおかわりならたくさんあるぞ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:33 
msgid "すごい！　すごい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:34 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:34 
msgid "はははっ、本当にたくさんあるからな！\nほら、もっともっと食べなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:35 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:35 
msgid "おや、手が止まってるじゃないか。\nまだたくさんあるぞ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:36 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:36 
msgid "え、えっと……ちょっと\nお腹がいっぱいになってきたかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:37 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:37 
msgid "ははは、甘いものは別腹と言うじゃないか。\nまだ沢山あるぞ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:38 
msgid "ほら、また手が止まっているじゃないか！\nさあもっと大福を食べるんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:39 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:39 
msgid "も、もうムリ―！\nうわ―ん、大福怖い―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:40 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:40 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:41 
msgid "た、助けて―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:42 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:42 
msgid "わ、私は何か間違いを……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:43 
msgid "うん。ありがとう、おじさん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:44 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:44 
msgid "あむあむ……んくっ、んくっ……ぷは―！\nお茶もおいしい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:45 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:45 
msgid "そうだろう？\nさ、もっと食べなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:46 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:46 
msgid "は―い！\nそうだ、おじさんも一緒に食べればいいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:47 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:47 
msgid "そうか、それじゃ私も食べるとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:48 
msgid "ふぅ、ごちそうさまでした！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:49 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:49 
msgid "おいしかったかい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:50 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:50 
msgid "うん、すっごくおいしかった！\nケ―キより好きかも！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:51 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:51 
msgid "そうかそうか！\nそれは良かった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:52 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:52 
msgid "あっ、そろそろ行かなきゃ！\nごちそうさまでした！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:53 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:53 
msgid "……自信を失いかけていた所を\nあの子に助けられたかもしれないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:54 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:54 
msgid "よし、はりきって大福を売るぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:55 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:55 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0171:56 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:56 
msgid "いっ、一個も無い！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:57 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:57 
msgid "（全部食べてしまった……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:58 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:59 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:59 
msgid "ん？　どうしたんだい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:60 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:60 
msgid "……しらない人からおかしをもらっちゃった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:61 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:61 
msgid "どうしよう、誘拐されるかも！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:62 
msgid "え、何を言ってるんだお嬢ちゃん、\nそんな、誘拐なんてしないよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:63 
msgid "あわわわわわわわわ……っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:64 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:64 
msgid "ゆ―かいはんは、みんなそう言うんだって\n本に書いてあった！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:65 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:65 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:66 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:66 
msgid "いや、本当に違うんだ！\n私は誘拐なんてするつもりは……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:67 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:67 
msgid "た、助けて―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:68 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:68 
msgid "ご、誤解を解く暇が無かった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:69 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:69 
msgid "う―む、世知辛い世の中になったもんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:70 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:70 
msgid "ハイ、ちょっといいですか―？\n今、誘拐がどうとか聞こえたんですが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:71 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:71 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:72 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:72 
msgid "子供が走って逃げていきましたし、すこ―し\nお話を聞かせて貰ってもいいでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0171:73 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0171:73 
msgid "（何故こうなる……）\n"
msgstr ""
