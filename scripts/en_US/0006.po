#  speaker 
#  warnings: 0 
#: sc/0006:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0006:0 
msgid "ん―、いい天気だ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:1 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:1 
msgid "こんな日は私のボケで\nみんなを爆笑させられる気がするな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:2 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:2 
msgid "よ―し、それじゃ早速教室についたら\n抱腹絶倒のボケをかましてみるか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:3 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:3 
msgid "……ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:4 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0006:4 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0006:5 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:5 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0006:6 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:6 
msgid "（こ、この犬は……いつも私が何か\n　失敗をした時にやってくる犬だ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:7 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:7 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:8 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:8 
msgid "……でも、まだ今日は\n何もボケてないから気にすることは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:9 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:9 
msgid "い、いや、もしかしたら\nこれから何か起こるのかも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:10 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0006:10 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:11 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:11 
msgid "ハッ！？　く、黒猫！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:12 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:12 
msgid "不吉だ……やっぱりお前が来たのは\n私に何か起こることを暗示してるのね！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:13 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:14 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:14 
msgid "黙ってないで何か言って――っ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:15 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:16 
msgid "おい、どこに行くんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:17 
msgid "えっ？　ああ、ちょっとトイレに……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:18 
msgid "トイレならあっちだろ？\nそっちは避難カプセルのある部屋だぞ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:19 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:20 
msgid "……おい、まさかお前。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:21 
msgid "だって！　嫌な予感がするんだ！\n今すぐこの船を降りないときっと弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:22 
msgid "ほらきた――！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0006:23 
msgid "うおおお！？\nいつの間にかアンテナが折れてる―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:24 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0006:24 
msgid "あっ、ゆっこ。おはよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:25 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:25 
msgid "あ、みおちゃん。\nスラマ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:26 
msgid "（あっ！　危ない！！\n　いつものクセでボケるところだった！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:27 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:27 
msgid "（この犬が来たということは少しのボケでも\n　ひどい目に遭うということ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:28 
msgid "（タイミングを誤ると\n　とんでもない痛手を負うことに！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:29 
msgid "（いや、でもスラマッパギは挨拶だし、\n　最近は割と浸透してきたし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:30 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:30 
msgid "ゆっこ？　どしたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:31 
msgid "（……いや、スラマッパギじゃダメだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:32 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:32 
msgid "（あれはもはやただの挨拶。\n　ボケと言うにはレベルが低すぎる！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:33 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:33 
msgid "ゆっこ？　お―い、大丈夫―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:34 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:34 
msgid "みおちゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:35 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:35 
msgid "えっ、な、なに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:36 
msgid "部屋の掃除をした！\n後はノミ退治のみ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:37 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:37 
msgid "……え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:38 
msgid "カタツムリに、勝ったつもりか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:39 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:39 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:40 
msgid "ネクタイが結べないのがネックたい！\nクレヨンを買ってくれよん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:41 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:42 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:42 
msgid "日本国旗は、二本こっきり―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:43 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:44 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:44 
msgid "！！！\n"
msgstr "!!!\n"

#  speaker 
#  warnings: 0 
#: sc/0006:45 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:45 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:46 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:46 
msgid "ん。そうだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:47 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:47 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:48 
msgid "うわあああああああああ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:49 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:49 
msgid "あっ、ちょっとゆっこ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:50 
msgid "うわあああああああああ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:51 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:51 
msgid "（ダメ！　お笑いへの情熱が\n　私にボケろと言っている！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:52 
msgid "みおちゃん、スラマッパギ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:53 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:53 
msgid "うん、おはよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:54 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:55 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:55 
msgid "（大丈夫。今のはただの挨拶。\n　決してボケて滑ったわけじゃない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:56 
msgid "今日、天気いいよね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:57 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:57 
msgid "うん。こんな日はテンキ―を\n買いに行きたくなるんだっけ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:58 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:58 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:59 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:59 
msgid "（まさかのボケ返し……！\n　しかも前に自分が言ったネタで！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:60 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:60 
msgid "……や、やだなぁ。\n同じネタはそうそう使わないって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:61 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:61 
msgid "あ、そう？　なんかまた\n同じこと言いそうな気がしたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:62 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:63 
msgid "（読まれてた……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:64 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:64 
msgid "（どうしたのみおちゃん……\n　今日に限ってどうしてそう手厳しいの？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:65 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:66 
msgid "あっ、この犬……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:67 
msgid "ひっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:68 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:69 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:69 
msgid "（ぐっはっは――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:70 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:70 
msgid "（いや、ここは少し慎重になろう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:71 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:71 
msgid "おはよう、みおちゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:72 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:72 
msgid "あれ？\n今日はスラマッパギじゃないんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:73 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:73 
msgid "うん。何回も同じネタで笑いを取るのは\n私としても不本意だしさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:74 
msgid "へ―。でもさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:75 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:75 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:76 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:76 
msgid "スラマッパギって挨拶でしょ？\n笑い取れるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:77 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:77 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:78 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:78 
msgid "……ゆっこ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:79 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:79 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:80 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:80 
msgid "ちょっ！？　ゆっこ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0006:81 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0006:81 
msgid "…………。\n"
msgstr ""
