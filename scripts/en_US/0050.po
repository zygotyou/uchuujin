#  speaker 
#  warnings: 0 
#: sc/0050:0 
msgid "はかせ"
msgstr "Professor"

#  text 
#  warnings: 0 
#: sc/0050:0 
msgid "ただいま―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:1 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0050:1 
msgid "おっ、帰ってきた弌弌ん？\nおい、なんだその口元のクリ―ムは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:2 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:2 
msgid "あっ、な、なのには内緒だよ？\nおねえさんに、ごちそうしてもらったの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:3 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:3 
msgid "お姉さん……？　まあ、誰かは知らんが\nその人に聞きに行ってたんだな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:4 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:4 
msgid "えっ？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0050:5 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:5 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:6 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:6 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0050:7 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:7 
msgid "ネジのことを\n聞いてきたんじゃないのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:8 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:8 
msgid "……忘れてた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:9 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:9 
msgid "えええっ！？\nじゃあ今まで何してたんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:10 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:10 
msgid "ご飯食べてました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:11 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:11 
msgid "照れるとこか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0050:12 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:13 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:13 
msgid "犬まで拾ってきたのか。\n娘が飼うのを許すかどうか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:14 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:14 
msgid "！？　犬だ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:15 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:15 
msgid "拾ったんじゃないのか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:16 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:16 
msgid "わ―――！？　こっち来るな――！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:17 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:17 
msgid "こら！　物を投げるな！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0050:18 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0050:19 
msgid "ティッシュの箱、アンテナに直撃！\nアンテナ壊れました！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0050:20 
msgid "なんてこった！\n動物虐待じゃないか！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0050:21 
msgid "でもさ、俺たち、この猫に\nアンテナ刺して利用してるんだよな？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0050:22 
msgid "なんてこった！？\n動物虐待じゃないか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:23 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:23 
msgid "でも、おねえさんは\nネジが可愛いって言ってくれたよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:24 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:24 
msgid "マジか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:25 
msgid "ふふふ―、はかせが正しかったでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:26 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:26 
msgid "む……じゃあ他の人は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:27 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:28 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:28 
msgid "そいつ以外の人の意見は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:29 
msgid "わかりません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:30 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:30 
msgid "だからなんで照れるんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:31 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:31 
msgid "それに、わからないってどういうことだ？\n色んなやつに意見聞きに行ったんだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:32 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:32 
msgid "だっておねえさんにしか聞いてないもん。\nでもきっとみんな可愛いって言うよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:33 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:33 
msgid "それじゃあ聞きに行った意味が弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:34 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:34 
msgid "……もういい。俺は寝るぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:35 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:36 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:36 
msgid "（ぷぷぷ！　阪本ったら自分が間違ってるって\n　わかったから豫ねちゃってる！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:37 
msgid "大丈夫だよ！　阪本のセンスが悪くても\n猫だから仕方ないもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:38 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:38 
msgid "悪いのはガキのセンスだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:39 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:39 
msgid "そんなことないもんね―。\nおねえさんも可愛いって言ったもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:40 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:40 
msgid "ならその人のセンスもおかしいんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:41 
msgid "ぷぷぷ。何を言っても２対１で\n阪本の負けだもんね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:42 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:42 
msgid "娘は嫌がってるんだから\n少なくとも２対２で負けではないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:43 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:44 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:44 
msgid "むしろ背中につけられる娘が\n嫌がってるんだからこっちの勝ちだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:45 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:45 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:46 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:46 
msgid "じゃあ作るはかせはいっぱい疲れるから\nはかせの勝ち！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:47 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:47 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:48 
msgid "あ、そうだ。作るのはかせなんだから\n好きに作ればいいんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:49 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:49 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:50 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:50 
msgid "阪本の気持ちもわかるよ。でもはかせと\nおねえさんが正しいのは認めないとね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:51 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:51 
msgid "意見が少なすぎて認めたくても\n認められないんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:52 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:52 
msgid "大体、そのお姉さんって誰だ？\n親戚か何かか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:53 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:53 
msgid "ううん。はかせも今日初めて会った人だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:54 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:54 
msgid "……おい。もしかして、知らない人に\n声かけて回るつもりだったのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:55 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:55 
msgid "うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:56 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:56 
msgid "危ないだろ……。\nお前はまだガキなんだから何があるか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:57 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:57 
msgid "大丈夫大丈夫。だってあのおねえさん、\nなのと知り合いだもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:58 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:59 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:59 
msgid "……ならいいか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:60 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:60 
msgid "うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:61 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:61 
msgid "残念だけど、これが一般の意見だからね。\n猫には難しかったかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:62 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0050:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0050:63 
msgid "さ―て、どんなのをつけようかな\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:64 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:64 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:65 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:65 
msgid "（ま、俺につくわけじゃないからいいか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:66 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:66 
msgid "形はどんなのがいいかな―。\n……ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:67 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:67 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:68 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:68 
msgid "（ぷぷぷ。ふて寝してる。\n　やっぱり猫だなぁ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0050:69 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0050:69 
msgid "ｙｙＹ\n"
msgstr ""
