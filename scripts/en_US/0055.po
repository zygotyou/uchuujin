#  speaker 
#  warnings: 0 
#: sc/0055:0 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0055:1 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:1 
msgid "おかしい。\n今日はよく金縛りになる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:2 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:2 
msgid "偶然？　それとも他に理由が？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:3 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:3 
msgid "……わからない。\n今日は偶然、そういう日だから？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:4 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:4 
msgid "あれが来ると動くのはせいぜい片手１本。\n対策を立てるべきか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:5 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0055:5 
msgid "うおわっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:6 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:6 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:7 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:7 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:8 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:8 
msgid "なに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:9 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:9 
msgid "も、ものすごい悪寒がする……\nあんた、なんて恐ろしい物を持って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:10 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:10 
msgid "恐ろしい物？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:11 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:11 
msgid "気付いてないのか？　そのカバンの中から\n恐ろしいくらいの波動が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:12 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:12 
msgid "カバンの中？\nこの中には教科書とかノ―トとか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:13 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:13 
msgid "弥佗菩薩くらいしかないけど？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:14 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:14 
msgid "それだ！　その菩薩から波動が！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:15 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:15 
msgid "この菩薩から……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:16 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:16 
msgid "悪いことは言わない。それは捨てなさい。\nさもなくば弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:17 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:17 
msgid "うっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:18 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:18 
msgid "こ、こんな風に、金縛りに……\n場合によってはもっと酷いことに……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:19 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:19 
msgid "あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:20 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:20 
msgid "（確かに今日の金縛りはゆっこから\n　弥佗菩薩をもらってから起こってた）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:21 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:21 
msgid "（じゃあ、この人の言うように、\n　ずっと持っていたら酷いことが起こる？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:22 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:22 
msgid "は、早く、菩薩を遠くに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:23 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:23 
msgid "（残念だけど、捨てるしかないか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:24 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:24 
msgid "バイバイ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:25 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0055:25 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0055:26 
msgid "うわっ！　ぼ、菩薩がこっちに！\n呪われちまう！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:27 
msgid "あ、慌てるな。実害があったとしても\nこのオッサンに行くだけだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:28 
msgid "そ、そうだよな。\n俺たちは、ここにいるわけだし……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:29 
msgid "そうそう。\nだから何も恐れることは弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:30 
msgid "えっ！？　ど、どうした！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:31 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:32 
msgid "みんな！　まさか俺たちにまで！？\nま、まずいぞ、このままじゃ船が……\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:33 
msgid "どうする、残った俺がなんとかしなければ、\n苦しみながら全滅するだけだ弌弌うっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:34 
msgid "スタ―ラ姫"
msgstr "Princess Starla"

#  text 
#  warnings: 0 
#: sc/0055:34 
msgid "…………！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:35 
msgid "スタ―ラ姫"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:35 
msgid "楽にしてやろう。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0055:36 
msgid "（ま、まさか……）\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0055:37 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0055:37 
msgid "（やっぱり――――っ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:38 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:38 
msgid "（でも、せっかく手に入った銅製の菩薩……\n　捨てるのは勿体ない……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:39 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:39 
msgid "あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:40 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:40 
msgid "（しまった……金縛りが……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:41 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:41 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:42 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:42 
msgid "（うっ……\n　やっぱり、動くのは片手くらいしか弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:43 
msgid "教頭先生"
msgstr "Vice Principal"

#  text 
#  warnings: 0 
#: sc/0055:43 
msgid "うおっ！？\n二人とも、どうかしましたか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:44 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:44 
msgid "（あ、この人、確か教頭……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:45 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:45 
msgid "怪我をしているんですか？\n待ってなさい、すぐに救急車を呼びます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:46 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:46 
msgid "（それよりも菩薩を……\n　って、喋れないんだった）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:47 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:47 
msgid "あ、あんた、救急車はいい。\nそれより、そこの菩薩を捨てなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:48 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:48 
msgid "（あれ？　この人喋れてる。どうして？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:49 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:49 
msgid "ぼ、菩薩？　うわ、何故ここに菩薩が！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:50 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:50 
msgid "私がプレゼントでもらった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:51 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:51 
msgid "（……あれ？　喋れた。どうして？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:52 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:52 
msgid "プレゼントで菩薩を……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:53 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:53 
msgid "そう。私は木製の方が好きだけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:54 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:54 
msgid "変わってる弌弌って、\n金縛りは解けたのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:55 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:55 
msgid "……解いた？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:56 
msgid "イタコ＆教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:56 
msgid "えええ――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:57 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:57 
msgid "（どうして解けた？　気の持ちよう？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:58 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:58 
msgid "とりあえず、この菩薩は供養する。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:59 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:59 
msgid "（バイバイ、銅製菩薩……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:60 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:60 
msgid "（困った。喋れないんじゃ\n　菩薩のことを伝えられない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:61 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:61 
msgid "そこの人、これは病気ではない。\n悪霊から金縛りを受けているのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:62 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:62 
msgid "（あれ？　この人は喋れる？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:63 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:63 
msgid "か、金縛り？　悪霊？\nではどうすれば？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:64 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:64 
msgid "私の言う通りにしなさい。\nそこにある菩薩を弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:65 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:65 
msgid "（……霊能者？　よくわからないけど\n　この人に任せれば大丈夫そう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:66 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:66 
msgid "その菩薩をまず清める。\n私の荷物の中から道具を弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:67 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:67 
msgid "は、はい！　これを振りかけるんですな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:68 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:68 
msgid "そう、そして私の言葉に続きなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:69 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:69 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:70 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:70 
msgid "（楽だ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:71 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:71 
msgid "ほおああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:72 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:72 
msgid "ほおああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:73 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:73 
msgid "（ほおああああ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:74 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:74 
msgid "（今は片手しか動かない。菩薩のことを\n　教頭に伝える方法は……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:75 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:75 
msgid "（…………）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:76 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:76 
msgid "（……無い）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:77 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:77 
msgid "うっ！？　こ、これは！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:78 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:78 
msgid "な、なんということだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:79 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:79 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:80 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:81 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:81 
msgid "（全滅……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:82 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:82 
msgid "くっ、こうなったら弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:83 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:83 
msgid "（ん？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:84 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:84 
msgid "……誰かの金縛りが解けるまで\n待つしかない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0055:85 
msgid "麻衣＆教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0055:85 
msgid "…………。\n"
msgstr ""
