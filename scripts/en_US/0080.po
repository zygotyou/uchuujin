#  speaker 
#  warnings: 0 
#: sc/0080:0 
msgid "きよし"
msgstr "Kiyoshi"

#  text 
#  warnings: 0 
#: sc/0080:0 
msgid "ただいま―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:1 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:1 
msgid "あ〜、お腹減った〜。\n今日のお昼ご飯何かな〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:2 
msgid "バディ＆コバディ"
msgstr "Buddy & Co."

#  text 
#  warnings: 0 
#: sc/0080:2 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0080:3 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:3 
msgid "バディ、コバディ、ただいま。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:4 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:4 
msgid "……あ、そうだったな。\n帰ってきたら散歩に行く約束だっけ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:5 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:5 
msgid "お腹減ったけど……いいや。\n先に散歩に行くか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:6 
msgid "バディ＆コバディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:6 
msgid "！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:7 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:7 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0080:8 
msgid "うわっ、なな、なんだ！？\n犬が勝手に走り出したぞ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0080:9 
msgid "何やってんだ！\n早く犬を止めるんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0080:10 
msgid "お、俺、機械音痴なんだって！\nどど、どのボタンを押せばいいんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0080:11 
msgid "あっ、待て！　わからないからって\nテキト―にボタンを押すんじゃ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0080:12 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0080:12 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:13 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:13 
msgid "バ、バディ！？　コバディ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:14 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:14 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:15 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:15 
msgid "また……なのか？\n散歩の約束があるのに、お前たちはまた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:16 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:16 
msgid "また、わけわからないことを\nしに行ってしまうのか！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:17 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:17 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:18 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:18 
msgid "あ……おじいちゃん……\n聞いてよ、バディたちが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:19 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:19 
msgid "バディたちが僕との散歩の約束を放って\nどこかに行っちゃったんだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:20 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:20 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:21 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:21 
msgid "きよしや……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:22 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:22 
msgid "な、なに？　おじいちゃん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:23 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:23 
msgid "信じなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:24 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:24 
msgid "えっ？\nおじいちゃん……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:25 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:25 
msgid "……信じなさい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:26 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:26 
msgid "な、何を……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:27 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:27 
msgid "信じろって、一体何を信じれば……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:28 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:28 
msgid "ハッ！？　そ、そうか！\nバディたちを信じろということだね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:29 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:29 
msgid "確かにバディたちはこの前、\nなんかよくわからないことをしてたけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:30 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:30 
msgid "でも、今日はきっと違うよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:31 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:31 
msgid "きっと、すぐ行かないといけないような……\nなにか大事な用なんだ。そう信じよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:32 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:32 
msgid "だって、僕はバディたちの飼い主じゃないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:33 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:33 
msgid "だからそう、バディたちには\nなにか大事な用があるって信じて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:34 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:34 
msgid "信じて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:35 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:35 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0080:36 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:36 
msgid "信じれね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:37 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:37 
msgid "信じろって……もしかして、おじいちゃんが\nバディたちを探してきてくれるってこと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:38 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:38 
msgid "……でも、なんだろう。\nさっきから嫌な胸騒ぎがするんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:39 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:39 
msgid "何か、大切な何かを忘れているような……\nおじいちゃんに伝えなければいけないような。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:40 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:40 
msgid "はっ、もしかして、\nおじいちゃんの身に何かが起きるんじゃ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:41 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:41 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:42 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:42 
msgid "……大丈夫だよね。おじいちゃんなら\n無事にバディたちを見つけてきてくれる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:43 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:43 
msgid "それに、もし何かあったとしても、\n携帯で連絡してくれるだろうし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:44 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:44 
msgid "……う―ん。\nやっぱり胸騒ぎがするんだよな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:45 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:45 
msgid "でも、大丈夫のはず！\n何かあれば携帯で弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:46 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:46 
msgid "……あれ？　さっきから胸騒ぎと一緒に、\nどうして携帯が気になるんだろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:47 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:47 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:48 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:48 
msgid "おじいちゃん、携帯忘れてるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:49 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:49 
msgid "信じろって……\nおじいちゃんを信じろってことかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:50 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:50 
msgid "でもおじいちゃん、\nどっかに行っちゃったし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:51 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:51 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:52 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:52 
msgid "もしかしてバディたちを捜しに\n行ってくれたのかな？　だったら僕も弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:53 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:53 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0080:54 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0080:54 
msgid "とりあえず、ご飯食べよう。\n"
msgstr ""
