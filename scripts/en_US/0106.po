#  speaker 
#  warnings: 0 
#: sc/0106:0 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0106:0 
msgid "ここ、テストにそのまま出すからな。\nちゃんと覚えとけよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:1 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:1 
msgid "特に相……む。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:2 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:2 
msgid "（相生のやつ、長野原から\n　ノ―トを受け取らなかったか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:3 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:3 
msgid "あ―、で、次の段落だが、\nここで主人公に心境の変化が弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:4 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:4 
msgid "（今度は相生が長野原に渡したぞ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:5 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:5 
msgid "（……なるほど、宿題を写してるんだな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:6 
msgid "（提出しないのは怒るが、\n　丸写しじゃ意味がないだろ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:7 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:7 
msgid "相生！　長野原！\n何をやってるんだ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:8 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:8 
msgid "……む？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:9 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0106:9 
msgid "あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:10 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0106:10 
msgid "……先生、なんですかそれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:11 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:11 
msgid "ノ―トを取り上げたつもりだったんだが……\nアンテナ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0106:12 
msgid "…………。\n"
msgstr ".........\n"

#  text 
#  warnings: 1 
#: sc/0106:13 
msgid "抜けた―――――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0106:14 
msgid "くっ、フィ―ドバックがくるぞ！\n総員対ショック防御―！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0106:15 
msgid "うわあ、神様―っ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0106:16 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0106:17 
msgid "……おや？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0106:18 
msgid "なんだ、大丈夫弌弌\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0106:19 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0106:19 
msgid "フェイント！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:20 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:20 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:21 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:21 
msgid "お前ら、本気で何やってるんだ……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:22 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:22 
msgid "（誰だこの一面に描かれてる謎の男は？）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0106:23 
msgid "みお"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0106:23 
msgid "えっと……いやその、中上手く描けなくって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:24 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:24 
msgid "私の描いた方が似てますよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:25 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:25 
msgid "何の話だ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:26 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:26 
msgid "いや、その……高崎先生のつもりなんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:27 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:27 
msgid "えっ！？\n"
msgstr "Ehh!?\n"

#  speaker 
#  warnings: 0 
#: sc/0106:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:28 
msgid "（このアゴの尖った優男はオレだったのか！\n　ということはこの妙に顔が巨大なのも！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:29 
msgid "どうです？\n後半はかなり似てきてると思うんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:30 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:30 
msgid "いやいやゆっこ、あんたのは似てないって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:31 
msgid "そんな事ないって！\nね、先生！　似てますよね！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:32 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:32 
msgid "私の方が上手ですよね！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:33 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:33 
msgid "そういう問題じゃないだろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:34 
msgid "相生、長野原！\n授業中に落書きしていいと思ってるのか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:35 
msgid "あいた〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:36 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:36 
msgid "うぅ、ひどいとばっちりだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:37 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:37 
msgid "いやいや、みおちゃんが主犯だから。\nむしろ私が被害者？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:38 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:38 
msgid "何言ってんの！？\nあんたが先に描いてきたんでしょうが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:39 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:39 
msgid "それにダメだししたみおちゃんが悪い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:40 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:40 
msgid "ハァッ！？\nあんた自分を棚に上げて何言ってんの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:41 
msgid "ほんとバカだな！\nそんなんだから絵も上手く描けないんだよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:42 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:42 
msgid "絵は関係ないだろうが―――！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:43 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:43 
msgid "むしろあんな絵ばっかり\n描いてる方がバカじゃない！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:44 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:44 
msgid "絵は関係ないって言ったのそっちだろうが！！！\n言った先から忘れるとかほんとアホだな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:45 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:45 
msgid "あ―そうですよ！\nアホだから弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:46 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:46 
msgid "二人とも廊下に立ってろ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:47 
msgid "ゆっこ＆みお"
msgstr "Yuuko & Mio"

#  text 
#  warnings: 0 
#: sc/0106:47 
msgid "は〜〜〜〜い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:48 
msgid "（まあ、上手い下手で言えば長野原の方だが、\n　相生の絵にも個性がある）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:49 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:49 
msgid "（それに教師として、一方をけなすというのも\n　どうかと思う……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:50 
msgid "（よし、ここは引き分けにしておこう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:51 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:51 
msgid "……そうだな、どっちも違った\n上手さがあると思うぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:52 
msgid "ちえ―、引き分けか―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:53 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:53 
msgid "むぅ、私の方が絶対上手いのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:54 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:54 
msgid "どっちの絵も特徴的だからな。\n素人の先生が優劣をつけるのは難しいんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:55 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:55 
msgid "ただ、先生でもわかることがある。\nそれはどっちも本気で描いてるってことだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:56 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:56 
msgid "教育者として生徒の努力を否定するなんて\nできるはずがないだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:57 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:57 
msgid "おぉ―、先生なんかいいこと言った！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:58 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:58 
msgid "……そういうもんかぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:59 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:59 
msgid "さて、結論が出た所でだ。\n相生、長野原。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:60 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:60 
msgid "あ―、これは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:61 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:61 
msgid "えっ、何？　えっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:62 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:62 
msgid "授業中に落書きは良くない。\n廊下に立ってろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:63 
msgid "ゆっこ＆みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:63 
msgid "ですよね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:64 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:65 
msgid "私ですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:66 
msgid "私に決まってるでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:67 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:67 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:68 
msgid "あれ、先生どこに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:69 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:69 
msgid "……え―、主人公の心境が変わることで\n価値観に変化が生まれるわけだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:70 
msgid "ゆっこ＆みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:70 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0106:71 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:71 
msgid "ではここから先を……相生―。\n６７ペ―ジから読め。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:72 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:72 
msgid "……せんせ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:73 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:73 
msgid "なんだ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:74 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:74 
msgid "あの、絵……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:75 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:76 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:76 
msgid "相生―、６７ペ―ジからだ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:77 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:77 
msgid "……あ、あの―。\n先生、絵の感想は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:78 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:78 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:79 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:79 
msgid "……せんせ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:80 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:80 
msgid "なんだ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0106:81 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0106:81 
msgid "教科書忘れました―。\n"
msgstr ""
