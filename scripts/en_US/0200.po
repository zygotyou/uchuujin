#  speaker 
#  warnings: 0 
#: sc/0200:0 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0200:0 
msgid "あ―あ、ヒマだなあ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:1 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:1 
msgid "予定が無いときに限って、\nみさともフェっちゃんも駄目なんだもんな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:2 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:2 
msgid "……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:3 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:3 
msgid "あそこにいるのは中之条じゃん！\nうそ！？　なんでぇ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:4 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:4 
msgid "……って、そうか。\n実家が大福屋だったよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:5 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:5 
msgid "なにか困ってるみたい。\n考え事して……どうしたんだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:6 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0200:6 
msgid "う〜ん……どうしようかな……\nあと一人……早く代役探さないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:7 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:7 
msgid "あ、あの……どうしたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:8 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:8 
msgid "あれ？　確かあなたは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:9 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:9 
msgid "うん、何度か会ってると思うけど\n……覚えててくれたんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:10 
msgid "はい。あ、そういえば、\n教室で呼ばれて、なぜか驚かれたことも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:11 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:11 
msgid "ん―、あれはちょっと理由があって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:12 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:12 
msgid "（あのとき、モヒカンなかったから）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:13 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:13 
msgid "それより、どうしたの？\n何か考え事していたようだったけど？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:14 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:14 
msgid "あ、実はちょっと困ったことがあって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:15 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:15 
msgid "弌弌って、そうだ！　今ヒマですか！？\n付き合ってもらえませんか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:16 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:16 
msgid "ＮＧｇｇｇｇｇｇ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:17 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:17 
msgid "（これってコクられた！？　デ―トの誘い！？\n　ていうか、それしかないじゃん！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:18 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:18 
msgid "駄目ですか？\n嫌ならいいんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:19 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:19 
msgid "も、もちろんイエス！　どこ！？\n花山公園！？　それとも伊香保温泉！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:20 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:20 
msgid "いえ、そんな遠くじゃなくて弌弌ここです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:21 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:21 
msgid "こ、ここって……商店街？\n大工カフェってこと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:22 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:22 
msgid "じゃなくて、うちの家です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:23 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:23 
msgid "ＸＤ９Ｇｇｇｇｇｇｇｇｇ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:24 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:24 
msgid "ちょっ！？　それは早い！　早すぎ！\nいつでもいいけど！　もうちょっと先に！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:25 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:25 
msgid "だったら今すぐお願いします！\nじゃないと父さんに怒られるんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:26 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:26 
msgid "と、父さん？　怒られるって……何で？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:27 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:27 
msgid "大福フェアです。\nこれ被ってください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:28 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:28 
msgid "こ、これって……確か大福くん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:29 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:29 
msgid "そうです、うちの店のマスコットです。今回は\n大福兄弟をやることになってまして……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:30 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:30 
msgid "バイト代はちゃんと払います！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:31 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:31 
msgid "……バイト？\nなんだ、バイトか―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:32 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:32 
msgid "（うちの隣にも大福屋さんあるし、\n　少し前に大工カフェできたから……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:33 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:33 
msgid "（なにかと宣伝が大変なのはわかるけど、\n　はぁ……バイトの誘いだったんだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:34 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:34 
msgid "お願いします！　一緒に大福兄弟を！\nあなたしかいないんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:35 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:35 
msgid "（あなたしか、いないっ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:36 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:36 
msgid "ＦＮｎｎｎｎｎｎｎｎｎｎｎｎｎ９Ｋ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:37 
msgid "……やっぱり、\n大福兄弟なんて無理ですよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:38 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:38 
msgid "あ―……\nううん、別にいいよ。ど、どうせヒマだしね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:39 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:39 
msgid "（っていうか、これってチャンス！\n　兄弟ってことは二人っきり！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:40 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:40 
msgid "（ふおおおおおおおおおおおおお！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:41 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:41 
msgid "……あれ？　大丈夫ですか？\nなんかすごく顔が赤いんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:42 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:42 
msgid "だ、大丈夫！　全然オッケ―！\nそれじゃ始めよっか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:43 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:43 
msgid "わかりました。\nそれじゃ、この大福くんを弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:44 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0200:44 
msgid "おお、兄弟は見つかったようだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:45 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:45 
msgid "二人共、今日はよろしく頼むぞ。\n大福の良さをみんなに知らせてやってくれ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:46 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:46 
msgid "わかってるって。\nがんばるからさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:47 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:47 
msgid "私もです！　一生懸命やります！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:48 
msgid "ぐはあっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:49 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:49 
msgid "な、何を……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:50 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:50 
msgid "そうだよ！\n父さん、何するんだよ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:51 
msgid "げふあっ！？\nだ、だから何で殴るんだよ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:52 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:52 
msgid "大福がしゃべるな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:53 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:53 
msgid "いや、でも……\nしゃべらないと宣伝にならないし？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:54 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:54 
msgid "何べんも言わせるな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:55 
msgid "うう……ど、どうします？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:56 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:56 
msgid "さすがに……ごめん無理。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:57 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:57 
msgid "いえ、僕も……\n……はい、父さん、大福くん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:58 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:58 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0200:59 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:59 
msgid "お、おい、二人共！？　どこに行く！\n大福を裏切るつもりか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:60 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:60 
msgid "……行ってしまったか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:61 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:61 
msgid "仕方がない、自分でやってみるか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:62 
msgid "しかし、兄弟を一人で、か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:63 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:63 
msgid "兄弟……そういえば昔、ダンゴにいたな。\n確か、やつらは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:64 
msgid "……重ねてみるか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:65 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:65 
msgid "大福くんの頭の上に大福くんが……\n上がお兄さんでしょうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:66 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:66 
msgid "ふらふらしてる……\nあ、お兄さん落ちた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:67 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:67 
msgid "……捕まるでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:68 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:68 
msgid "……捕まったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:69 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:69 
msgid "……ほんとだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:70 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:70 
msgid "お詫びってわけじゃないんですけど……\n飲み物、おごらせてもらえませんか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:71 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:71 
msgid "うん、ありがとう。\n大工カフェでも行ってみる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0200:72 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0200:72 
msgid "（……あれ？　これってデ―ト？）\n"
msgstr ""
