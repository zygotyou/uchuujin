#  speaker 
#  warnings: 0 
#: sc/0062:0 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0062:0 
msgid "（う―……生徒の気持ちを考えたら\n　声もかけれなくなっちゃった……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:1 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:1 
msgid "……はぁ。立派な先生になるために\n生活指導担当になったのに、これじゃあ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:2 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0062:2 
msgid "あれ、桜井先生。\nこんなところで何をしているんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:3 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:3 
msgid "あっ、高崎先生。実は、風紀が乱れないように\n生徒に指導をしていたんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:4 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:4 
msgid "なんと！　放課後でも指導にあたるとは\n素晴らしい心がけです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:5 
msgid "で、でも……私、気が弱くて\nなかなか声をかけれなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:6 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:6 
msgid "高崎先生みたいに物怖じしないようになるには\nどうしたらいいんでしょうか……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:7 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:7 
msgid "そ、そうですね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:8 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:8 
msgid "（おどおどと小動物を侈侏とさせる姿が\n　桜井先生の魅力ではあるのだが……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:9 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:9 
msgid "（だが桜井先生は本気で悩んでいるんだ。\n　ここは真面目に答えてあげなければ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:10 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:10 
msgid "そうだ、ではまず\n敬語をやめてみてはどうでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:11 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:11 
msgid "敬語をですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:12 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:12 
msgid "はい。相手を桜井……あ、いや、弟の桜井誠と\n思えば気軽に話せるのでは？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:13 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:13 
msgid "マコ……弟と思ってですか……\nそれならできそうな気がします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:14 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:14 
msgid "ありがとうございました！\n私、がんばってみます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:15 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:15 
msgid "はい、がんばってください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:16 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:16 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0062:17 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:17 
msgid "（よ―し。みんなを\n　マコちゃんだと思って……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:18 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0062:18 
msgid "よっ、ほっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:19 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:19 
msgid "（あっ、マコちゃんだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:20 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:20 
msgid "こ、こら！　歩きながらボ―ル蹴ったら\n危ないでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:21 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:21 
msgid "うわっ！？　姉ちゃん！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:22 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:22 
msgid "弌弌あっ、ボ―ルが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:23 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0062:23 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0062:24 
msgid "あっはっはっはっはっは……こんな形で\nアンテナが折れると笑うしかないな―。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0062:25 
msgid "あははは、そうだな。\nこの中に、よほど不運なやつがいるんだな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0062:26 
msgid "それかこのオッサンが不運なのかもな。\nアハハハハハハハハ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0062:27 
msgid "あはははははは！！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0062:28 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0062:28 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0062:29 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0062:29 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:30 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:30 
msgid "あっ、笹原君。さような弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:31 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:31 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:32 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:32 
msgid "こら―笹原―！\nどこまで逃げるつもりよ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:33 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:34 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:34 
msgid "こ、こら―！\n待ちなさ―い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:35 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:35 
msgid "わっ！？　さ、桜井先生？\nどうしたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:36 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:36 
msgid "ど、どうしたじゃないでしょ。\nそ……それ、何持ってるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:37 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:37 
msgid "それ？　あ、これですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:38 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:38 
msgid "そうだよ。そんなもの持ってくるなんて\n校則違反でしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:39 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:39 
msgid "あ、はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:40 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:40 
msgid "（あれ……？\n　なんか桜井先生のキャラが違う……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:41 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:41 
msgid "もう、他の人が見て\n真似しちゃったらどうするの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:42 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:42 
msgid "ご、ごめんなさい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:43 
msgid "また次も見つけたら今よりも何倍もきつく\nお説教だからね。わかった？　マコちゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:44 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:44 
msgid "はい……。\n"
msgstr "Yes...\n"

#  speaker 
#  warnings: 0 
#: sc/0062:45 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:45 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:46 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:46 
msgid "マコちゃん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:47 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:47 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:48 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:48 
msgid "（あうう、せっかく上手くいってたのに\n　名前を間違えちゃった……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:49 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:49 
msgid "先生、マコちゃんって誰ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:50 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:50 
msgid "あっ、そ、その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:51 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:51 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:52 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:52 
msgid "は、話を替えないの！\n今は立花さんのことでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:53 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:53 
msgid "えっ？　あ、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:54 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:54 
msgid "危ない物を持ってるのもダメだけど、\n人に向けてるところもよく見るよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:55 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:56 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:56 
msgid "女の子なんだから、\nもっとお淑やかにね。わかった？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:57 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:57 
msgid "はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:58 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:58 
msgid "はい。それじゃあ行っていいよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:59 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:59 
msgid "はい、失礼します……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:60 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0062:61 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0062:61 
msgid "（うう、相手をマコちゃんだと思うと\n　次と言いたい事が言えたけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:62 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:63 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:63 
msgid "胃が痛い……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:64 
msgid "あの、私の名前はみさとなんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:65 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:65 
msgid "あっ、はい。\n立花みさとさんですよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:66 
msgid "誰ですか？　マコちゃんって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:67 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:67 
msgid "あ、それは、私の弟のことで……\n誠って名前なんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:68 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:68 
msgid "へ―。それでマコちゃんって\n呼んでるんですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:69 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:69 
msgid "はい。でもマコちゃんって呼ばれるの、\nあんまり好きじゃないみたいですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:70 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:70 
msgid "男の子ですからね。\nちゃん付けは嫌だと思いますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:71 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:71 
msgid "そうみたいですね。\nいつも怒られちゃって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:72 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:72 
msgid "え―、本当ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:73 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:73 
msgid "あ、そろそろ行かないと。\nそれじゃ先生、さようなら―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:74 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:74 
msgid "あっ、はい。さようなら。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:75 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:76 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:77 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:77 
msgid "あっ……\nお説教のこと忘れてた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:78 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:78 
msgid "（マ、マコちゃんだと思って喋ってたから\n　名前間違えちゃった……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:79 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:79 
msgid "（う―、せっかくビシッと決められたのに……\n　恥ずかしいよ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:80 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:81 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:81 
msgid "あの、その、立花さん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:82 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:82 
msgid "あ、はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:83 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:83 
msgid "その……こういう物は\n風紀が乱れちゃうので……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:84 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:84 
msgid "はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:85 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:85 
msgid "あの、学校にはできるだけ……\n持ってこないように……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:86 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:86 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0062:87 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0062:87 
msgid "（あっ、いつもの桜井先生に戻ってる）\n"
msgstr ""
