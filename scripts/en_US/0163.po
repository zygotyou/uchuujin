msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:73 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:73 
msgid "う、うん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:74 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:74 
