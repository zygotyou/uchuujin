msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:62 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:62 
msgid "えっ？　わっ、くすぐったいよぉ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:63 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:63 
