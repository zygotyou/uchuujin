msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0163:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0163:1 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:1 
msgid "……もうみんな帰ったみたいだね。\nそろそろ見回りは終わりにしようかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:2 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:2 
msgid "……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:3 
msgid "きよし"
msgstr "Kiyoshi"

#  text 
#  warnings: 0 
#: sc/0163:3 
msgid "う―ん……バディとコバディ、\n一体どこに行ったんだよ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:4 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:4 
msgid "（あの子、どうしたんだろう……？\n　ここの生徒……じゃないよね？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:5 
msgid "あ、あの―、スイマセン……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:6 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:6 
msgid "はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:7 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:7 
msgid "ここの生徒じゃないですよね？\n敷地内には入っちゃダメなんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:8 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:8 
msgid "あ、ご、ごめんなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:9 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:9 
msgid "誰か捜してるんですか？\nお兄さん？　それともお姉さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:10 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:10 
msgid "ううん、うちで飼ってる犬を探してて……\nバディとコバディって言うんだけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:11 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:11 
msgid "犬……？\nあ……もしかしてあの……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:12 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:12 
msgid "知ってるの？\nバディとコバディは今どこに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:13 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:13 
msgid "ス、スイマセン、知ってはいるんですけど、\n場所まではわからなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:14 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:14 
msgid "そうですか……\nはぁ。あいつら、どこに行ったんだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:15 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0163:15 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:16 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:16 
msgid "あ……。\n"
msgstr "Ah...\n"

#  speaker 
#  warnings: 0 
#: sc/0163:17 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:17 
msgid "あっ！　バディ！\n捜したんだぞ！　どこにいたんだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:18 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:18 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:19 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:19 
msgid "見つかってよかったですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:20 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:20 
msgid "うん、ありがとう！\nほらバディ、お前もお礼を言うんだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0163:21 
msgid "うわっ、やめろ！\nそ、そんなに頭を押さえたら弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0163:22 
msgid "うわあああ、壊れたぁぁっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:23 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:23 
msgid "そ、そんなに落ち込まないで。\n私も一緒に捜すから。ねっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:24 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:24 
msgid "えっ？　捜してくれるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:25 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:25 
msgid "はい。私と一緒なら校内も捜せるから……\nきっと見つかりますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:26 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:26 
msgid "ありがとうお姉ちゃん！\nじゃあ、早く捜しに行こうよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:27 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:27 
msgid "はい、それじゃ弌弌え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:28 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:28 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:29 
msgid "桜井先生＆きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:29 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0163:30 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:30 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:31 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:31 
msgid "こ、この鹿もキミのペット？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:32 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:32 
msgid "ち、違うよ。鹿なんて飼ってないもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:33 
msgid "だよね……\nもしかして迷い込んじゃったのかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:34 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:34 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:35 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:35 
msgid "わっ！？\nあ、あの鹿、怒ってるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:36 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:36 
msgid "（うぅ、怖い……。でも、大人として、\n　この子だけでも守らないと……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:37 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:37 
msgid "お姉ちゃん、どうしよう！？\nあの鹿、こっちを狙ってるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:38 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:38 
msgid "〜〜〜〜っ！\nこ、こら！　止まりなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:39 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:39 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:40 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:40 
msgid "きゃっ！　ど、ど、ど―ど―。\n落ち着いてください……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:41 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:41 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:42 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:42 
msgid "あっ、すごい、\nお姉ちゃんの言うこと聞いたよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:43 
msgid "わぁ……ありがとう鹿さん。\nもしかしたら言葉がわかるのかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:44 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:45 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:45 
msgid "ここには入ってきちゃダメだよ？\nあと乱暴もダメだからね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:46 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:46 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:47 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:47 
msgid "ね―、そろそろ\nバディたちを捜しに行こうよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:48 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:48 
msgid "あ、うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:49 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:49 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:50 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:50 
msgid "いい？　誰かに見つかる前に\nちゃんと家に帰るんだよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0163:51 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:51 
