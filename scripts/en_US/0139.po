#  speaker 
#  warnings: 0 
#: sc/0139:0 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0139:0 
msgid "さ―て、早く帰って\n次回作のネ―ムでも考えよ―っと。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0139:1 
msgid "みお"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0139:1 
msgid "（今度はどんなカップリングがいいかな？\n　むふふ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:2 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:2 
msgid "（次は今までとは\n　違うベクトルの作品にしてみるのもいいな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:3 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:3 
msgid "（違うベクトル……う―ん……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:4 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:4 
msgid "（おっ、そうだ。たまには渋いオジサマとか\n　出してみてもいいかな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:5 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:5 
msgid "（うん、面白いな。次はその方向性で\n　行ってみようかな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:6 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:6 
msgid "（でもオジサマ系は資料がないんだよね。\n　どこかにモデルでもいないかな……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:7 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:7 
msgid "オジサマ……オジサマ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:8 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:8 
msgid "校長先生と教頭先生のカップリング！？\n教頭先生が攻め！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:9 
msgid "？？？（教頭先生）"
msgstr "??? (Vice Principal)"

#  text 
#  warnings: 0 
#: sc/0139:9 
msgid "責める、か！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:10 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:10 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:11 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:11 
msgid "何だろ？\n今、誰かの声が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:12 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:12 
msgid "……ま、いいや。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:13 
msgid "教頭先生"
msgstr "Vice Principal"

#  text 
#  warnings: 0 
#: sc/0139:13 
msgid "あの生徒の話はよくわからなかったが……\n校長を責める、か……それはぜひしてみたい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:14 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:14 
msgid "（徹底的に校長の責任を……責めて責めて、\n　校長の座から降ろしてみたい！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:15 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0139:15 
msgid "ひっ！？　きょ、教頭先生……？　そ、その……\n私を攻めたいとは……ま、まさか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:16 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:16 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0139:17 
msgid "……攻めたい、と言ったな。\nまさか……本当に……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0139:18 
msgid "ああ……\n攻めと言ったら他にないだろ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0139:19 
msgid "まさか、\nこのメガネの人があんな風に弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0139:20 
msgid "……言うな。\nみなまで言うな……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0139:21 
msgid "…………ああ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0139:22 
msgid "おまえたちの思考回路、腐りすぎだ―っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:23 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:23 
msgid "（いやいやいやいやいやいや！\n　あり得ないから！　絶対ナイから！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:24 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:24 
msgid "（ったく、なんて想像してるんだか……\n　これじゃもう末期症状だよ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:25 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:25 
msgid "（まあでも、教頭はメガネだし……\n　美化して描けばそれなりに……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:26 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:26 
msgid "（いや〜……それもちょっと\n　無理があるなぁ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:27 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:27 
msgid "……ん？　あそこにいるのって教頭？\nこんなとこで何をして弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:28 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:28 
msgid "えええ―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:29 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:29 
msgid "（なななな、なんで教頭が\n　校長の上に馬乗りになってるの――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:30 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:30 
msgid "！？　だ、誰だ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:31 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:31 
msgid "あっ、しまった……み、見つかった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:32 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:32 
msgid "そ、その制服、うちの生徒……\nま、まさか、見ていたのか……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:33 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:33 
msgid "（そんなに慌ててるってことは……\n　見られちゃまずいことを校長に……！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:34 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:34 
msgid "た、頼む……\nこのことは内緒にしてくれ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:35 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:35 
msgid "（内緒に！？　ってことは校長と教頭って、\n　やっぱりそういう関係なんだ〜〜！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:36 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:36 
msgid "……このことが知れたら……\n私は……私は……っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:37 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:37 
msgid "（バレたら教頭と校長は解任！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:38 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:38 
msgid "このまま行けば……\nせっかくのチャンスなんだっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:39 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:39 
msgid "（でも、弍のなくなった二人は\n　今まで以上に強い仆で結ばれて弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:40 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:40 
msgid "（心も体も……\n　文字通り、結ばれるチャンス――っ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:41 
msgid "（すごい、こんな身近に\n　オジサン同士のカップリングが………！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:42 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:42 
msgid "（でも……でも……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:43 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:43 
msgid "頼む！　絶対に誰にも言わないでくれ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:44 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:44 
msgid "い、言いませんっ！\n誰にも言いません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:45 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:45 
msgid "約束……できるな……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:46 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:46 
msgid "は、はいっ！　もう絶対！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:47 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:47 
msgid "校長と教頭がランデブ―だなんて、\n死んでもいいませんっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:48 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:48 
msgid "……なに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:49 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:49 
msgid "じゃ、じゃあ、失礼しますっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:50 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:50 
msgid "……ランデブ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:51 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:51 
msgid "……はっ！？\n教頭先生、私に心臓マッサ―ジを！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:52 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:52 
msgid "（……し、しまった、せっかくのチャンスが！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:53 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:53 
msgid "はぁ……はぁ……\nあるんだ！　ホントにあるんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:54 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:54 
msgid "でも……でも、なんていうか……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:55 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:55 
msgid "リアルだとキモ―――イ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:56 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:56 
msgid "違う！　誤解だ！　今回は私じゃないぞ！\nここに来たらすでに校長が倒れてたんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:57 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:57 
msgid "あ、そ、そうだったんですか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:58 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:58 
msgid "（でも、今回は、って……\n　じゃあ普段はどうなのよ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:59 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:59 
msgid "あ―、ごほん。\nそれでだね、キミ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:60 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:60 
msgid "は、はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:61 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:61 
msgid "校長が息をしとらんようなんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:62 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:62 
msgid "心臓マッサ―ジは試してみたのだが……\nすまんが、人工呼吸をしてもらえるかね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:63 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:63 
msgid "……え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:64 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:64 
msgid "ほれ、早くせんと校長が死んでしまうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:65 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:65 
msgid "い、いや、そういうのはやっぱり、\n経験豊富な教頭先生の方が！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:66 
msgid "（何が悲しくてこんなオッサンに\n　人工呼吸しなきゃダメなのよ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:67 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:67 
msgid "何を言う。\n男同士ではどうも都合が悪いだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:68 
msgid "それはまぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:69 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:69 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0139:70 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:70 
msgid "いや、男同士も、それはそれでアリ……\nと言うか、私的には全然ＮＪですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:71 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:71 
msgid "何をいってるんだ。\nこういうのは最低限男女でやるべきだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:72 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:72 
msgid "いやいや！\n人命救助に男も女もないですから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:73 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:73 
msgid "いやいや、\n人命救助の経験も立派な教育だよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:74 
msgid "いやいや、\n謹んで辞退しますってば！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:75 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:75 
msgid "いやいやキミが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:76 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:76 
msgid "いやいや教頭先生が！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:77 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:77 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:78 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:78 
msgid "み、見てません！　何も見てませ――ん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:79 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:79 
msgid "逃げた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:80 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:80 
msgid "生徒が、私から……逃げた……\n私が一体何をしたのと言うのだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:81 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:81 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:82 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:82 
msgid "逃げられたと言えば……\n小学生の時弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:83 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:83 
msgid "鬼ごっこの鬼……なかなかタッチできなくて\n１ヶ月以上みんなに逃げられ続けた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:84 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:84 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:85 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:85 
msgid "タッチと言えばあれは７年前……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:86 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:86 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0139:87 
msgid "ナレ―タ―お姉さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0139:87 
msgid "その時、大きな川がはっきり見えました。後日、\n校長先生はそう真剣に語ったと言います。\n"
msgstr ""
