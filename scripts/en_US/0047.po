#  speaker 
#  warnings: 0 
#: sc/0047:0 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0047:0 
msgid "え―っと、こんなものかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:1 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:1 
msgid "（しかし、楽とはいえ夕食をコンビニ弁当で\n　済ませていると寂しくなるな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:2 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:2 
msgid "（はぁ――――あっ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:3 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:3 
msgid "（桜井先生に漬物を漬けてもらいてぇ―！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:4 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:4 
msgid "（桜井先生はどの漬物が得意だろうか。\n　沢庵漬けなんて得意そうに思えるが……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:5 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:5 
msgid "はっ、あれは……桜井先生！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:6 
msgid "（大工カフェの前でどうしたんだろう？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:7 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0047:7 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0047:8 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:8 
msgid "……ふぅ。\n"
msgstr "... heh.\n"

#  speaker 
#  warnings: 0 
#: sc/0047:9 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:9 
msgid "（……ためいき？\n　良くわからないが困っているみたいだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:10 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:10 
msgid "弌弌これはチャンスじゃないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:11 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:11 
msgid "……よし、行くぞ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:12 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:12 
msgid "って、おぉっとぉ！\nこ、校長先生！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:13 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:13 
msgid "ど、どうも！\nこんな所で会うなんて奇遇ですね！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0047:14 
msgid "お、おい。こういう時は、\n頭を下げ返すもんだよな？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0047:15 
msgid "おう、それが礼儀ってヤツだ。……いや、\nちょっと待て！　この距離で下げたら！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0047:16 
msgid "……ん？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0047:17 
msgid "あ――っ、そういうことか――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:18 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:18 
msgid "（しかし、走り出してみたものの……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:19 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:19 
msgid "（何がどうチャンスなんだオレ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:20 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:20 
msgid "（しかも、桜井先生が何を困ってるかも\n　わからないのに走り出してどうすんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:21 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:21 
msgid "（……いや、違う！　違うぞ高崎学！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:22 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:22 
msgid "（誰であろうと目の前に\n　困っている人が居るなら助ける）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:23 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:23 
msgid "（それが教師ってもんだろ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:24 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:24 
msgid "桜井先生！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:25 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:25 
msgid "あっ、高崎先生っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:26 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:26 
msgid "どうされましたか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:27 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:27 
msgid "あっ、えっと、\nちょっと注文の仕方がわからなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:28 
msgid "なるほど、オレに任せてください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:29 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:29 
msgid "あっ、は、はい。\nよろしくおねがいします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:30 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:30 
msgid "え―っと、メニュ―は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:31 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:31 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:32 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:32 
msgid "（さ、さっぱりわからん……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:33 
msgid "あ、あの……高崎先生？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:34 
msgid "（し、しかしわからないと言うのは。\n　それだけはできない！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:35 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:35 
msgid "……き、気合です！！\n勢いで言えば何とかなります！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:36 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:36 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0047:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:37 
msgid "何を注文するつもりでしたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:38 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:38 
msgid "あ、普通にコ―ヒ―を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:39 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:39 
msgid "コ―ヒ―！　ホット！　中ぐらい！\nこれで通じます！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:40 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:40 
msgid "！！！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:41 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:41 
msgid "は、はいっ、やってみますっ。\nその、ありがとうございました……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:42 
msgid "玉村"
msgstr "Tamamura"

#  text 
#  warnings: 0 
#: sc/0047:42 
msgid "いらっしゃいませ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:43 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:43 
msgid "……ふぅ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:44 
msgid "（いい事をした後は気持ちがいいもんだ。\n　弌弌それが教師の醍醐味ってやつかな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:45 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:45 
msgid "桜井先生！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:46 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:46 
msgid "あっ、高崎先生っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:47 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:47 
msgid "何かお困りのようですが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:48 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:48 
msgid "あっ、実は少し寄っていこうと思ったんですが、\n注文の仕方がよくわからなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:49 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:49 
msgid "なるほど、どれどれ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:50 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:51 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:51 
msgid "（さ、さっぱりわからん……。\n　だが、ここで正直に言うのはっ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:52 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0047:52 
msgid "あれ、先生たち何やってるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:53 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:53 
msgid "あっ、相生さん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:54 
msgid "あ、もしかして注文で困ってるとか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:55 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:55 
msgid "実はそうなんですよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:56 
msgid "あ、じゃあ私教えましょうか？\nこないだ注文のしかた覚えたんですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:57 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:57 
msgid "あ、ほんと？\nそれじゃお願いしていいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:58 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:58 
msgid "まっかせてくださいよ―！\n高崎先生と二人分ですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:59 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:59 
msgid "あ……いや、オレは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:60 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:60 
msgid "（待てよ、これは桜井先生と\n　お茶をするチャンスだぞ高崎学！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:61 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:61 
msgid "あ、ああ！　頼む！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:62 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:62 
msgid "（ありがとう相生！\n　今度の宿題は少し簡単にしてやるからな！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:63 
msgid "ティ―チャ―だから\nやっぱりお茶がいいですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:64 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:64 
msgid "あ、カフェだからキョ―シ―の方がいいかな？\nな―んて、あはははははは！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:65 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:66 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:67 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:67 
msgid "桜井先生！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:68 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:68 
msgid "あっ、高崎先生っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:69 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:69 
msgid "買い食いは風紀違反ですよっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:70 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:70 
msgid "あっ、ス、スイマセン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:71 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:71 
msgid "（何言ってんのオレ―！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:72 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:72 
msgid "（しっかりしろ高崎学！\n　話したかったのはそこじゃないだろ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:73 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:73 
msgid "あ、いえっ、そうじゃなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:74 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:74 
msgid "は、はい、なんでしょうかっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:75 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:75 
msgid "えっと、その……アレです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:76 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:76 
msgid "教育者として生徒に正しい姿を見せるのがその、\nなんだ……教育者です！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:77 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:77 
msgid "あ、ハイッ、スイマセン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:78 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:78 
msgid "もう買い食いはやめますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:79 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:79 
msgid "高崎先生は教師の鑑です。\n今日はご忠告ありがとうございましたっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:80 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0047:81 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0047:81 
msgid "（ち、ちが―――――う！！\n　ちがうんです、桜井せんせ―――！！）\n"
msgstr ""
