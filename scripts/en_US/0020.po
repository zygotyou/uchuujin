#  speaker 
#  warnings: 0 
#: sc/0020:0 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:0 
msgid "あぁ……お腹すいた……\nけっきょくお昼、食べ損ねちゃったもんなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:1 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:1 
msgid "購買、なんか売れ残ってるといいけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:2 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:2 
msgid "……あれ？　あそこにいるのは笹原と……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:3 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0020:3 
msgid "む、これはいつぞやの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:4 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0020:4 
msgid "え！？　あ……笹原先輩っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:5 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0020:5 
msgid "おお〜！　なんたる偶然！\nこれは面白いことになってきましたな〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:6 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:6 
msgid "ちょっとゆっこ！\n余計なこと言わないでよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:7 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:7 
msgid "わかってますって。みお、頑張りな〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:8 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:8 
msgid "あれは……１年生？\nなんか普通に話してるみたい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:9 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:9 
msgid "……何よ、デレデレしちゃって。\nムカつくわね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:10 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:10 
msgid "あ、あの、笹原先輩、まだでしたら\n朝ご飯ご一緒しませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:11 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:11 
msgid "……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:12 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:12 
msgid "朝って何！？\nもうとっくにお昼だよみおちゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:13 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:13 
msgid "えっ！？　あ―っ！\nす、すいません……私……つい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:14 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:14 
msgid "ぷっははははは。その意気や良し！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:15 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:15 
msgid "人は誰しも、己の限界を超え成長するものだ。\nこれからも精進せよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:16 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:16 
msgid "（バカ笹原！\n　なんでそこで説教なのよ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:17 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:17 
msgid "って、なんでそこで説教！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:18 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:18 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0020:19 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0020:19 
msgid "！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:20 
msgid "……なにいっ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:21 
msgid "なんだ、どうかしたか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:22 
msgid "……よくわからん。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:23 
msgid "ただ結果は見ての通りさ。\n急に制御不能になった。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:24 
msgid "ど、どういうことだ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:25 
msgid "いや……\nだから、よくわかんね―んだって。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:26 
msgid "わかんね―じゃね―だろ！\n制御不能になった原因はなんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:27 
msgid "俺に言われたってわかんね―よ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:28 
msgid "つまり……今、オレたちには理解できない\n何かが起こったということか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:29 
msgid "ああ……。\n"
msgstr "Ah\n"

#  text 
#  warnings: 1 
#: sc/0020:30 
msgid "そうか……俺たちは今、\n地球人の神秘に触れたのか……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:31 
msgid "……そうだな。そうかもしれん。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:32 
msgid "…………。\n"
msgstr ".........\n"

#  text 
#  warnings: 1 
#: sc/0020:33 
msgid "（お前らサボらずにちゃんと見てろよ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:34 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:34 
msgid "……なによ、この違い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:35 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:35 
msgid "悔しいけど私よりもあの子の方が\n女の子らしい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:36 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:36 
msgid "多分、あの隣の子も私なんかより\n女の子らしいんだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:37 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:37 
msgid "うう、笹原が鼻の下を伸ばすのも\nわかる気がする……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:38 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:38 
msgid "……何よ。私と話してる時は\n鼻の下なんて伸ばさないくせに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:39 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:39 
msgid "……はっ！？　な、何言ってるの私！\n笹原に鼻の下なんか伸ばされても困るだけよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:40 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:40 
msgid "ううう……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:41 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:41 
msgid "あ―もう、なんだかイライラしてきたっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:42 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:42 
msgid "あ、あれ？　不発？\nちいぃぃっ！　こんな時にっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:43 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:43 
msgid "仕方ないわね……こら―っ！　笹原ぁ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:44 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:44 
msgid "む？　立花みさと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:45 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:45 
msgid "あんた、なに１年の子に\nちょっかい出してんのよ！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0020:46 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0020:46 
msgid "しかも学校の廊下で堂と！\n少しは風紀ってもん考えなさいよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:47 
msgid "それは誤解だ立花みさとよ。私はただ\nこの者たちと偶然会って話をしていただけだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:48 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:48 
msgid "別に異性との交遊に\n及ぼうとしていたわけではないぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:49 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:49 
msgid "い、異性と……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:50 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:50 
msgid "よ、よく言うわね！\n鼻の下伸ばして口説いてたくせに！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:51 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:51 
msgid "口説く……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:52 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:52 
msgid "いやいや、そういうわけではない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:53 
msgid "第一、私がこの者を口説いていたとして、\nなぜ、立花みさとが怒らねばならんのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:54 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:54 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:55 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:55 
msgid "な、なによその顔！\nか、勘違いしないでよね！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:56 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:56 
msgid "私はただ、この子にいかがわしいこと\nしたいと思っただけなんだから！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:57 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:58 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:58 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:59 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:59 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:60 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:60 
msgid "……って、ダメじゃない！\n私は上級生なんだから、余裕をもたないと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:61 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:61 
msgid "とりあえず深呼吸して……す―、は―。\nよし！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:62 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:62 
msgid "あら〜、笹原、何してんの？\nその子たち、誰？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:63 
msgid "む、立花みさとか。誰と言われても……\nここで偶然会っただけだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:64 
msgid "またまた〜照れちゃって。\n偶然にしちゃ仲良すぎでしょ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:65 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:65 
msgid "そ、そんな、私は別にっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:66 
msgid "うんうん、わかってるって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:67 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:67 
msgid "ほう、ひと目でわかってもらえたか。\nさすがは立花みさと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:68 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:68 
msgid "笹原〜、なかなか良い子じゃない。\nあんたなんかにはもったいないわ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:69 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:69 
msgid "……はて、わかったと言うが、\nどうにも食い違いがあるようなのだが？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:70 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:70 
msgid "隠さなくても大丈夫だって。\n誰にも言わないから〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:71 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:71 
msgid "あなたと笹原、お似合いだよ。\nこれからも仲良くね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:72 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:72 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:73 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:73 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:74 
msgid "……お、おおっ…おおおっ…\nおおおおおおっ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:75 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:75 
msgid "へやっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:76 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:76 
msgid "げふあっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:77 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:77 
msgid "って、なんで私が\nイライラしなきゃいけないのよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:78 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:78 
msgid "笹原が誰と何を話そうが、\n私には関係ないじゃない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:79 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:79 
msgid "そうよ。私はお腹が減ってるから買い物に行く。\nそのためにこの廊下を通るだけなんだから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:80 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:80 
msgid "よし、じゃあ、行くわよ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:81 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:81 
msgid "ん？　立花みさとではないか。\n何か用弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:82 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:82 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:83 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:83 
msgid "……用ではなかったのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:84 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:84 
msgid "あれ、あの人、確か剣道部の……\n笹原先輩、お知り合いですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:85 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:85 
msgid "うむ、クラスメイトだ。\nあやつにはいつも世話に弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:86 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:86 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:87 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:87 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:88 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:88 
msgid "今の人、さっきここ、通ったよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:89 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:89 
msgid "う、うん……戻ってくるの、早かったね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:90 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:90 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:91 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:91 
msgid "そ、それで笹原先輩、\n今日、部活は何時頃まで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:92 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:92 
msgid "なに！？　なに！？\n今なんか通った――――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:93 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:93 
msgid "もう、ゆっこってば！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:94 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:94 
msgid "ご、ごめん、でも今、\n確かになんか変なのが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:95 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:95 
msgid "は？　なにか変なもの？\n別に変なものなんて、どこにも弌弌っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:96 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:96 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:97 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0020:97 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0020:98 
msgid "ゆっこ"
