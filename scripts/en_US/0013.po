#: sc/0013:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0013:0 
msgid "ひゃ―、間に合った〜。\nギリギリセ―フだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:1 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0013:1 
msgid "誰のせいよ、まったく……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:2 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:2 
msgid "事故だよ事故。\n犬に噛まれたと思って諦めよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:3 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:3 
msgid "はいはい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:4 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:4 
msgid "（まあ怪我したわけでも\n　授業に遅れたわけでもないからいいか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:5 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:5 
msgid "（さて、そんじゃ授業の準備っと。\n　最初は国語だっけ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:6 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:6 
msgid "……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:7 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:7 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0013:8 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:8 
msgid "（無い！？　カバンの中に原稿が無い！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:9 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:9 
msgid "（うちに忘れた？　ううん、そんなはず無い。\n　何度も確認してから、うちを出たし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:10 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:10 
msgid "（あっ、そっか！\n　さっき、ゆっことぶつかった時に……っ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:11 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:11 
msgid "（な、なんてこった……！\n　アレを、誰かに見られたら……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:12 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:12 
msgid "（笹原先輩と中之条くんの掛け算を\n　誰かに見られたら……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:13 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:13 
msgid "（私は……！\n　私は、死ぬしか……ないっ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:14 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:14 
msgid "…………ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:15 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0013:15 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0013:16 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:16 
msgid "あれ、何かくわえて弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:17 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:17 
msgid "！？　私の原稿！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0013:18 
msgid "お、おい……この漫画……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0013:19 
msgid "うわぁ……\nん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:20 
msgid "スタ―ラ姫"
msgstr "Princess Starla"

#  text 
#  warnings: 0 
#: sc/0013:20 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0013:21 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0013:21 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0013:22 
msgid "スタ―ラ姫"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:22 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0013:23 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0013:23 
msgid "えええ――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:24 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:24 
msgid "（ぶつかったのは少し前。だったらブツは\n　まだ現場に残っている可能性が高い！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:25 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:25 
msgid "（今からなら間に合う！\n　授業を抜けてでも取りに行かないと！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:26 
msgid "あっ、みおちゃん、ちょっと待って。\nあのね弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:27 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:27 
msgid "止めないでゆっこ！！\n仕方がないの！　仕方なかったのよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:28 
msgid "えっ？　でも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:29 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:29 
msgid "お願い行かせて！！\n私のためを思うなら、黙って行かせて！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:30 
msgid "いや、だけど……\nこれ、みおちゃんのじゃ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:31 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:31 
msgid "え？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0013:32 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:32 
msgid "あっ、そ、それは！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:33 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:33 
msgid "さっきカバンの中身拾ってる時に\n見つけたんだけどさ、みおちゃんのだよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:34 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:34 
msgid "そ、そうなんだ。\nありがとうゆっこ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:35 
msgid "あ、ちょいストップ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:36 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:36 
msgid "えっ？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0013:37 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:37 
msgid "いや、ほら、拾ったのは私なんだからさ、\nお礼として見せて欲しいな―、なんて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:38 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:38 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:39 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:39 
msgid "これ、新しいマンガでしょ？\n今度はどんなの描いたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:40 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:40 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:41 
msgid "（言えない！　先輩とクラスメイトが\n　モデルのマンガなんて言えない！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:42 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:42 
msgid "……あ、大丈夫だって。どういうのかは、\n私は知ってるわけだし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:43 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:43 
msgid "（それでも今回の内容は……！\n　見せられるわけが無いっ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:44 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:44 
msgid "すぐ返すから、ちょっとだけ見るね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:45 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:45 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:46 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:46 
msgid "セイヤッ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:47 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:47 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:48 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:49 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:49 
msgid "これでいい……これで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:50 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:50 
msgid "（ごめん、ごめんね……\n　せめて……安らかに……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:51 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:51 
msgid "ま、待って、ゆっこ！\nそれは……っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:52 
msgid "どれどれ〜？　お、マンガの新作なんだ。\nちょっと読ませてね〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:53 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:53 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:54 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:55 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:55 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:56 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:57 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0013:57 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:58 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:58 
msgid "…………ひっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:59 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:59 
msgid "あ……ああ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:60 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:61 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:61 
msgid "（……相生さん。なんで僕を見て、\n　あんなに複雑な顔をしていたんだろう？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:62 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:62 
msgid "ダ、ダメだよ、\n落とし物の中身勝手に見ちゃ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:63 
msgid "……？　うん。\nでもコレ、みおちゃんのでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:64 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:64 
msgid "そ、そうだけど……\n私のじゃないかもしれないし！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:65 
msgid "じゃあ、確認の為にも尚更見なきゃ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:66 
msgid "で、でも人の物を勝手に見たらダメだって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:67 
msgid "じゃあ、みおちゃんが確認して？\nみおちゃんのだったら見せて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:68 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:69 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:69 
msgid "はい。\n中身確認してみて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:70 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:70 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:71 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:71 
msgid "（うっ……この原稿は、紛れも無く私の……\n　しかしそれを言えばコレを見られる……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:72 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:72 
msgid "ねえねえ、どう？　みおちゃんのだった？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:73 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:73 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:74 
msgid "…………違う！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:75 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:75 
msgid "（はっ！？　私は一体何を！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:76 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:76 
msgid "そっか―。じゃ、後は先生に任せますか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:77 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:77 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:78 
msgid "校内放送"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:78 
msgid "落とし物のお知らせです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:79 
msgid "校内放送"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:79 
msgid "１年Ｐ組の中之条くんと２年Ｏ組の笹原くんに\nよく似た絵のマンガ原稿が届いています。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:80 
msgid "校内放送"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:80 
msgid "心当たりのある人は、\n職員室まで取りに来てください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0013:81 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0013:81 
msgid "…………。\n"
msgstr ""
