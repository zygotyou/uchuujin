#  speaker 
#  warnings: 0 
#: sc/0151:0 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0151:0 
msgid "あっ、昨日はつぼみだったのに。\nきれいな花が咲いてる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:1 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:1 
msgid "世話をしてた甲斐があるなぁ。\nこの調子でもっと大きくなるんだぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:2 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:2 
msgid "ふぅ……しかし、暑いなぁ。\n花たちには多めに水あげないとな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:3 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0151:3 
msgid "おっ、パンク少年。\n今日は花に水やりかい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:4 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:4 
msgid "えっ？　……あ、こんにちは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:5 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:5 
msgid "（この人は確か、２年の先輩だったっけ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:6 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:6 
msgid "誰も水をあげないから。当番じゃないけど\nやるようにしてるんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:7 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:7 
msgid "お―、いいね。パンクな髪型してるくせに\n優しいところがあるじゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:8 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:8 
msgid "あはは、この髪型は生まれつきですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:9 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:9 
msgid "またまた―。そうやってギャップを出して\nポイントアップを狙ってるな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:10 
msgid "そんな。本当のことを言ってるだけですって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:11 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:11 
msgid "く―、にくいね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:12 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:12 
msgid "いえ、だから……ん？\nあっ、先輩、蝶ですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:13 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:13 
msgid "お、ホントだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:14 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:14 
msgid "あ、飛んだ。\n次はどこに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:15 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0151:15 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:16 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:16 
msgid "おおっ、校長先生の頭に着地弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:17 
msgid "蝶"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:17 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:18 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:18 
msgid "失敗っ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:19 
msgid "ぎゃはははは！\nとまりたくても、とまれないじゃないか！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:20 
msgid "くっ、くくくく、いかん、\n面白すぎて仕事が手につかない！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:21 
msgid "くく、は、早く蝶を追い払えって……\nじゃないと俺の腹筋が死ぬ……！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:22 
msgid "オ、オッケ―、グフッ、任せろ。\nこうやって手で払って……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:23 
msgid "…………あ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:24 
msgid "…………えっ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:25 
msgid "…………ごめん。\nアンテナ、折っちゃった……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0151:26 
msgid "ばっかやろ――――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:27 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:27 
msgid "あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:28 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:28 
msgid "ブッ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:29 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:29 
msgid "あはははは！\n蝶が、蝶がモヒカンに止まった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:30 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:30 
msgid "あはは……な、何でこんなとこに……\n花に止まれば甘い蜜が吸えるのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:31 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:31 
msgid "ククク、もしかしたら\nモヒカンが甘いのかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:32 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:32 
msgid "あはは、そんなバカな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:33 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:33 
msgid "どれどれ―？　一本拝借。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:34 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:34 
msgid "痛っ！？\n……せ、先輩！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:35 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:35 
msgid "うわっ！　ほんと甘い匂いがするよ！\nど、どういうこと！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:36 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:36 
msgid "えっ！？\n"
msgstr "Ehh!?\n"

#  speaker 
#  warnings: 0 
#: sc/0151:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:37 
msgid "（甘い？　バカな……！\n　髪が甘いなんて、そんなこと……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:38 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:38 
msgid "（暑さで汗もかいているんだぞ？　しょっぱい\n　ならまだしも、甘いだなんて……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:39 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:39 
msgid "（いや、しかし、先輩は甘いと言っている。\n　こんなことで嘘をつくはずがない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:40 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:40 
msgid "（だとすると……やはり甘いのか？\n　僕の髪は蝶が寄り付くくらい甘い……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:41 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:41 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0151:42 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:42 
msgid "ほれほれ、試しに食べてみなよ。\n弌弌って、そりゃ無理か―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:43 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:43 
msgid "いえ、いただきます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:44 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:44 
msgid "むしった！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:45 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:45 
msgid "（そう、原因追究のためには\n　何事もチャレンジしてみなければ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:46 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:46 
msgid "食べた！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:47 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:47 
msgid "これは！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:48 
msgid "……マズッ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:49 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:49 
msgid "……匂いだけか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:50 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:50 
msgid "うははは！　こりゃもう\nモヒカンじゃなくてカットパインだね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:51 
msgid "そ、そんなバカな……\n甘いわけ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:52 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:52 
msgid "いやいや、甘い匂いするって！\nこりゃ蝶も寄ってくるわけだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:53 
msgid "いやいや、そんなことあるわけ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:54 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:54 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0151:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:55 
msgid "鹿！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:56 
msgid "ヤギ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:56 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:57 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:57 
msgid "ヤギ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:58 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:58 
msgid "あははは、匂いに誘われて\n鹿とヤギがやってきたよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:59 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:60 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:60 
msgid "ペロペロ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:61 
msgid "蝶"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:61 
msgid "ちゅ―ちゅ―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:62 
msgid "ヤギ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:62 
msgid "ベロベロ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:63 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:63 
msgid "（……今日の僕のモヒカンは、\n　一体、どうなっているんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:64 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:64 
msgid "あ、甘い……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:65 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:65 
msgid "そっ、甘いの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:66 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:67 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:67 
msgid "それ……多分、整髪料……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:68 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:68 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0151:69 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:69 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:70 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:70 
msgid "……あ、逃げた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0151:71 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0151:71 
msgid "……なんであの蝶、とまったんだろ。\n"
msgstr ""
