#: sc/0058:0 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0058:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0058:1 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:1 
msgid "（さて、今度はどのタイミングで鹿が来る？\n　来ないという事だけはありえないが……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:2 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:2 
msgid "コジロウ、まだ感じないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:3 
#, fuzzy
msgid "コジロウ"
msgstr "Koujirou"

#  text 
#  warnings: 0 
#: sc/0058:3 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:4 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:4 
msgid "ふむ、まだか。\nとなれば……今朝と同じ場所か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:5 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:5 
msgid "今ならまだ退く事は出来る。\n違う道を通ればいいだけの話だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:6 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:6 
msgid "だが、しかし。\n笹原家長男として、逃げるわけにはいかん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:7 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:7 
msgid "……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:8 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:8 
msgid "やはり今朝と同じ場所か！\n行くぞ、コジロウ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:9 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:9 
msgid "こ、これは……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:10 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0058:10 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0058:11 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:11 
msgid "校長！！\nな、何故、校長が鹿に！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:12 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:12 
msgid "おのれ鹿！\nこうなれば校長の弔い合戦だ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0058:13 
msgid "どうして鹿が襲ってくんだよ！？\n先回りして良い画を撮ろうとしただけなのに！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0058:14 
msgid "俺が知るかよ！！\nそんなことより、アンテナが取れたぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0058:15 
msgid "アンテナが取れた！？\nいや、ならラッキ―だ、取れただけなら弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:16 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:16 
msgid "うおおおおお！！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0058:17 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0058:17 
msgid "踏んでった―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:18 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:18 
msgid "ここか！？\nよし、コジロウ応戦の準備……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:19 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:19 
msgid "何だコレは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:20 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:20 
msgid "（何だこの白く珍妙な被り物は！\n　それも１つじゃない、３つだと！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:21 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:21 
msgid "むぅ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:22 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:22 
msgid "（中央の者は女子のようだな。\n　もう一人は……子供であろうか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:23 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:23 
msgid "（そして最後……四足の獣。\n　いや、こやつは弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:24 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:24 
msgid "どう好意的に見ようが鹿だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:25 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:25 
msgid "なるほど……つまりそれは防具か！\nそして残る二人は仲間……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:26 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:26 
msgid "ならば、目には目を。\n仲間には仲間を。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:27 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:27 
msgid "どうだ、これで数の上では同数。\n貴様の優位は無くなったぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:28 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:28 
msgid "！！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:29 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:29 
msgid "さて……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0058:30 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0058:30 
msgid "コジロウ、少学校に忘れ物をした。\n一旦戻ってくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:31 
msgid "執事"
msgstr "Butler"

#  text 
#  warnings: 0 
#: sc/0058:31 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:32 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:32 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:33 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:33 
msgid "……後は頼んだぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:34 
msgid "執事"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:34 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:35 
msgid "行くぞコジロウ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:36 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:36 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:37 
msgid "執事"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:37 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:38 
msgid "執事"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:38 
msgid "うおおおおおお！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:39 
msgid "いや、野暮な突っ込みはよそう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:40 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:40 
msgid "このかぶいた衣装で並んで待っているのだ。\n私に求められているのはただ１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:41 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:41 
msgid "すなわち、私もかぶるべきであろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:42 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:42 
msgid "さあ、私にもそのかぶりものをよこすがいい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:43 
msgid "大福くん（鹿）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:44 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:45 
msgid "な、なんと。\nもう予備が無いのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:46 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:46 
msgid "弌弌くっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:47 
msgid "まさか、こんなことで\n敗北の味を教えられるとは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:48 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:48 
msgid "なるほど、鹿の分際で実に頭脳的な\n攻撃に出てきたな……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:49 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:49 
msgid "しかし、次は通用せんぞ！\nコジロウ！　一時退却だ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:50 
msgid "大福くん（鹿）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:50 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:51 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:51 
msgid "……おお、思い出した。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:52 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:52 
msgid "そうだ、これは確か大福くんとやらだ。\n以前見たことがある。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:53 
msgid "しかし、生憎ながら大福は欲しておらん。\n……ふむ、困ったな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:54 
msgid "大福くん（鹿）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:54 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:55 
msgid "……そんな目をされても困るな。\n大福なぞ要らん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:56 
msgid "大福くん（鹿）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:56 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:57 
msgid "ええい、どれだけ言おうと要らんからな。\nコジロウ、行くぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:58 
msgid "……途中で大福屋に寄るぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:59 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:59 
msgid "弌弌あれだけ言われたら、\n流石に気持ちも揺らぐというものだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:60 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:60 
msgid "今日のおやつは大福とお茶で決まりだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:61 
msgid "大福くん（ゆっこ）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:61 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:62 
msgid "大福くん（ゆっこ）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:62 
msgid "……何にも言ってないよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0058:63 
msgid "大福くん（きよし）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0058:63 
msgid "うん。\n"
msgstr ""
