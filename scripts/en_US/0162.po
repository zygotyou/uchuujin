#  speaker 
#  warnings: 0 
#: sc/0162:0 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0162:0 
msgid "大福〜、大福はいかがですか〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:1 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0162:1 
msgid "（あっ、何かやってる……大福フェア？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:2 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:2 
msgid "甘くて丸くて白いよ〜。\n白さでうちの大福に勝てる店は無いよ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:3 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:3 
msgid "大福かぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:4 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:4 
msgid "お茶菓子に良さそうだし\n買って帰ろうかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:5 
msgid "マコちゃんも食べるかな？\nちょっと多めに買っていって弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:6 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:6 
msgid "弌弌あっ。この匂い……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:7 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:7 
msgid "コ―ヒ―の匂い……\n大工カフェからだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:8 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:8 
msgid "う―、大福もいいけど、大工カフェの\nケ―キとコ―ヒ―も美味しそう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:9 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:9 
msgid "う―ん……\nどっちを買って帰ろうかな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:10 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0162:10 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0162:11 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:11 
msgid "……あっ、猫さんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:12 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:12 
msgid "ここは人が多いから危ないよ？\n踏まれないように気をつけてね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:13 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:13 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0162:14 
msgid "猫相手でも優しいなぁ。\n俺もあんな風に優しくされたいな―。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0162:15 
msgid "ホントホント。\nあ―あ、何で俺、猫じゃないんだろう。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0162:16 
msgid "まったくだ、あはははは。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0162:17 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0162:17 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0162:18 
msgid "あ―――！\nアンテナ、折れてる――！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0162:19 
msgid "な、何があったんだ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:20 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:20 
msgid "あっ、スカ―トに何か引っかかっちゃった！\nね、猫さんは大丈夫？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0162:21 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0162:21 
msgid "そんなことで――――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:22 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:22 
msgid "……そうだ。一回見に行って\n美味しそうだった方を買おう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:23 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:23 
msgid "スイマセン、ちょっと見させてください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:24 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:25 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:25 
msgid "大福をそんな不純な目で見ないでくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:26 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:26 
msgid "……えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:27 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:27 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:28 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:28 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:29 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:29 
msgid "不純な目……ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:30 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:30 
msgid "そうだ。うちの大福を\nケ―キなんかと同列に見ていただろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:31 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:31 
msgid "あっ、美味しそうな方を買おうかな―って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:32 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:32 
msgid "うちの大福の方がうまい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:33 
msgid "でも、ケ―キも美味しいですよ？\n生徒さんからよく話聞きますし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:34 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:34 
msgid "ロ―ルケ―キは生地がふわふわで\nクリ―ムも甘くて、本当に美味しいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:35 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:35 
msgid "大福を仞めるな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:36 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:36 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:37 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:37 
msgid "ロ―ルケ―キを仞めないでください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:38 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:39 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:39 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:40 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:40 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:41 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:41 
msgid "……なら、ロ―ルケ―キを買いなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:42 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:42 
msgid "……はいっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:43 
msgid "あ、ス、スイマセン……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:44 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:44 
msgid "全く……これだから大福の良さを知らん者は。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:45 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:45 
msgid "スイマセン……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:46 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:46 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:47 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:47 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:48 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:48 
msgid "……大工カフェに行こう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:49 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:49 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:50 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:50 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:51 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:51 
msgid "（不純な目って何だろう……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:52 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:52 
msgid "（不純な目？　よくわからないけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:53 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:53 
msgid "ん―、大福とケ―キ、\nどっちにしようかな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:54 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:54 
msgid "だから、大福を不純な目で見るんじゃない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:55 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:55 
msgid "（あっ、でもケ―キは前に食べたし。\n　久しぶりに大福にしよう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:56 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:56 
msgid "おい、聞いているのかね？\n大福を不純な目で弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:57 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:57 
msgid "あ、スイマセン、大福４つください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:58 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:59 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:59 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:60 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:60 
msgid "澄んだ目をしている……\nそうか、あんたも大福の良さに気付いたか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:61 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:61 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:62 
msgid "これはサ―ビスだ。\nタダであげよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:63 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:63 
msgid "えっ、えっ？　いいんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:64 
msgid "もちろんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:65 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:65 
msgid "わ―、ありがとうございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0162:66 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0162:66 
msgid "（大福の良さとかよくわからないけど……\n　でも、いっか）\n"
msgstr ""
