#: sc/0090:0 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0090:0 
msgid "え―っと、ゆっこがバナジュ―で、\n私は……なんにしようかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:1 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:1 
msgid "う―ん……\nオレンジって気分かな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:2 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0090:2 
msgid "む？　貴様は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:3 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:3 
msgid "え？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0090:4 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:4 
msgid "（さ、さ、さ、笹原先輩―――――！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:5 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:5 
msgid "近頃良く会うな。……おお、自販機と言えば、\n先日は貴重な金子をすまなかった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:6 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:6 
msgid "え、あ、ハイっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:7 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:7 
msgid "ふむ、やはり貴様もジュ―スを買うのか。\nなにを所望するのだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:8 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:8 
msgid "え、えと……私は、オレンジを……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:9 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:9 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0090:10 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:10 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:11 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:11 
msgid "あれ？　あれ？　あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:12 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:12 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0090:13 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:13 
msgid "（またか―！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:14 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:14 
msgid "ふむ……この自販機……\n一時限りの戯れかと思ったが弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:15 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:15 
msgid "どうして、稀代の傾奇者と見える。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:16 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:16 
msgid "えっ？　あっ、はいっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:17 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:17 
msgid "止むを得ん、外の自販機に行こう。\n……ちと遠いが、帯同するか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:18 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:18 
msgid "えっ！？　は、はい！　喜んで！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:19 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:19 
msgid "（きゃ〜！\n　笹原先輩と並んで歩けるなんて〜！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:20 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0090:20 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:21 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:21 
msgid "……オレンジジュ―ス？\nホットコ―ヒ―を選んだのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:22 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:22 
msgid "いや、これは冷たい物を飲むべきと言う\n運命なのかもしれん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:23 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:23 
msgid "では、一気に弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:24 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:24 
msgid "あづ―――――――――！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:25 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:25 
msgid "（しまった！\n　思わずカップを放り投げて弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:26 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:26 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0090:27 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:27 
msgid "あづ―――――――――！！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0090:28 
msgid "あ。\nアンテナ、壊れた。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0090:29 
msgid "ホントだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:30 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:30 
msgid "（けど……さすがにちょっとくやしいなぁ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:31 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:31 
msgid "（私のお小遣いだって、\n　そんなに多いわけじゃないし……よし！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:32 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:32 
msgid "さ、笹原先輩！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:33 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:33 
msgid "む？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:34 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:34 
msgid "先輩って、詩、得意なんですよね。\n一句詠んでくれませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:35 
msgid "ほう、貴様も風流を解すのか。\nよかろう。う―む……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:36 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:36 
msgid "（今だ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:37 
msgid "久方の弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:38 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:38 
msgid "ジョイヤ―ッ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:39 
msgid "じょいや―響ける春の日に弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:40 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:40 
msgid "ヘヤッ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:41 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:41 
msgid "かかとめりこみ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:42 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:42 
msgid "セアッ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:43 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:43 
msgid "じゅ―す賜わん〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:44 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:44 
msgid "（ここまでやっても出ないか……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:45 
msgid "どうであった？\n我ながら良い出来だと思うのだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:46 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:46 
msgid "あ、あ〜、えっと……\nもう一句お願いしていいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:47 
msgid "ふふ、仭欲なやつめ。\nしかし、よかろう。では……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:48 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:48 
msgid "ヘグラァッ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:49 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:49 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:50 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:50 
msgid "…………あっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:51 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:51 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:52 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:52 
msgid "え、あ、あの……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:53 
msgid "自販機よ、金を返せと、殴り蹴る。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:54 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:54 
msgid "うむ、傑作だ！！\n貴様のおかげだ。感謝するぞ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:55 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:55 
msgid "へっ！？　は、はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:56 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:56 
msgid "（せ、先輩が、\n　私の手を握って〜〜〜〜〜〜！！）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0090:57 
msgid "みお"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0090:57 
msgid "（わ、私、壊れちゃいそう〜〜〜〜〜〜）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:58 
msgid "ぬ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:59 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:59 
msgid "ど、どう……かな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:60 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:60 
msgid "あっ、やった―！　先輩、出ました！\nオレンジジュ―ス出ましたよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:61 
msgid "ほう、これは丐倖。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:62 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:62 
msgid "ん――、美味しいです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:63 
msgid "では、私も失敬。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:64 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:64 
msgid "え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:65 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:65 
msgid "〜〜〜〜〜〜〜〜〜〜！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:66 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:66 
msgid "うむ、美味。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:67 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:67 
msgid "（か、か、間接キス〜〜〜〜〜〜〜〜！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:68 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:68 
msgid "おっと。あまり飲んでは悪いな。\nそら、返そう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:69 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:69 
msgid "え、あ、はい、どうも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:70 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:70 
msgid "では、私はこれで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:71 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:71 
msgid "あ、はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:72 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:72 
msgid "このコップ……ごくり、\n私……飲んでいいんだよね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:73 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:73 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:74 
msgid "カップが！？\n……狙撃っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:75 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:76 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:76 
msgid "……なぜ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:77 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:77 
msgid "（こ、これだけやれば、自販機だって\n　わかってくれるでしょ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:78 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:78 
msgid "あっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:79 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:79 
msgid "む？　どうした。\nジュ―スが出たのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:80 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:80 
msgid "まだですけど……\n動いてるから、たぶん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:81 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:81 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:82 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:82 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:83 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:83 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:84 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:84 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:85 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:85 
msgid "…………ごくん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:86 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:86 
msgid "……オレンジジュ―スです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0090:87 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0090:87 
msgid "……良き哉。\n"
msgstr ""
