#  speaker 
#  warnings: 0 
#: sc/0011:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0011:0 
msgid "くそ―！　せっかく大金出して買ったのに\nお守りの効果全然ないじゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:1 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:1 
msgid "あのおばあさん、どこにいる！？\n見つけ出してお金返してもらう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:2 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:2 
msgid "くっ、とは言え\nまだこの辺りにいるかどうか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:3 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:3 
msgid "初めて見た人だから\nこの街の人じゃないだろうし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:4 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:4 
msgid "とにかく探し回るしかない。\n絶対に見つけてやる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:5 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:5 
msgid "あっ！？　見つけた！！\nさっきのおばあさんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:6 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0011:6 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0011:7 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:7 
msgid "わっ、すみません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:8 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:8 
msgid "でもそれどこじゃないんで！\n待て―！　おばあさ―ん！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0011:9 
msgid "おい、アンテナのダメ―ジ状態を調べろ！\n今の衝撃で破損したかもしれん！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0011:10 
msgid "了解！　破損レベル確認！\nアンテナ弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0011:11 
msgid "アンテナ……アウトです。\n脆すぎま―す！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0011:12 
msgid "やっぱりか――――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:13 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:13 
msgid "ちょっとおばあさん！\nどういうこと！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:14 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0011:14 
msgid "はい？　あっ、さっきの悪霊の……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:15 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:15 
msgid "それじゃまるで私が悪霊みたいじゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:16 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:16 
msgid "何を怒っているんです？\n悪霊は除霊されたでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:17 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:17 
msgid "全然だよ！\n全くもってこれっぽっちも効果ないよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:18 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:18 
msgid "ふむ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:19 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:19 
msgid "うわっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:20 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:20 
msgid "な、何！？　びっくりしたぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:21 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:21 
msgid "あなたについていた悪霊、\nどうやら本性を現したようですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:22 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:22 
msgid "へっ？　本性？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:23 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:23 
msgid "まんまと佶されました。さっきまでは\n弱い悪霊の振りをしていたようです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:24 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:24 
msgid "ええっ！？　ど、どうするの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:25 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:25 
msgid "こうなったら私が知っている手段は１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:26 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:26 
msgid "この高価なお守りを\n身につけるしかありません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:27 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:27 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:28 
msgid "もういいよお守りは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:29 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:29 
msgid "いいんですか？　さっきのとは\n効果も値段も数倍のお守りですよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:30 
msgid "すすす、数倍！？\nあれよりまだ高くなるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:31 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:31 
msgid "除霊とはそういうものです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:32 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:32 
msgid "それで、どうします？\n悪霊は本性を現しましたよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:33 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:33 
msgid "うっ……で、でも、もうお金は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:34 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:34 
msgid "いくらあります？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:35 
msgid "え？　５千円……\nでも値段が数倍だと５千円じゃ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:36 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:36 
msgid "じゃあサ―ビスで５千円で。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:37 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:37 
msgid "本当！？　買う！　買います！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:38 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:38 
msgid "はい。まいどあり―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:39 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:39 
msgid "……これで本当に除霊ができるんですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:40 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:40 
msgid "はい。……あ、もう効果が。\n悪霊が苦しんでいます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:41 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:41 
msgid "ほ、本当！？\nすげ―！　このお守りすげ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:42 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:42 
msgid "大事にしてください。\nあなたの身を守る物ですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:43 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:43 
msgid "はい！　ありがとう、おばあさん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:44 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:44 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0011:45 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:45 
msgid "フッ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:46 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:46 
msgid "またそうやって偽物を売る気？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:47 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:47 
msgid "偽物じゃないです。本物です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:48 
msgid "でも、こっちのお守りは効果なかったし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:49 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:49 
msgid "悪霊が強いんです。だからこそ\n２つのお守りの力を合わせるのです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:50 
msgid "（えっ？　何それ。ちょっとカッコイイ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:51 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:51 
msgid "どうするんです？　こっちを買わないと\nそのお守りは効果ありませんよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:52 
msgid "……値段次第。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:53 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:53 
msgid "希望は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:54 
msgid "タダ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:55 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:55 
msgid "１万円。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:56 
msgid "３千円。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:57 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:57 
msgid "７千円。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:58 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:59 
msgid "ゆっこ＆イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:59 
msgid "５千円。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:60 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:61 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:61 
msgid "買った。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:62 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:62 
msgid "まいどあり―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:63 
msgid "ちょっと待って。そもそも最初に買ったお守りが\n効果なかったのはそっちの責任でしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:64 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:64 
msgid "ではお詫びも兼ねてこのお守りの値段を\n５千円まで下げましょう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:65 
msgid "５千円か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:66 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:66 
msgid "（厳しい値段だけど……でも、\n　悪霊が本性を現した今、頼れるのは……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:67 
msgid "……買ったら悪霊が除霊されるんですね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:68 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:68 
msgid "もちろん。私を信じてください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:69 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:69 
msgid "（信じれね―……\n　だけど、信じるしか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:70 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:70 
msgid "……それじゃあ、買います。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:71 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:71 
msgid "はい。まいどあり。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:72 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:72 
msgid "あの、今度こそこのお守りで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:73 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:73 
msgid "それでは、私は私用があるのでこれで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:74 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:74 
msgid "あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:75 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0011:76 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0011:76 
msgid "……うん。信じよう。\n"
msgstr ""
