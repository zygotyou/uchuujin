#  speaker 
#  warnings: 0 
#: sc/0024:0 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0024:0 
msgid "東雲なの……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:1 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:1 
msgid "あいつを捕獲して\nあの体を調べつくしたい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:2 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:2 
msgid "……と、何度思ったことか。\nはぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:3 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:3 
msgid "今まで何度も捕獲を試みたが、\n失敗続きだからな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:4 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:4 
msgid "だが、それも今日までだ。\n東雲との長い戦いも今日で幕を引くぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:5 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:5 
msgid "なんせ風の噂で東雲が雷に弱いと\n聞いたからな。これなら捕獲は簡単だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:6 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:6 
msgid "まずは東雲を呼び出し、\n部屋に入ってきたところで雷の動画を見せる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:7 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:7 
msgid "そして動けなくなった東雲を捕獲……\nふふ、完亢だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:8 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:8 
msgid "さて。\n早速、東雲に見せる動画を探すか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:9 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:9 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0024:10 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:10 
msgid "……うむ。これでいいか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:11 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:11 
msgid "後はリピ―ト再生にして、\n音量ももっともっと大きく……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:12 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:12 
msgid "お―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:13 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:14 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:14 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:15 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:15 
msgid "（私……雷苦手だった……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:16 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0024:16 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:17 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:17 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:18 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:18 
msgid "……あれ？　音が止まった？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:19 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:19 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:20 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:20 
msgid "校長！？　いつの間に！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:21 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:21 
msgid "（気が付いたら、すぐ近くにいた。\n　そしてドアが開く音はしなかった弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:22 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:22 
msgid "（弌弌ということはつまり……\n　校長はずっと部屋にいて、見ていた！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:23 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:23 
msgid "うわああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 6 
#: sc/0024:24 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 6 
#: sc/0024:24 
msgid "！！？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0024:25 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0024:25 
msgid "えええ――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0024:26 
msgid "ちょ、どうして攻撃されたんだ？\n俺は人助けをしたはずなんだけど！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0024:27 
msgid "ああ、確かに人助けをした！\nお前は間違っていないさ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0024:28 
msgid "だよな？　だよな？\n一体彼女は何が気に食わなかったんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0024:29 
msgid "……わからん。\n大人の女は謎が多すぎるぜ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0024:30 
msgid "お前ら、そんなこと後で考えればいいだろ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0024:31 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0024:31 
msgid "ああああ！！\nそうだった―――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:32 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:32 
msgid "わっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:33 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:33 
msgid "（う〜〜！　怖くて体が動かない……\n　誰か、動画を止めてくれ……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:34 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:34 
msgid "ひっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:35 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0024:35 
msgid "失礼しま―す。あの、ここから\n雷の音が聞こえるんですけど何を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:36 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:36 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:37 
msgid "……中村先生？　何してるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:38 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:38 
msgid "うううっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:39 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:39 
msgid "……あっ。なるほど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:40 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:40 
msgid "先生、この動画止めますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:41 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:41 
msgid "（う……と、止まった……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:42 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:42 
msgid "大丈夫ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:43 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:43 
msgid "え？　あ……中之条……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:44 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:44 
msgid "先生、雷が怖いんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:45 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:45 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0024:46 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:46 
msgid "（怖い。怖いが……それをわざわざ\n　こいつには言いたくはない……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:47 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:47 
msgid "……怖くない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:48 
msgid "え？　でも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:49 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:49 
msgid "こ、怖ければわざわざ\n動画を見るわけがないだろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:50 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:50 
msgid "それもそうですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:51 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:51 
msgid "いいか？　勘違いするなよ。私は研究の一環で\nこの動画を見ていただけだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:52 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:52 
msgid "えっ？　研究ですか？\nどんな研究をしているんです？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:53 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:53 
msgid "（うわ……なんか食いついた。\n　めんどくさいなこいつ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:54 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:54 
msgid "何か僕に手伝えることはありますか？\n何でもしますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:55 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:55 
msgid "……じゃあ、実験台になってくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:56 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:56 
msgid "はい！　何の実験ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:57 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:57 
msgid "こいつだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:58 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:58 
msgid "んぎゃああああああぁ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:59 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:60 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:61 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:61 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:62 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:62 
msgid "逃げろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:63 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:63 
msgid "い、いや、別に怖いとか\nそういうことではなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:64 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:64 
msgid "でも怖いならどうして雷の動画なんか\n見てたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:65 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:65 
msgid "だから怖いわけでは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:66 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:66 
msgid "もしかして苦手な雷を克服するために\n慣れようとしてたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:67 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:67 
msgid "違う。私は苦手でもないし\n怖くもないから……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:68 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:69 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:69 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:70 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:70 
msgid "……でも、雷って怖いですよね。\n僕もちょっと苦手かな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:71 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:71 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:72 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:72 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:73 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:73 
msgid "……ま、まあ、少しだけな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:74 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:74 
msgid "ははは。そうですよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:75 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:75 
msgid "でも、雷が苦手なんて、\n可愛いところがあるんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:76 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:77 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:77 
msgid "えっ！？　中村先生！？\nどこに行くんですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:78 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:78 
msgid "くっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:79 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:79 
msgid "こ、怖くなんかない。\nな、何ならまた鳴らしてみようか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:80 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:80 
msgid "え？　いえ、別に鳴らさなくても……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:81 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:81 
msgid "え、遠慮するな。\nまさか雷が怖いなんてことはないだろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:82 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:82 
msgid "は、はぁ。でも先生が怖いんじゃ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:83 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:83 
msgid "怖くない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:84 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:84 
msgid "は、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:85 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:85 
msgid "怖くないからな。\nだからもう一度鳴らしてもなんてことない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:86 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:86 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0024:87 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:87 
msgid "いくぞ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:88 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:88 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:89 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:89 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:90 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:90 
msgid "ひっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:91 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:91 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0024:92 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0024:92 
msgid "あっ……。\n"
msgstr ""
