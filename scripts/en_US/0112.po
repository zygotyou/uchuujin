#: sc/0112:0 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0112:0 
msgid "そういえば、次の授業は化学であったか。\nトイレが済んだら移動せねばならんな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:1 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:1 
msgid "……む、課題が出ていたのを失念していた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:2 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:2 
msgid "私とした事が迂佛な。\n仕方が無い、こうなれば誰かに弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:3 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0112:3 
msgid "えっ……あわわわっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:4 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:4 
msgid "むう……っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:5 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:5 
msgid "……何とか止まれたか。\n危ない所であった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:6 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:6 
msgid "はぁ……ぶつからなくて良かった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:7 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:7 
msgid "済まなかったな、こちらの前方不注意だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:8 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:8 
msgid "あっ、いえっ！　こちらこそ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:9 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:9 
msgid "しかしお互い大事無くて一安心弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:10 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:10 
msgid "笹原ァ――――！！\nあんた下級生に何やってんのよ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:11 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:11 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0112:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0112:12 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:13 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:13 
msgid "なっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:14 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:14 
msgid "ワンちゃんが！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:15 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:15 
msgid "よ、よけて――――っ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0112:16 
msgid "…………。\n"
msgstr ".........\n"

#  text 
#  warnings: 1 
#: sc/0112:17 
msgid "……バリア、間に合ったか。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0112:18 
msgid "いいや、手遅れさ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0112:19 
msgid "そ、そんな……あんなにかわいい犬を\n犠牲にしてしまったなんて……俺は……俺は！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0112:20 
msgid "は、何言ってんだ、犬は無事だぜ？\nアンテナのバリアが間に合わなかったんだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0112:21 
msgid "なるほど、そりゃ手遅れだ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0112:22 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0112:22 
msgid "でも、犬は助かって良かったぜ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:23 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:23 
msgid "きゃぁぁぁぁっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:24 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:24 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:25 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:25 
msgid "あいたたた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:26 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:26 
msgid "あっちゃ〜、下級生巻き込んじゃっ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:27 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:27 
msgid "え……ええ……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:28 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:28 
msgid "……え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:29 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:29 
msgid "あ、あわわわ……っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:30 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:30 
msgid "あ……ああっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:31 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:31 
msgid "ふむ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:32 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:32 
msgid "素晴らしい！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:33 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:33 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:34 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:34 
msgid "まさかここで爆発にあわせて手を飛ばすとは！\nなんたる胆力、なんたる傾きぶり！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:35 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:35 
msgid "は、はい！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:36 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:36 
msgid "気に入った！\nこれが噂のロボッ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:37 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:37 
msgid "笹原ァ！\nアンタそれより先にやる事あるでしょうが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:38 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:38 
msgid "やる事……？\nなるほど、確かにそうだったな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:39 
msgid "すまんな立花みさと。\n礼を言うぞ、危うく己を見失う所だった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:40 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:40 
msgid "べ、別に礼なんていいから早くやりなさいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:41 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:41 
msgid "うむ、ではトイレへ馳せ参じてくる。\n急いでいたのを忘れておったわ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:42 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:42 
msgid "そっちじゃない！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:43 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:43 
msgid "手、手がっ！　手が！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:44 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:44 
msgid "落ち着け立花みさと。\nそれはそこの娘がやった手品だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:45 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:45 
msgid "え、そ、そうなの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:46 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:46 
msgid "あ……はっ、はい！\nそうです！　そうなんですよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:47 
msgid "立花みさとが驚かせるから、\nとっさに驚かせ返したらしいな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:48 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:48 
msgid "いやはや、何とも機転の利く娘だ。\n天晴れとしか言いようが無いな。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0112:49 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0112:49 
msgid "う、うん……？\n…何か違和感が弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:50 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:50 
msgid "ホントですよ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:51 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:51 
msgid "だって、そんな、人間の手が\n飛ぶわけないじゃないですか〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:52 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:52 
msgid "そうだよね……あははははは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:53 
msgid "そういうわけだ。\n決して噂のロボット生徒ではないからな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:54 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:54 
msgid "ぎゃっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:55 
msgid "おっと、こんな場合ではなかった。\nトイレへ急がねば。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:56 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:56 
msgid "えっ、ええ〜っ！？\nあの、私のこの状態は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:57 
msgid "トイレの前には全て些事。\nそれでは失礼する。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:58 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:58 
msgid "ちょ、ちょっと待ちなさい！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:59 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:59 
msgid "アンタのせいでしょアンタの！！\n責任とって何とかしなさいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:60 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:60 
msgid "落ち着け立花みさと、\nどう考えてもお前のせいではないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:61 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:61 
msgid "アンタが下級生に\n変なことしてたせいでしょうが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:62 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:62 
msgid "ただぶつかりそうになって避けただけだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:63 
msgid "あ、そ、そうなんですよ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:64 
msgid "……あ、そ、そうだったんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:65 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:65 
msgid "うむ。\nそういうわけだからトイレへ行かせてくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:66 
msgid "え……う、うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:67 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:67 
msgid "では二人とも達者でな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:68 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0112:69 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0112:69 
msgid "あっ、ちょ、ちょっと！\n手とか拾うの手伝いなさいよ〜〜〜！！\n"
msgstr ""
