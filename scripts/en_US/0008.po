#  speaker 
#  warnings: 0 
#: sc/0008:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0008:0 
msgid "はぁ……今日はさんざんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:1 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:1 
msgid "滑ることがこんなに怖いのは久しぶりだ……\n怖くて普段の力が出せないよ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:2 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:2 
msgid "はぁ。帰る前に何か飲んで気分転換しよっ。\n何にしようかな〜？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:3 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0008:3 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0008:4 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:4 
msgid "ぎゃあああああ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:5 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:5 
msgid "な、何故にここにお前が……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:6 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:6 
msgid "ま、まさか、また私のボケを滑らせるために\nここまでやってきた！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:7 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:7 
msgid "くそっ……\n私をいじめて何が楽しいのさ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:8 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:8 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:9 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:9 
msgid "……いや、お前は悪くないか。\nお前はずっと忠告をしてくれたんだよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:10 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:10 
msgid "なのに私は忠告を無視してボケた。\n悪いのは私でお前に非はないよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:11 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:11 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:12 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:12 
msgid "はは、でも今はもうボケる気力もないし、\n心配しなくても大丈夫だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:13 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:13 
msgid "お前も何か飲む？　私が乂ってあげるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:14 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:14 
msgid "犬はコ―ラ大丈夫かな。\n飲んでみる〜？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:15 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:15 
msgid "あっ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:16 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:16 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:17 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:17 
msgid "ご、ごめん、つまずいて……\nあ―あ、びしょびしょだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0008:18 
msgid "……ん？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0008:19 
msgid "なんでっ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0008:20 
msgid "ジュ―スがアンテナにかかったからだ！\nショ―トしやがった！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0008:21 
msgid "えええ―っ！？\nた、耐水性じゃないのかよ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0008:22 
msgid "俺も今知ったよ！\n設計スタッフは何考えてんだ――！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:23 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:23 
msgid "さっ、何か飲んで\n気持ちを入れ替えるか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:24 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:24 
msgid "さ―て、何にしようかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:25 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:25 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:26 
msgid "あれ？　あれ？　お金入れたのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:27 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:27 
msgid "えっ？　嘘！？\nお金、自販機に飲まれた！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:28 
msgid "返却口には……くっ！　落ちてない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:29 
msgid "くそ―！　戻って来い！　戻って来い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:30 
msgid "このポンコツが―！\nお金返せ――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:31 
msgid "せいや！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:32 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:32 
msgid "おっ！　動いた？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:33 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:33 
msgid "ん？　ん？　どうなったのコレ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:34 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:34 
msgid "うわっ！？　な、なにこの音！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:35 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0008:35 
msgid "こら―！　相生―！\n何をやってるんだ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:36 
msgid "た、高崎先生！？\nあ、あの、これは、違うんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:37 
msgid "何が違うんだ！\nお前が自販機を殴ったところを見たぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:38 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0008:39 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:39 
msgid "相生！　ちょっと職員室まで来い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:40 
msgid "う、うう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:41 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:41 
msgid "うわああああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:42 
msgid "あっ！？　相生！　待て―――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:43 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:44 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:44 
msgid "はぁぁ……ついてないのも\nここまで来ると笑えてくるね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:45 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:45 
msgid "ま、１杯２００円と思えばいいか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:46 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:46 
msgid "コ―ラっと。\n……ごくごく。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:47 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:47 
msgid "ブ―――――――！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:48 
msgid "あったか〜い！\nどうしてコ―ラがあったかいのさ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:49 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:49 
msgid "つめた〜いコ―ナ―にあるのに！\nあったか〜いバ―ジョンないのに！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:50 
msgid "冷たいのをゴクゴク飲んで\n爽やかな気持ちになりたかったのにっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:51 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:51 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:52 
msgid "はぁ……\n今日はなんてついてないの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:53 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:53 
msgid "自販機にまでこんな扱いを受けて……\n惨めね、ゆっこ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:54 
msgid "もう今日は素直に帰ろう。\nこれ以上傷口を広げないために……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:55 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:56 
msgid "田中"
msgstr "Tanaka"

#  text 
#  warnings: 0 
#: sc/0008:56 
msgid "さ―って、何飲もうかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0008:57 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0008:57 
msgid "あれ？　お金入ってるぞ？\nもしかして買えたりする？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0008:58 
msgid "田中"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0008:58 
msgid "お―、ラッキ―\n誰だか知らないけどサンキュ―。\n"
msgstr ""
