#: sc/0036:0 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0036:0 
msgid "僕の夢は科学者になることだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:1 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:1 
msgid "だから世の中の幽霊だどうたらという\n霊的現象は全く信じていない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:2 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:2 
msgid "いないが……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:3 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:3 
msgid "何故……僕の髪の毛が\nこんな風になってるんだ……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:4 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:4 
msgid "何か違和感があると思ったら……\nうっすらと透けている……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:5 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:5 
msgid "ここに確かにあるのに……\n触ることができない……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:6 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:6 
msgid "そう、まるで幽霊のように、\nここにあるのに触ることができない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:7 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:7 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0036:8 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:8 
msgid "……そうか、これは夢だ。\nきっと僕は今、悪い夢を見て弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:9 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:9 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:10 
msgid "……やめよう。例え信じられない事でも\n受け入れなければいけない時がある。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:11 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:11 
msgid "それが今だ……悲観に走る暇があるのなら\n学校に行くまでに問題を解決しないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:12 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:12 
msgid "とは言え、触れもしない物を相手に\n僕はどんな方法を取れば弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:13 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:13 
msgid "……ん？　これは……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:14 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:14 
msgid "育毛剤だ……。\nもしかして父さんが使ってるのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:15 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:15 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:16 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0036:16 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:17 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:17 
msgid "ハッ！？　いつの間に！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0036:18 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0036:18 
