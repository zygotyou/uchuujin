#  speaker 
#  warnings: 0 
#: sc/0098:0 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0098:0 
msgid "……よし。\nここまでが次のテスト範囲だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:1 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:1 
msgid "もちろん前回の授業で出た元素記号も\n問題として出すので覚えるように。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:2 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0098:2 
msgid "ええ―！　こんなの覚えられませ―ん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:3 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:3 
msgid "教科書の７４ペ―ジを見ろ。\n元素記号はここに全て書かれている。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:4 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:4 
msgid "覚えやすくするために、\n歌にして覚えるのもアリだ。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0098:5 
msgid "田中"
msgstr "Tanaka"

#  text 
#  warnings: 2 
#: sc/0098:5 
msgid "すいへ―り―べ―ぼくのふね\nってやつだよね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:6 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:6 
msgid "そうだ。自分の覚えやすい歌にして覚えろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:7 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0098:7 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0098:8 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:8 
msgid "……ん？　校長？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:9 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:9 
msgid "……あ、あわわ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:10 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:10 
msgid "な、なにか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:11 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:11 
msgid "ふ……普通に授業してる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:12 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:12 
msgid "！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0098:13 
msgid "おい、オッサンになんてこと言わせてんだよ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0098:14 
msgid "で、でも！\nお前もそう思うだろ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0098:15 
msgid "ああ、いつも悪巧みしてるし、\n信じられないって！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0098:16 
msgid "まぁ、確かにそうだが弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0098:17 
msgid "って、オッサン！！\n何された――――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:18 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:18 
msgid "覚えやすい歌か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:19 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:19 
msgid "む？　あまり時間も無いな。\nでは残った時間で元素記号を覚えてみろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:20 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:20 
msgid "（さてと、残った時間で私は\n　東雲なのの捕獲方法でも……）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0098:21 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0098:21 
msgid "〜〜〜\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:22 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:22 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0098:23 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:23 
msgid "（なんだ？　勉強なのに\n　相生が楽しそうにしている？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:24 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:24 
msgid "（一体何を書いている？　元素記号だよな？\n　何か相生の気に入る要素があったか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:25 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:25 
msgid "ねえねえみおちゃん。これ見て。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:26 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0098:26 
msgid "ん？　…………プフッ！\n何やってるのアンタ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:27 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:27 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:28 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:28 
msgid "（な、何が書かれている？\n　くっ、悔しいが、気になるぞ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:29 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:29 
msgid "（気になるな……\n　よし。ちょっと覗きに行ってみるか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:30 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:30 
msgid "……あっ、ちょ、ヤバイゆっこ。\nすぐ消さないと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:31 
msgid "えっ？　うわっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:32 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:32 
msgid "（何っ！？　消す？　それほどまでに\n　私に見られてはマズイものなのか！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:33 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:33 
msgid "相生！　見せろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:34 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:34 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:35 
msgid "！！！\n"
msgstr "!!!\n"

#  speaker 
#  warnings: 0 
#: sc/0098:36 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:36 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:37 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:37 
msgid "（逃げた！？　ということはあのノ―トには\n　私に見られるとかなりまずいことが！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:38 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:38 
msgid "逃がすかっ！　待て相生！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:39 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:39 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:40 
msgid "はっ、はっ、はっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:41 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0098:41 
msgid "あっ、ろ、廊下を走っちゃだ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:42 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:42 
msgid "あっ……行っちゃった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:43 
msgid "…………あ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:44 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:44 
msgid "はぁ、はぁ、はぁ……\nま、待て、相生……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:45 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:45 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:46 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:46 
msgid "足、速すぎ……お、追いつけない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:47 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:47 
msgid "！？　えっ！？　えっ！？　中村先生！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:48 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:48 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:49 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:49 
msgid "（気になる……が、\n　つられて見に行くのも亶だな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:50 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:50 
msgid "（しかし一度気にしてしまうと\n　見たくて見たくてたまらなくなるな……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:51 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:51 
msgid "ねえゆっこ、ここはもっと\nこうした方がいいんじゃない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:52 
msgid "おお！　流石みお先生！\nさっきよりも全然良くなったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:53 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:53 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:54 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:54 
msgid "（な、何だ、一体何が\n　さっきよりも良くなったんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:55 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:55 
msgid "（それにさっきはどんな物だったんだ？\n　気になる気になる気になる……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:56 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:56 
msgid "（あっ、終わった……\n　結局ノ―トの中身は見られずじまいか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:57 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:57 
msgid "（…………あ、そうだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:58 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:58 
msgid "これで授業は終わる。\nあと、ノ―トを提出するように。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:59 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:59 
msgid "ふふふ、我ながら\n素晴らしい方法を思いついたものだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:60 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:60 
msgid "これなら誰にも見られることなく、\nいくらでも相生のノ―トの中身を見られるぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:61 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:61 
msgid "さて、相生のノ―トには一体何が書かれて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:62 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:63 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:63 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:64 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:64 
msgid "消されてる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:65 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:65 
msgid "（よし。さりげなく、\n　遠くからノ―トを覗いてやる）\n"
msgstr ""

#  speaker 
#  warnings: 3 
#: sc/0098:66 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0098:66 
msgid "〜〜〜\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:67 
msgid "でさ、こんな感じじゃないかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:68 
msgid "あはははは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:69 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:69 
msgid "（ここからノ―トを……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:70 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:70 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:71 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:71 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:72 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:72 
msgid "（似てね――――！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:73 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:73 
msgid "（と言うか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0098:74 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0098:74 
msgid "（自習しろよ……）\n"
msgstr ""
