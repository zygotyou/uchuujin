#  speaker 
#  warnings: 0 
#: sc/0081:0 
msgid "きよし"
msgstr "Kiyoshi"

#  text 
#  warnings: 0 
#: sc/0081:0 
msgid "はぁ、はぁ……おじいちゃんたち、\nどこへいったんだろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:1 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:1 
msgid "おじいちゃんがバディたちを捜しに行って、\nずいぶん経つけれど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:2 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:2 
msgid "もうバディたちに会えたのかな？\nもし、事故にでも巻き込まれていたら……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:3 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:3 
msgid "……だ、大丈夫だよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:4 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:4 
msgid "あ、そうだ。ここなら人が多いから……\n誰か見かけた人がいるかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:5 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:5 
msgid "あ、あの人に聞いてみよう。\nあの―、すいませ―ん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:6 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0081:6 
msgid "ん？　どうしたんだ？\n大福を買いたいのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:7 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:7 
msgid "あ、大福じゃなくて、\nおじいちゃんと犬を探してて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:8 
msgid "おじいちゃんと犬を？\nよくわからんが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:9 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:9 
msgid "そこにいるおじいさんじゃ駄目なのかね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:10 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0081:10 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0081:11 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:11 
msgid "駄目です。僕が捜してるのは\nおじいちゃんで……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:12 
msgid "おい、この子供の捜してる犬って……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:13 
msgid "ああ、アンテナをつけたあの犬だな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:14 
msgid "もしかして、俺たちのせいかな？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:15 
msgid "と言うか……姫のせいだろ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:16 
msgid "そうか！\n元はと言えば、姫が原因だもんな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:17 
msgid "ああ、俺たちは悪くない。\n悪いのは全部、姫だ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:18 
msgid "だよな！\nあはははは！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:19 
msgid "ははははは弌弌あれ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:20 
msgid "どうした？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:21 
msgid "お、音声が……ひ、姫に聞かれてた！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0081:22 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 2 
#: sc/0081:23 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0081:23 
msgid "うあああああああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:24 
msgid "キミ、バイトしていかないか？\n人手が足りずに困っているんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:25 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:25 
msgid "えっ、バイト？　でも僕弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:26 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:26 
msgid "大丈夫。バイトと言っても今日だけ、\nたった３〜４時間のバイトだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:27 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:27 
msgid "しかもバイト代は１万円！\nどうだ？　やってみないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:28 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:28 
msgid "嘘っ！？　１万円！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:29 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:29 
msgid "本当だ。だがそれは\nしっかりと働いたらの話だぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:30 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:30 
msgid "す、すごい……\n１万円なんてお小遣いの何倍だろう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:31 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:31 
msgid "どうだ？\nバイトやってみないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:32 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:32 
msgid "や、やります！\nやらせてください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:33 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:33 
msgid "よし。頑張るんだぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:34 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:34 
msgid "はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:35 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:35 
msgid "（おじいちゃんたち、ごめん！\n　僕、少しだけバイトしていくよ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:36 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:36 
msgid "（お土産買って帰るから待っててね！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:37 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:37 
msgid "あの、僕、小学生だから\nバイトはできないんで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:38 
msgid "これは町内会のイベントで出店しているんだ。\nただのバイトとは違うから大丈夫。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:39 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:39 
msgid "で、でも、もし先生とかに見つかったら\nきっと怒られるから……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:40 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:40 
msgid "なあに、バイト中はずっとコレをつけるから\n見つからないよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:41 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:41 
msgid "えっ？　わっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:42 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:42 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:43 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:43 
msgid "これで今からキミは大福くんだ。\n親にも先生にも正体はバレないぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:44 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:45 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:45 
msgid "（こ、これは……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:46 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:46 
msgid "（もしバレたら……いじめられる！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:47 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:47 
msgid "（うう、絶対にバレるわけにはいかないぞ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:48 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:48 
msgid "（い、１万円ってすごい……！\n　でも僕はおじいちゃんたちを捜さないと）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:49 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:49 
msgid "ごめんなさい。\n僕にはバイトをしている時間は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:50 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:50 
msgid "あ―、そう言えば犬とおじいさんを\n見たような気がするな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:51 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:51 
msgid "えっ、本当？\nおじいちゃんたちはどこに行ったの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:52 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:52 
msgid "どこに行ったんだったかな。\nう―ん、思い出せないなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:53 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:53 
msgid "頑張って思い出してください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:54 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:54 
msgid "そうは言っても仕事で忙しくて……\nバイトも探さないといけないしなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:55 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:55 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:56 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:56 
msgid "わ、わかったぞ。おじさん、\nそう言って僕を佶そうとしているな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:57 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:57 
msgid "バイトはしないからね！\nおじいちゃんたちは自分の力で捜してやる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:58 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:58 
msgid "ん―？　そっちには\n行ってなかった気がするな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:59 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:59 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:60 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:60 
msgid "……じゃあ、どっちに行ったの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:61 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:61 
msgid "さあ、どっちだったかな―。\n忙しくて忙しくて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:62 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:62 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:63 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:63 
msgid "バイトするから、教えてください……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0081:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0081:64 
msgid "…………。\n"
msgstr ""
