#  speaker 
#  warnings: 0 
#: sc/0048:0 
msgid "はかせ"
msgstr "Professor"

#  text 
#  warnings: 0 
#: sc/0048:0 
msgid "ん――……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:1 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:1 
msgid "ん――……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:2 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0048:2 
msgid "おいガキ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:3 
msgid "ん―？　どうしたの阪本？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:4 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:4 
msgid "それはこっちのセリフだ。\nさっきから何を个ってやがる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:5 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:5 
msgid "それはね―、なののことだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:6 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:6 
msgid "娘の？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:7 
msgid "そう。今度はなのに\nどんな改造してあげようかな―って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:8 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:8 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0048:9 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0048:9 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:10 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:10 
msgid "あっ！？　犬だ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:11 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:11 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0048:12 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:12 
msgid "あわわわわ、かまれちゃう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:13 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:14 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:14 
msgid "なにか頭に見えるな……アンテナ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:15 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:15 
msgid "アンテナ―――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0048:16 
msgid "うわああああ！？\nアンテナ引っこ抜かれた――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0048:17 
msgid "怖がっていたのに、\nなんでそんなことをするんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0048:18 
msgid "……怖すぎたんじゃね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:19 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:19 
msgid "改造って、\nまた娘に変な機能つけるつもりか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:20 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:20 
msgid "変じゃないよ？\nカッコイイし可愛いよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:21 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:21 
msgid "お前は娘をカッコよくしたいのか\n可愛くしたいのかどっちだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:22 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:22 
msgid "どっちも！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:23 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:23 
msgid "……だったらまず\nあのダサいネジを取れ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:24 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:24 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:25 
msgid "ダサくないもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:26 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:26 
msgid "ダサい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:27 
msgid "ダサくないもん！\n可愛いもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:28 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:28 
msgid "ダサい。\n可愛くない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:29 
msgid "それは阪本がダサいからだもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:30 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:30 
msgid "えええ―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:31 
msgid "そうだよ、ダサい阪本には\nネジの良さはわからないもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:32 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:32 
msgid "なら良さのわかるやつは\nお前以外にどこにいる？　いないだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:33 
msgid "いるもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:34 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:34 
msgid "いない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:35 
msgid "いるもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:36 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:36 
msgid "いない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:37 
msgid "うう〜〜〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:38 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:38 
msgid "じゃあ捜してくるもん！\nみんなネジは可愛いって言うもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:39 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:39 
msgid "フン。いるわけないだろそんなやつ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:40 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:40 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:41 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:41 
msgid "……あのガキ、一人で外に出て大丈夫だよな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:42 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:42 
msgid "ダサくないもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:43 
msgid "弌弌あ、レトロだからダサいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:44 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:44 
msgid "そういうダサさじゃなくてだな。\nネジ自体がダメだと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:45 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:45 
msgid "じゃあどんなネジがいいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:46 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:46 
msgid "まずはネジをつけることをやめないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:47 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:47 
msgid "じゃあ羽とかがいいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:48 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:48 
msgid "そうじゃなくてだな、\n背中に何かをつけることをやめるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:49 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:49 
msgid "じゃあどこに何をつければいいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:50 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:50 
msgid "だからな、余計なものを\nつける必要は無いと言ってるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:51 
msgid "ダメ！\nそれじゃ何も可愛くないもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:52 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:52 
msgid "だからってネジが\n可愛いなんて言ってるのはガキくらいだぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:53 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:53 
msgid "そんなことないよ？\nみんな可愛いって言うよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:54 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:54 
msgid "じゃあ誰かに聞いてみろ。\nそしたら自分の間違いに気付くだろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:55 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:55 
msgid "わかった！　じゃあ可愛いって言ったら\nネジつけるからね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:56 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:56 
msgid "好きにしろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:57 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:57 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:58 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:58 
msgid "……どっちになっても俺に害はない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:59 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:59 
msgid "ぷぷぷ。\n可愛くない、だって。ぷぷぷ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:60 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:60 
msgid "なんだ？　何がおかしい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:61 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:61 
msgid "だって猫にそんなことわかるわけないもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:62 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:62 
msgid "猫でもわかることを\n言ってるつもりなんだがな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:63 
msgid "じゃあはかせが他の人にもネジが\n可愛いかどうか聞いてきてあげる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:64 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:64 
msgid "それならいいでしょ？\nきっとはかせが正しいってわかるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:65 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:65 
msgid "望むところだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:66 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:66 
msgid "じゃあ今から聞きに行ってくるね。\n負けたらご飯抜きね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:67 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:67 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0048:68 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0048:68 
msgid "……えっ？\n"
msgstr ""
