#: sc/0204:0 
msgid "玉村"
msgstr "Tamamura"

#  text 
#  warnings: 0 
#: sc/0204:0 
msgid "いらっしゃいませ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:1 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0204:1 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0204:2 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:2 
msgid "（あっ……この人は……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:3 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:3 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:4 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:4 
msgid "（きょ、今日は何だろう……\n　また大福が一番だって言いに来たの……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:5 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:5 
msgid "あの……ご注文はお決まりでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:6 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:6 
msgid "……キミ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:7 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:7 
msgid "は、はいぃ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:8 
msgid "頼む！\n何も言わずに私についてきてほしい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:9 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:9 
msgid "えっ、えええ―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:10 
msgid "気付いたのだ！　大福フェアには\nキミのような人材が必要だと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:11 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:11 
msgid "……へっ？　人材？　大福フェア？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:12 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:12 
msgid "そうだ！　是非キミに\n大福くんの着ぐるみを被ってほしい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:13 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:13 
msgid "は、はぁ……ですが、\n私は今、ここでバイトをしていますし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:14 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:14 
msgid "構わん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:15 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:15 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0204:16 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:16 
msgid "（いや……ダメでしょ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:17 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:17 
msgid "頼む！　一緒に来てくれ！\nお客を呼ぶためには、どうしても必要なんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:18 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:18 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:19 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:19 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:20 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:20 
msgid "……ご注文は以上でしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:21 
msgid "あ？\nああ、今、頼んだ通りだが……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0204:22 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0204:22 
msgid "それでは、少お待ち下さいませ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:23 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:23 
msgid "お待たせいたしました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:24 
msgid "おお、来てくれる弌弌！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:25 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:25 
msgid "い、いや、\n品物は注文してないのだが弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:26 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:26 
msgid "ご注文の品は、\nこちらでよろしいでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:27 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:27 
msgid "弌弌塩っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:28 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:28 
msgid "ありがとうございました。\nそれでは次にお待ちのお客様、どうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:29 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:29 
msgid "（ど、どういう意味だ……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:30 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:30 
msgid "（はっ！　そうか私が邪魔な客……\n　塩を撒いて片付けたい客と言う意味か！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:31 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:31 
msgid "（い、いやまて、そんなはずはない！\n　塩を撒きたい相手に直接渡してどうする！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:32 
msgid "通行人男性"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:32 
msgid "ねえ店員さん。このゴミって\nどうすればいいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:33 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:33 
msgid "あ、はい、返却口はあちらで。後片付けは\nセルフサ―ビスでお願いしております。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:34 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:34 
msgid "……後片付けはセルフサ―ビス？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:35 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0204:35 
msgid "……ねえ、父さん。\n塩なんかかけられて、一体どうしたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:36 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:36 
msgid "かけられてなどいない！\nセルフサ―ビスだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0204:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0204:37 
msgid "？？？？\n"
msgstr ""
