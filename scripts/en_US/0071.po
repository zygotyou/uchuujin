#  speaker 
#  warnings: 0 
#: sc/0071:0 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:0 
msgid "よ―し、練習開始だ！\nまずはリフティングから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:1 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0071:1 
msgid "はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:2 
msgid "関口"
msgstr "Sekiguchi"

#  text 
#  warnings: 0 
#: sc/0071:2 
msgid "（部長の方が初心者なのに。\n　なんか不思議な感じ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:3 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:3 
msgid "おっと！\n悪い関口、ボ―ル取ってくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:4 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:4 
msgid "はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:5 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:5 
msgid "サンキュ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:6 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:6 
msgid "（ううん初心者なのによりも、むしろ部長が\n　真面目に練習しているのが不思議かも……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:7 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:7 
msgid "なあ桜井くん、ちょっといいか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:8 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:8 
msgid "はい、何ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:9 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:9 
msgid "碁石を出すタイミングがつかめなくてさ。\n何かコツとかあるかい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:10 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:10 
msgid "あ、それはですね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:11 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:11 
msgid "こうです。こうやって高さを保ちながら\n胸ポケットから碁石を出して……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:12 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:12 
msgid "こうして……こうか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:13 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:13 
msgid "そうです、そうしてそこから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:14 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0071:14 
msgid "一人ジャ―マン！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:15 
msgid "大工＆関口＆桜井誠"
msgstr "Daiku, Yuria, & Makoto"

#  text 
#  warnings: 0 
#: sc/0071:15 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0071:16 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:16 
msgid "………………。\n"
msgstr ".........\n"

#  text 
#  warnings: 1 
#: sc/0071:17 
msgid "ああ――っ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0071:18 
msgid "そりゃそうだよ！\nあんな勢いで頭からじゃダメに決まってるよ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0071:19 
msgid "すまん、ついオレが囲碁サッカ―に\nハマってしまったばっかりに……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0071:20 
msgid "最近、オレの碁石が減っていたのは、\nお前のせいか―っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:21 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:21 
msgid "（こんな風に真面目な顔してると、\n　格好いいんだけど……）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0071:22 
msgid "関口"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0071:22 
msgid "（いつも無茶苦茶なことばかりするから。\n　今日だって朝から色大変な目にあったし）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:23 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:23 
msgid "（……そういえば、\n　今日は授業が無い時はずっと一緒に居る）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:24 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:24 
msgid "（弌弌触れなかったり、\n　お弁当褒められて嬉しかったりしたっけ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:25 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:25 
msgid "（うん。やっぱり私、部長のこと弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:26 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:26 
msgid "お―い関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:27 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:27 
msgid "っ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:28 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:28 
msgid "大丈夫か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:29 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:29 
msgid "ど、どうしました？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:30 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:30 
msgid "いや、ボ―ルがそっち行ったから\n取ってくれって言おうと思ったんだけどさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:31 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:31 
msgid "あ……いつの間に。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:32 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:32 
msgid "結構前からだけど、大丈夫か？\n何か顔も赤いみたいだし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:33 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:33 
msgid "なんでもないです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:34 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:34 
msgid "ん―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:35 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:35 
msgid "！！！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:36 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:36 
msgid "（ぶ、部長の顔が……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:37 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:37 
msgid "うおっ！？　凄い熱だぞ、関口！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:38 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:38 
msgid "え……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:39 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:39 
msgid "何でこんなになるまでほっといたんだ！\n桜井くん！　保健の先生呼んできてくれ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:40 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:40 
msgid "はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:41 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:41 
msgid "あ、あの……私なんとも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:42 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:42 
msgid "ないわけないだろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:43 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:43 
msgid "弌弌っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:44 
msgid "うおぉっ、また熱が！\n保健の先生じゃない、救急車だ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:45 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:45 
msgid "（えぇっ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:46 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:46 
msgid "部長、だから本当になんとも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:47 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:47 
msgid "いいか？　無理しなくていいんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:48 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:48 
msgid "ほら、そこで大人しくしとけ！\n救急車呼ぶついでに冷やすもんもってくる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:49 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:49 
msgid "……どうしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:50 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:50 
msgid "結構熱あるような……ないような？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:51 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:51 
msgid "ど、どっちですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:52 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:52 
msgid "（顔、近い……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:53 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:53 
msgid "いまいちわかんないんだよなぁ。\n何か熱上がってきてるような気もするし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:54 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:54 
msgid "一応保健室行くか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:55 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:55 
msgid "多分大丈夫です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:56 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:56 
msgid "本当に？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:57 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:57 
msgid "大丈夫です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:58 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:58 
msgid "いや、一応熱計ったほうがいいな。\n保健室から借りてきてやるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:59 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:59 
msgid "……部長があんなに近くに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:60 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:60 
msgid "（ドキドキした……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:61 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:61 
msgid "体温計取ってきてくれるらしいから、\nこのまま桜井くんと待っておかないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:62 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:62 
msgid "……桜井くん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:63 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:63 
msgid "……ッ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:64 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:64 
msgid "え―っと、なんていうか……その。\nごめんね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:65 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:65 
msgid "や……っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:66 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:66 
msgid "うわっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:67 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:67 
msgid "あ……っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:68 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:68 
msgid "部長っ、大丈夫ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:69 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:69 
msgid "あいたたた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:70 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:70 
msgid "すみません……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:71 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:71 
msgid "あ―、びっくりした。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:72 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:72 
msgid "悪かったな、\n急にやったからびっくりしたんだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:73 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:73 
msgid "その通りです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:74 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:74 
msgid "いや―、うっかりしてたよ。\nそういや関口も女の子だもんな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:75 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:75 
msgid "あ……。\n"
msgstr "Ah...\n"

#  speaker 
#  warnings: 0 
#: sc/0071:76 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:76 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0071:77 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:77 
msgid "あ、いやっ！\n今のはうっかり口が滑ったっていうか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:78 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:78 
msgid "ぐへぇ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0071:79 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0071:79 
msgid "今のは部長が悪いです。\n"
msgstr ""
