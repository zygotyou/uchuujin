#  speaker 
#  warnings: 0 
#: sc/0121:0 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:0 
msgid "も―、私ったら理科実験室に忘れ物なんて\nうっかりしすぎっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:1 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:1 
msgid "早くしないと次の授業が……\n数学の赤城先生、厳しいから怒られちゃうよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:2 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:2 
msgid "あっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:3 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0121:3 
msgid "うわっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:4 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:4 
msgid "きゃっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:5 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:5 
msgid "だ、大丈夫、安中さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:6 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:6 
msgid "あ、うん！　ありがとう中之条くん。\n急いでいたら、思わず走っちゃって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:7 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:7 
msgid "気持ちはわかるけど、走って変に転んだら、\n大ケガして大変だと思うんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:8 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:8 
msgid "うん、ありがとう。\nぶつかっちゃってごめんね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:9 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:9 
msgid "それじゃ、私急いでるからっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:10 
msgid "あ、安中さん！\nそんなに走ったらまた弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:11 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0121:11 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0121:12 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:12 
msgid "きゃあっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:13 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:13 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0121:14 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:14 
msgid "弌弌ぶつかるよって……\n大丈夫、安中さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:15 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:15 
msgid "えへへ……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0121:16 
msgid "いや―、この子はほほえましいなぁ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0121:17 
msgid "すっごく和むよな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0121:18 
msgid "ああ、今のでアンテナが折れたことなんて\n大したことじゃないよな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0121:19 
msgid "そうさ！　はははははっ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0121:20 
msgid "ははははははっ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0121:21 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0121:21 
msgid "大したことだ―――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:22 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:22 
msgid "でも、今ので\n中之条くんまで倒れなくて良かった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:23 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:23 
msgid "いつも第一発見者なのに、今度は\n犯人になっちゃうとこだったよ―。なんて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:24 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:24 
msgid "……あ、そういえば、\n何度も高崎先生を呼んできてくれたんだよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:25 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:25 
msgid "えっと……\nありがとう、安中さん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:26 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:26 
msgid "どういたしまして。\nでも、何でいつもあんなことになってるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:27 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:27 
msgid "う―ん、それは多分弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:28 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:28 
msgid "ぐげっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:29 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:29 
msgid "！！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:30 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:30 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:31 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:31 
msgid "な、中之条くんっ、しっかりして！！\n目を覚まして！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:32 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:32 
msgid "ど、どうしよう！\n高崎先生を呼びに行く時間もないのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:33 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:33 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:34 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:34 
msgid "……こ、こうなったら。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:35 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:35 
msgid "えいっ！\n"
msgstr "Hah!\n"

#  speaker 
#  warnings: 0 
#: sc/0121:36 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:36 
msgid "ぐほぁっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:37 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:37 
msgid "よし、これで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:38 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:38 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:39 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:39 
msgid "あ、あれ、起きない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:40 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:40 
msgid "も、もう一回！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:41 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:41 
msgid "げふっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:42 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:42 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:43 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:43 
msgid "弌弌起きない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:44 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:45 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:45 
msgid "高崎せんせ―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:46 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:46 
msgid "な、中之条くんしっかり！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:47 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:47 
msgid "う―ん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:48 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:48 
msgid "大丈夫？　ねえ中之条くん！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:49 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:49 
msgid "う……いたたた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:50 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:50 
msgid "あっ、良かった！\n大丈夫っぽいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:51 
msgid "今、何が……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:52 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:52 
msgid "手が飛んできて頭に当たったんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:53 
msgid "手……？　手が飛んできた？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:54 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:54 
msgid "そうだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:55 
msgid "ぐへっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:56 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:56 
msgid "ほら、ちょうどこんな風に。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:57 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:57 
msgid "って、ええ――――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:58 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:58 
msgid "中之条くん、しっかり！\nしっかりして！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:59 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:60 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:60 
msgid "……ダメだこりゃ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:61 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:61 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:62 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:62 
msgid "気絶してる……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:63 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:63 
msgid "……どうしよう、早く忘れ物取りに行かないと\n授業はじまっちゃうのに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:64 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:64 
msgid "こんな時、高崎先生だったら……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:65 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:65 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:66 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:66 
msgid "思い出した！\n確かあの時、高崎先生は言ってたよ……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:67 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:67 
msgid "中之条くんっ！\nこんな所で寝ちゃだめだよっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:68 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:68 
msgid "（そう、中之条くんは気絶してない！\n　寝てるだけかもしれない！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:69 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:69 
msgid "（だ、だって白目剥いてないし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:70 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:70 
msgid "寝……じゃ……ない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:71 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:71 
msgid "眠……む……い？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:72 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:72 
msgid "ええっ！　本当に眠いの？　それなら\nしょうがないけど、せめて端っこで寝ようよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:73 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:73 
msgid "んしょ……これでよし。\nゆっくり眠ってね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:74 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:74 
msgid "さてと、忘れ物取りにいかないと。\n急がないと遅刻しちゃうよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:75 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0121:76 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0121:76 
msgid "……た、高崎先生は……\n何をおしえたん……だ……。\n"
msgstr ""
