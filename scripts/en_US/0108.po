#  speaker 
#  warnings: 1 
#: sc/0108:0 
msgid "はかせ"
msgstr "Professor"

#  text 
#  warnings: 1 
#: sc/0108:0 
msgid "もぐもぐ……ん―、うまうま\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:1 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:1 
msgid "あっ、もうポテチなくなっちゃった。\n他のおかしは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:2 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:2 
msgid "あれ？　ない？　あれあれ？\n戸棚にも冷蔵庫にもない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:3 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0108:4 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:4 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0108:5 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0108:5 
msgid "はかせ、またおかし全部食べたんですか！？\n晩ご飯抜きです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:6 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:6 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:7 
msgid "ど、どうしよう！？\nなのに怒られちゃう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:8 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:8 
msgid "新しいの買ってきて……\nでもはかせお金持ってないし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:9 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:9 
msgid "う―ん、う―ん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:10 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0108:10 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:11 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:11 
msgid "わっ！　犬だ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:12 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:12 
msgid "……あれ、近づいてこないの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:13 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:13 
msgid "あ、そうだ。犬が食べたことにすれば\nはかせは怒られない！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:14 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:14 
msgid "やった！　隠してたおかしも食べよ―！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0108:15 
msgid "ほほう、なかなか考えたな、この子。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0108:16 
msgid "だが大人ってのはそんな子供の嘘を\n余裕で見抜けるもんだよな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0108:17 
msgid "よし。この子が怒られるのもかわいそうだし、\n少し暴れさせて協力してやるか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:18 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:18 
msgid "うわ―ん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:19 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:19 
msgid "こら―！　いたずらするワンちゃんは\n出て行きなさ―い！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0108:20 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0108:20 
msgid "やりすぎた―――！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:21 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:21 
msgid "あっ！　そうだ！\nお菓子そっくりのものを作ればいいんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:22 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:22 
msgid "なのはあんまりおかし食べないから\nこれで少しは誤魔化せるはず！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:23 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:23 
msgid "早速おかしそっくりのものを作ろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:24 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:24 
msgid "あっ、でもおかしにそっくりなだけじゃ\n面白くないよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:25 
msgid "何かなのがびっくりするような\n面白いものを作ろ―っと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:26 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:26 
msgid "どんな物にしようかなぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:27 
msgid "チョコ型のボタンを作って\nなのが食べたら、腕が飛ぶ装置とか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:28 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:28 
msgid "リモコンで動くおかしを作って\nなのが食べようとしたら逃げたりとか！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0108:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0108:29 
msgid "いいぞいいぞ。\nもっと面白いものを考えようっ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:30 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:30 
msgid "あははははははは！　あはははは！\n面白いアイデアいっぱい出た！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:31 
msgid "さ―て、なのが帰ってくる前に\n作ろうかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:32 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:32 
msgid "ただいま―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:33 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:34 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:34 
msgid "びっくり箱みたいにすれば、なのが開けた瞬間、\n驚いて腰を抜かすぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:35 
msgid "そしたらなのは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:36 
msgid "わっ！？　ビックリした！\nもう！　驚かせないでください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:37 
msgid "ぷぷぷ。怒るなのの姿が\n想像できちゃう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:38 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:38 
msgid "あ、あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:39 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:39 
msgid "え、えっと、びっくり箱は無し。\n怒らせちゃダメなんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:40 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:40 
msgid "そうだ、食べられないけど感触が楽しめる\nおかしを用意したら弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:41 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:41 
msgid "わっ、このおかし面白い。\nはかせの発明ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:42 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:42 
msgid "えっへん。すごいでしょ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:43 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:43 
msgid "すごいです。\nそれで、本物はどこにあるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:44 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:44 
msgid "えっ？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0108:45 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:45 
msgid "あっ！　またこんなにおかし食べて！\nはかせの晩ご飯は抜きです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:46 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:46 
msgid "あれ――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:47 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:47 
msgid "……でも、なのは面白い機能を見ても\n誤魔化されないかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:48 
msgid "目的は誤魔化すことだし、\n本物そっくりに作るだけにしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:49 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:49 
msgid "ふぅ。やっとできた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:50 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:50 
msgid "ただいま―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:51 
msgid "あっ、帰ってきた。\n早速この偽物を設置して……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:52 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:52 
msgid "あっ、はかせ。ただい弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:53 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:53 
msgid "あっ！　はかせ、またゴミ散らかして！\nしかもおかし全部食べてるじゃないですか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0108:54 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0108:54 
msgid "ばれた！？\n"
msgstr ""
