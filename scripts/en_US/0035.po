#  speaker 
#  warnings: 0 
#: sc/0035:0 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0035:0 
msgid "それじゃ、オレはこっちなんで。\nお疲れ様でした！　また明日！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:1 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:1 
msgid "ああ、お疲れ様。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:2 
msgid "関口"
msgstr "Sekiguchi"

#  text 
#  warnings: 0 
#: sc/0035:2 
msgid "お疲れ様です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:3 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:3 
msgid "さ―て、いつも通りここからは二人だな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:4 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:4 
msgid "……そうですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:5 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:5 
msgid "……あ、そうだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:6 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:6 
msgid "関口、大工カフェ寄ってかないか？\nオレのど渇いちゃってさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:7 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:7 
msgid "いいです。\n私は別にのど渇いてませんし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:8 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:8 
msgid "ははは、そう言うなよ。\n乂るから寄ってけって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:9 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:9 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0035:10 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:10 
msgid "よ―し決まり。じゃあ行くか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:11 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0035:11 
msgid "おう。ケ―キもつけてくれよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:12 
msgid "大工＆関口"
msgstr "Kenzaburou & Yuria"

#  text 
#  warnings: 0 
#: sc/0035:12 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0035:13 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:13 
msgid "あっ、やば……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:14 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:14 
msgid "おおお！\n関口！　この猫喋るぞ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0035:15 
msgid "うわっ！　つかまっちまった！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0035:16 
msgid "あああ！\nそんなっ、乱暴に扱わないで！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0035:17 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0035:17 
msgid "うわああああ――――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:18 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:18 
msgid "いや―、それにしても今日は楽しかったな。\n明日からも頑張らないとな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:19 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:19 
msgid "部長。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:20 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:20 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:21 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:21 
msgid "囲碁サッカ―、\n本気でやる気なんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:22 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:22 
msgid "ああ、もちろんだ。\nこんなに面白いもんほっとけないだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:23 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:23 
msgid "……試合も頑張ってくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:24 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:24 
msgid "え？　オレは試合に出ないよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:25 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:25 
msgid "……え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:26 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:26 
msgid "バカだな関口、今日はじめた初心者なのに\n試合に出てどうするんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:27 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:27 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0035:28 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:28 
msgid "あの。\nだったら何のために練習してたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:29 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:29 
msgid "おいおい関口、何言ってんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:30 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:30 
msgid "こんだけお膳立てしといて\nオレが練習してなかったらおかしいだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:31 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:31 
msgid "それに、次の試合に出られないだけで\nその次の試合くらいまでには形になってるさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:32 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:32 
msgid "むしろそのくらいまでには\n部長としてやれるようになっとかないとな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:33 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:33 
msgid "部長……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:34 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:34 
msgid "そんなに本気だったんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:35 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:35 
msgid "ああ、ボ―ドゲ―ムやってるぐらい\n楽しかったしな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:36 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:36 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0035:37 
msgid "大工"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0035:37 
msgid "元お遊びで作った部活なんだし、\n楽しく遊べるならいいだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:38 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:38 
msgid "……そうですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:39 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:39 
msgid "よ―し！　明日もがんばるぞ！\n関口も頼むな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:40 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:40 
msgid "あ……はい。\n私もがんばります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:41 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:41 
msgid "それじゃ、また明日な！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:42 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:42 
msgid "あ、部長。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:43 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:43 
msgid "コ―ヒ―、まだ来てないのに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:44 
msgid "そりゃ付き合いだからに決まってるだろ？\nぶっちゃけ練習とかめんどくさいしさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:45 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:45 
msgid "でも、人集めといて何もしないのは\n上に立つものとしてどうかと思うしな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:46 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:46 
msgid "なるほど。\nその、部長ってすごいですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:47 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:47 
msgid "ん―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:48 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:48 
msgid "その……失礼ですけど、\nもっと不真面目な人だと思ってました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:49 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:49 
msgid "はははっ、関口は正直だなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:50 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:50 
msgid "ス、スミマセン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:51 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:51 
msgid "いいっていいって。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0035:52 
msgid "大工"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0035:52 
msgid "ま、それに、ちゃんと練習しておいた方が\n後便利ってのもあるんだけどな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:53 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:53 
msgid "……便利？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:54 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:54 
msgid "本当に行われていた大会だと思わなかったから、\nあの時は簡単にコレでどうにかしたけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:55 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:55 
msgid "一生懸命やってる桜井くんも居るし、\nいまは下手なコレもできないだろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:56 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:56 
msgid "だから、前の県大会みたいな手は\n使えないんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:57 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:57 
msgid "なるほど……練習試合じゃなくて、\nその後の大会のことを考えてるんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:58 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:58 
msgid "その通り。それに県大会に出たおかげで\n次からシ―ドなんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:59 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:59 
msgid "え……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:60 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:60 
msgid "うち、シ―ドらしい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:61 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:61 
msgid "だからこれも前と違って、普段から\n本気でやらないとバレる可能性があるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:62 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:63 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:63 
msgid "関口、もし何か聞かれても\n知らないって言ってくれよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:64 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:64 
msgid "……は、はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:65 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:65 
msgid "そりゃ決まってるだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:66 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:66 
msgid "みんなのためですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:67 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:67 
msgid "いいや、内申書のためだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:68 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:68 
msgid "初心者なのに一から部活を立ち上げた\n練習熱心な部長。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:69 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:69 
msgid "しかも顧問は元高校ＬＵＯ、\n後輩は中学ＬＵＯときたもんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:70 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:70 
msgid "これはおいしいだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:71 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:71 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:72 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:72 
msgid "県大会にも出たし、\nあとは全国で勝ったりした日には凄いぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:73 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:73 
msgid "もしかしたら推薦とかイケるかもな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:74 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:74 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:75 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:75 
msgid "関口も副部長とかやらないか？\nかなりおいしいポストだと思うんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:76 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:76 
msgid "……あの、部長。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:77 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:77 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:78 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:78 
msgid "似合ってないですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:79 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:79 
msgid "……なんだよ、バレバレかよ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:80 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:80 
msgid "だって内申書のためだったら\n普通の部に入ればいいですし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:81 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:81 
msgid "あ―、それもそっか。……よし！\n弌弌じゃあ今から野球部に入部してくるわ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:82 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:82 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:83 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:83 
msgid "な―んてな。\n今度こそひっかかっただろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:84 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:84 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:85 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:85 
msgid "（みんなのためなのを認めるのが\n　恥ずかしいんだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0035:86 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0035:86 
msgid "（……気付いてないフリしよう）\n"
msgstr ""
