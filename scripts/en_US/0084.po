#  speaker 
#  warnings: 0 
#: sc/0084:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0084:0 
msgid "あと少しで体育祭だね―。\nなんかテンション上がってくるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:1 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0084:1 
msgid "そうですね。\n私は体育祭は初めてなので楽しみです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:2 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0084:2 
msgid "じゃあ少し練習した方がいいかもね。\nほら、リレ―のバトン渡しとか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:3 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:3 
msgid "お、そうだね。\nなのちゃんしたことないでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:4 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:4 
msgid "はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:5 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:5 
msgid "じゃあ少しだけ練習する？\nバトンは携帯でいいよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:6 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:6 
msgid "はい。お願いします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:7 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:7 
msgid "じゃあ行くよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:8 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:8 
msgid "はい、お願いします！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:9 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:9 
msgid "よっ、と……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:10 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:10 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:11 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:11 
msgid "なのちゃん、みおちゃんが近くに来たら\n走りながらバトンを受け取るんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:12 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:12 
msgid "は、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:13 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:13 
msgid "なのちゃん、バトン渡すよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:14 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:14 
msgid "はいっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:15 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:15 
msgid "おっ、ナイスキャッチ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:16 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:16 
msgid "そのまま走って、\n今度はゆっこにバトンを渡して！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:17 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:17 
msgid "はい！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:18 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:18 
msgid "わっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:19 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:19 
msgid "いたたた……\nどうしてこんなところにワンちゃんが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:20 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:20 
msgid "あっ！！　ご、ごめんなさい！\nすぐに退くから！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:21 
msgid "ふぅ、間一髪だぜ。\nあいつが助かってよかった。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:22 
msgid "とっさにバリアを展開したのか。\nお前の犬好きには恐れ入るよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:23 
msgid "へへっ。そういうお前も、\n反重力場を発生させてたじゃないか。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:24 
msgid "ま、俺も犬は嫌いじゃないしな。\n辛い目には遭わせたくない。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:25 
msgid "……その結果、\n俺たちが辛い目に遭いそうだな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:26 
msgid "所詮は試作機能だったからな。\n負荷がかかりすぎちまったみたいだ。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0084:27 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0084:27 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0084:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:28 
msgid "さあ急いでなのちゃん！\nちんたらしてるとビリになるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:29 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:29 
msgid "はい！！\n急いでバトンを渡します！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:30 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:30 
msgid "（これは練習だけど、練習だと思って\n　手を抜いたら駄目）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:31 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:31 
msgid "（これは本番だと思って全力で走る！\n　全力で相生さんにバトンを渡す！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:32 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:32 
msgid "うおおおおお！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:33 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:33 
msgid "おおおおおおお！？？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:34 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:34 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0084:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:35 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:36 
msgid "（と、飛んでった―！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:37 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:37 
msgid "相生さん！　避けてください！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:38 
msgid "はああああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:39 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:39 
msgid "受け止めた――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:40 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:41 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:41 
msgid "！！！\n"
msgstr "!!!\n"

#  speaker 
#  warnings: 0 
#: sc/0084:42 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:42 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:43 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:43 
msgid "（あっ！？　手！！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:44 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:44 
msgid "（早くあの手をはめ直さないと\n　片手が無い姿を多くの人に見られる！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:45 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:45 
msgid "相生さん！　止まってください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:46 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:46 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0084:47 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:47 
msgid "（ダメだ、止まらない……追いかけるにしても\n　こんなに距離を離されたら……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:48 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:48 
msgid "（いや！？　諦めるな！！\n　まだ手は残っている！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:49 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:49 
msgid "（文字通り、片手だけは残っている！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:50 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:50 
msgid "いっけぇえええええ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:51 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:51 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:52 
msgid "はああああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:53 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:53 
msgid "また受け止められた！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:54 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:55 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:56 
msgid "！！！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0084:57 
msgid "なの"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0084:57 
msgid "！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:58 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:58 
msgid "（手を相生さんに持っていかれた！\n　……いや、それよりも問題は……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:59 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:60 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:60 
msgid "（手が飛ぶところを\n　長野原さんに見られた……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:61 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:61 
msgid "（これで私がロボだということが広まったら\n　もう学校に通うことはできない……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:62 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:63 
msgid "長野原さん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:64 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:64 
msgid "えっ！？　な、なに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:65 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:65 
msgid "……見ましたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:67 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:68 
msgid "……な、何のこと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:69 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:69 
msgid "それはもちろん弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:70 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:70 
msgid "ああ、なのちゃんのバトンを受け取る\nタイミングは見たよ。１００点満点。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:71 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:71 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:72 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:72 
msgid "（良かった！\n　手が飛ぶ所を見られてない！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:73 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:73 
msgid "あ、ありがとうございます！\nじゃあもう一度練習しましょう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:74 
msgid "あ、うん、ゆっこ呼んでこないとね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:75 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:75 
msgid "私行ってきます！\nちょっと待っててください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:76 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:76 
msgid "う、うん、お願い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:77 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:77 
msgid "（私はまた学校に通えるんだ……\n　本当に嘘みたい……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:78 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:78 
msgid "……飛ぶんだ、あれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:79 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:79 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:80 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:80 
msgid "な、なのちゃん……今、手が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:81 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:81 
msgid "え？　手が、ど、どうしたんですか？\n痛いんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:82 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:82 
msgid "えっ？\nいや、なのちゃんの手が弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:83 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:83 
msgid "相生さん、どこまで行ったんでしょうね。\n全然帰ってきませんよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:84 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:84 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:85 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:85 
msgid "はっ、はっ、はっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:86 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:86 
msgid "あっ、お帰りなさい相生さん。\nどこまで行ってたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:87 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:87 
msgid "あ、ちょっとそこまで……\nところでなのちゃん……この手弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:88 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:88 
msgid "あっ、ちょっとすみません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:89 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:89 
msgid "！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:90 
msgid "ゆっこ＆みお"
msgstr "Yuuko & Mio"

#  text 
#  warnings: 0 
#: sc/0084:90 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:91 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:91 
msgid "弌弌えっと、なんでしょうか相生さん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:92 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:92 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:93 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:93 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:94 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:94 
msgid "体育祭、頑張ろうね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0084:95 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0084:95 
msgid "はいっ。\n"
msgstr "Okay.\n"
