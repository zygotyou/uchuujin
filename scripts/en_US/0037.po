#  speaker 
#  warnings: 0 
#: sc/0037:0 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0037:0 
msgid "結局、学校に来てしまった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:1 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:1 
msgid "（髪型は元のモヒカンに戻せたから良いが、\n　相変わらず幽霊になったままだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:2 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:2 
msgid "（こんなことになってるなんて、\n　誰にも気付かれるわけにはいかない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:3 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:3 
msgid "（なあに、今日は運良く半日だ。\n　午前の授業を耐えれば、すぐに終わり）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:4 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:4 
msgid "（それまでの間、誰にもモヒカンのことを\n　気付かれなければ良いだけさ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:5 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0037:5 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0037:6 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:6 
msgid "ん？　あれ、犬が入り込んで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:7 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:7 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:8 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:8 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0037:9 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:9 
msgid "（こ、この犬……モヒカンを見ている！？\n　もしかしたら動物にはわかるのかも……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:10 
msgid "（！？　まずい、このまま見つめられていたら、\n　他のみんなにも気付かれてしまう……！！）\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0037:11 
msgid "うおっ！？　いきなりなんだ！？\nこっちに向かってきたぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0037:12 
msgid "に、逃げろ！\nこのままでは犬が捕まってしまう！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0037:13 
msgid "ああっ、ダメだ！\n逃げても追いかけてくるぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0037:14 
msgid "あああっ、捕まった！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0037:15 
msgid "アンテナも壊れた――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:16 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0037:16 
msgid "あ、あの、中之条くん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:17 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:17 
msgid "……あっ、桜井先生。\nおはようございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:18 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:18 
msgid "あっ、はい、おはようございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:19 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:19 
msgid "どうかしましたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:20 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:20 
msgid "え、えっと……\nその髪型のことなんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:21 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:21 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:22 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:22 
msgid "前に、髪についての事情は\n聞かせてもらいましたけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:23 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:23 
msgid "で、でも、やっぱり今のままでは\n風紀的にアレなので……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:24 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:24 
msgid "その……髪の毛は立たせないように\nしてもらえると……その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:25 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:25 
msgid "（くっ……桜井先生の意見はもっともだ……\n　理由はどうあれ、僕は風紀を乱している）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:26 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:26 
msgid "（しかし！　しかし今は！\n　下ろしたくても触ることすらできない！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:27 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:27 
msgid "あ、そうです。\nどうせなら、かわいい髪型にしてみませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:28 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:28 
msgid "チュ―リップとかですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:29 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:29 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:30 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:30 
msgid "チュ―リップだけは勘弁してください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:31 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:31 
msgid "え、あ、スイマセン！\nそれなら、チュ―リップはなしでいいですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:32 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:32 
msgid "他の花も嫌です！\n助けてください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:33 
msgid "ハ、ハイ！\n花全般、なしでいいです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:34 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:34 
msgid "じゃあ、この髪型でもいいですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:35 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:35 
msgid "（そう、冷静に考えてみると、\n　別のどんな髪型でも今はダメなんだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:36 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:36 
msgid "（なんとかして……なんとかして、\n　この状況を回避しなければ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:37 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:37 
msgid "えっ？\nこの髪型でもって……そ、それは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:38 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:38 
msgid "いいですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:39 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:39 
msgid "ひ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:40 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:40 
msgid "きょ、許可します！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:41 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:41 
msgid "よかった！\n……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:42 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:42 
msgid "あれ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:43 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:43 
msgid "か、かわいい髪型……ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:44 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:44 
msgid "はいっ。\n"
msgstr "Okay.\n"

#  speaker 
#  warnings: 0 
#: sc/0037:45 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:45 
msgid "た、例えばどんな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:46 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:46 
msgid "そうですね……あ、あの、長さを知りたいので\nとりあえず、一度崩してもらえますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:47 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:47 
msgid "（くっ、まずい、今の僕の髪が\n　さわれないことを知られては……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:48 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0037:49 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:49 
msgid "……あ、あの、中之条くん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:50 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:50 
msgid "……じ、実は、僕、５円ハゲが……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:51 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:51 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:52 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:52 
msgid "６円でもいいんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:53 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:53 
msgid "……あがった！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:54 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:54 
msgid "だから、下ろしたくないんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:55 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:56 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:56 
msgid "……６円なら、しかたありませんね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:57 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:57 
msgid "ありがとうございます、先生っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:58 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0037:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:59 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:59 
msgid "……どうして？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:60 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:60 
msgid "かわいい髪型……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:61 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:61 
msgid "はいっ！　かわいい髪型なら、\nきっと風紀を乱すことはないと思います。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:62 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:62 
msgid "……でも、桜井先生。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:63 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:63 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:64 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:64 
msgid "男子がかわいい髪型にしても、\nあまり、かわいくないと思いますが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:65 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:66 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0037:67 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:67 
msgid "あ―……。\n"
msgstr "Uh......\n"

#  speaker 
#  warnings: 0 
#: sc/0037:68 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0037:68 
msgid "……ごめんなさい。\n"
msgstr ""
