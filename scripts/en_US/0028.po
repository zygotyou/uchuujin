#  speaker 
#  warnings: 0 
#: sc/0028:0 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0028:0 
msgid "くそ―……結局はかせから作戦を\n教えてもらえなかった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:1 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:1 
msgid "しかし、あんな小さな天才が\nこんな身近にいるなんて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:2 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:2 
msgid "ムカつく子供だったが腕は本物だ。\n将来は物凄い発明家になるだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:3 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:3 
msgid "恐らく東雲なのを作った人物も、あのように\n子供の頃から天才だったのだろうな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:4 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:4 
msgid "……さて、そろそろ学校に戻るか。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0028:5 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 1 
#: sc/0028:5 
msgid "はいはい寄ってらっしゃい見てらっしゃい。\nあんころ大福からわらび餅まで色あるよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:6 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:6 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:7 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:7 
msgid "そこのお姉さん。大福はどうだい？\nケ―キなんかよりよっぽど美味しいよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:8 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:8 
msgid "大福……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:9 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:9 
msgid "（ああ、いつもここでやってる胡散臭い……\n　そう言えば少し小腹がすいてきたな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:10 
msgid "今日は大福フェアだ。\n今なら緑茶もセットでついてくるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:11 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:11 
msgid "ん―……そうだな。\nなら大福を２つ。あと持ち帰りで４つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:12 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:12 
msgid "まいどあり！　はい、大福。\n緑茶も亞れたてだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:13 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:13 
msgid "どうも。\nでは早速……はむっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:14 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:14 
msgid "（ん〜〜……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:15 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:15 
msgid "（甘くてうまい……緑茶も合うし、\n　２個なんてぺろりと食べられるな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:16 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:16 
msgid "…………ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:17 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0028:17 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0028:18 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:18 
msgid "（校長も大福を買いにきたのか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:19 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:19 
msgid "よし……\n腹ごしらえもすんだし、学校に帰るか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:20 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:20 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:21 
msgid "あ―、ちょっとおじいさん。\n代金をまだもらってないよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:22 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:22 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:23 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:24 
msgid "あっ！？　食い逃げ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:25 
msgid "バカ野郎！\nどうして食い逃げなんかしてるんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:26 
msgid "すまん！　だが許してくれ！\n俺、大福が食べてみたかったんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:27 
msgid "でも電波越しじゃ食べられないから……せめて、\nこのオッサンに食べてもらいたかったんだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:28 
msgid "そうだったのか……なら弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:29 
msgid "って許せるか――っ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:30 
msgid "ぐふっ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:31 
msgid "まったくお前と言うやつは。\n食い逃げなんてさせたら弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:32 
msgid "ああ……。\nオッサンが町の人たちにつかまって……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:33 
msgid "やっぱりボコられた―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:34 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:34 
msgid "ずず―……\nふぅ。うまかった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:35 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:35 
msgid "さて、それじゃあ学校に戻るか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:36 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:36 
msgid "あ、ちょっとお姉さん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:37 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:37 
msgid "ん？　もう大福はいらないぞ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:38 
msgid "そうじゃなくて、お代。\n大福のお金、まだもらってないよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:39 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:39 
msgid "ああ、そう言えば。\nえっと、財布は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:40 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:40 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:41 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:41 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:42 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:42 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:43 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:43 
msgid "（財布が……無い！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:44 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:44 
msgid "……どうしたんだい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:45 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:45 
msgid "財布……忘れてきた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:46 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:46 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:47 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:47 
msgid "（うう……ポケットに偶然\n　お金が入ってたり……しないか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:48 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:48 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:49 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:49 
msgid "（くっ、どうする……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:50 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:50 
msgid "……ん？　大福の中之条？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:51 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:51 
msgid "ん？　うちの店が何か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:52 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:52 
msgid "中之条とは……あのモヒカンの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:53 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:53 
msgid "ああ、私の息子のことかね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:54 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:54 
msgid "（息子！　それならば！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:55 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:55 
msgid "店主、実は私は時定高校の\n教師をやっているんだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:56 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:56 
msgid "おや、先生でしたか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:57 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:57 
msgid "ああ。そこで、大福の代金だが、\nあいにく今は財布が無いので……。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0028:58 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0028:58 
msgid "息子の成績、\n最高の５にすることで手を打たないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:59 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:59 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0028:60 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:61 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:61 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:62 
msgid "せ、先生……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:63 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:63 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:64 
msgid "……大福、おまけしますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:65 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0028:66 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0028:66 
msgid "……む。\nモヒカンめ、理科の成績は元５だったか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:67 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:67 
msgid "しかし……\nこのままでは、店主に申し訳が立たないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:68 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:69 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:69 
msgid "６、と。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:70 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:70 
msgid "困るなお姉さん。そう言うのは食べる前に\n気付いてくれないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:71 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:71 
msgid "む―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:72 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:72 
msgid "それで、どうするんだ？\nまさかこのまま無銭飲食する気か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:73 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:73 
msgid "ひとまずツケといてくれ。\nすぐに財布を取ってくる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:74 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:74 
msgid "信用ならないな。\nそのまま逃げる気じゃないだろうな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:75 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:75 
msgid "私が食い逃げした時は時定高校に\n捜しに来ればいい。私はそこの教師だからな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:76 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:76 
msgid "時定高校の？　先生だったのかい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:77 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:77 
msgid "で、一旦ツケでいいか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:78 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:78 
msgid "ああ。暫く待ってあげよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:79 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:79 
msgid "うむ。ではまた後で。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:80 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:81 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:81 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0028:82 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:82 
msgid "初めて教師になって良かったと思った……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:83 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:83 
msgid "財布は無いので、\n今あるものを代金代わりにできないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:84 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:84 
msgid "そういうのはやってないんだが……\n……ちなみに、何があるんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:85 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:85 
msgid "そうだな、まずは……睡眠薬。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:86 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:86 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:87 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:87 
msgid "今なら亳れ薬もセットでついてくる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:88 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:88 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:89 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:89 
msgid "後は……回路ショ―ト君か。\nまあ強力なスタンガンと思ってくれればいい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:90 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:90 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:91 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:91 
msgid "どれも代金以上の価値はあると思うが、\nどうだろうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:92 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:92 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:93 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:93 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:94 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:94 
msgid "怖っ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0028:95 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0028:95 
msgid "！？\n"
msgstr ""
