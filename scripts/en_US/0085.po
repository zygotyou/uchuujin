#  speaker 
#  warnings: 0 
#: sc/0085:0 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0085:0 
msgid "それでは小テストを始めます。\nどうぞっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:1 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0085:1 
msgid "（よ―し、頑張るぞ―）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:2 
msgid "はかせ"
msgstr "Professor"

#  text 
#  warnings: 0 
#: sc/0085:2 
msgid "ん〜〜？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:3 
msgid "テレビでもエアコンでもないし……\nこのボタン、何のボタンだったっけ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:4 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:4 
msgid "ん〜〜〜〜？？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:5 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:5 
msgid "（あれ？　この問題は……\n　昨日勉強したはずなんだけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:6 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:6 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:7 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0085:7 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0085:8 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:8 
msgid "あれ？\nどうして、ワンちゃんがこんなところに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:9 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:9 
msgid "ダメですよ―。\nおうちに帰りなさ〜い。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0085:10 
msgid "いいな―、この人。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0085:11 
msgid "ああ、こういう優しそうな人は、\n体罰とか絶対しないよな―。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0085:12 
msgid "ははは、幻想抱きすぎだろ？\nでも、その通りだろうな―。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0085:13 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0085:13 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0085:14 
msgid "何でアンテナが！？\nあ！　頭をなでられてる！？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0085:15 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0085:15 
msgid "優しさが、まさかの体罰化―――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:16 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:16 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:17 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:17 
msgid "（な、なんで腕からロ―ルケ―キが！？\n　どうしてこのタイミングで！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:18 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:18 
msgid "（と言うか、またロ―ルケ―キが入ってる！\n　いつの間に補充を！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:19 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:19 
msgid "（と、とにかく、\n　見られる前にしまわないと！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:20 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:20 
msgid "（ふぅ……これで良し）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:21 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:21 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0085:22 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:22 
msgid "（良くなかった！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:23 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:23 
msgid "（ま、まさか、今のを先生に見られて……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:24 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0085:24 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:25 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:25 
msgid "（ぎゃああああ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:26 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:26 
msgid "（まだいた！\n　見てしまった人が、まだいた―！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:27 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:27 
msgid "（どうしよう！　どうすれば？\n　これはテストより難題では……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:28 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:28 
msgid "（と、とんでもない物をみてしまった！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:29 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:29 
msgid "（僕の目の錯覚だろうか？\n　でも、さっきは確かに……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:30 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:30 
msgid "（どうすれば……どうすれば……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:31 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:31 
msgid "（………………）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:32 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:32 
msgid "先生！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:33 
msgid "はっ、はい！！\nどうしましたか、中之条くん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:34 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:34 
msgid "見てしまったんです！\n東雲さんの弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:35 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:35 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:36 
msgid "（ど……どうしよ―！\n　わたしがロボットだってバレちゃう！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:37 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:37 
msgid "それは……本当ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:38 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:38 
msgid "はい、見てしまったんです。\n東雲さんの弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:39 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:39 
msgid "（ああああっ、もう終わりだ―！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:40 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:40 
msgid "弌弌テストを！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:41 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:41 
msgid "って、テスト？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:42 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:42 
msgid "はい、見たのはテストです。\n……カンニングです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:43 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:43 
msgid "そんな！\n先生、中之条くんが見たのは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:44 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:44 
msgid "……わかりました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:45 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:45 
msgid "（……！？　もしかしたら二人は、\n　見なかったことにしてくれようと？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:46 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:46 
msgid "でも、中之条くん。今はテスト中ですから、\nえっと……先生と廊下でお話ししましょう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:47 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:47 
msgid "はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:48 
msgid "（弌弌うん、これでよかったんだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:49 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:49 
msgid "（これでよかったんですよね弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:50 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:50 
msgid "んぎゃああああああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:51 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:51 
msgid "きゃ―――――――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:52 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0085:52 
msgid "東雲なのが出てくると思ったら、\nまさか、こんなことになるとは弌弌。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:53 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:53 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0085:54 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:54 
msgid "逃げろっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:55 
msgid "ここは？\n僕は一体なにを……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:56 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:56 
msgid "えっと、私は何を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:57 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:57 
msgid "あっ、中之条くんどうしたんですか？\nテスト始まってますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:58 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:58 
msgid "はい、すみません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:59 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:60 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:60 
msgid "テスト、受けようっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:61 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:61 
msgid "（あ、そうだ、テスト！\n　小テストだから急がないと時間が弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:62 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:62 
msgid "（でも、二人の視線が気になって、\n　テストなんて受けられません！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:63 
msgid "（ああ……どうすれば……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:64 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:64 
msgid "（あっ、そうだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:65 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:65 
msgid "桜井先生、ちょっと質問があります！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:66 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:66 
msgid "はっ、はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:67 
msgid "えっとですね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:68 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:68 
msgid "先生、ロ―ルケ―キは好きですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:69 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:69 
msgid "！？\nす、好きですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:70 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:70 
msgid "良かった！\nなら後で、中之条くんも呼んで食べませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:71 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:71 
msgid "え、えっと……\nそれはつまり……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:72 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:72 
msgid "何も見ていませんよね？\n見ていませんよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:73 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:73 
msgid "……はい。\n"
msgstr "...yes.\n"

#  speaker 
#  warnings: 0 
#: sc/0085:74 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:74 
msgid "ありがとうございます。\nテスト頑張ります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:75 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:75 
msgid "……頑張ってください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:76 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:77 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:77 
msgid "（秘密を守る為とは言え、\n　賄賂を渡すことになるなんて……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:78 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:78 
msgid "（ごめんなさい、はかせ……\n　私は悪い子です……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:79 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:79 
msgid "あれ〜？　ロ―ルケ―キどこに置いたっけ？\n阪本知ってる〜？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:80 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0085:80 
msgid "それなら、またおまえが悪さをして、\n娘の腕の中にしまってただろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:81 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:81 
msgid "あれれ、そうだったっけ〜？\nあははははは〜！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:82 
msgid "ナレ―タ―お姉さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:82 
msgid "その後、事情が判明して、\nお説教の時間が準備されたと言います。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:83 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:83 
msgid "（あ、でも下手に騒ぐと\n　みんなに広がる気がする……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:84 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:84 
msgid "（もうロ―ルケ―キはしまったんだから\n　普通にしてればいいんだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:85 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:85 
msgid "（だって腕からロ―ルケ―キが\n　出てくるなんて誰も信じないし！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:86 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:86 
msgid "（そうとわかればテストの続きをしようっと）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:87 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:87 
msgid "あ、あ、あの、東雲さん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:88 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:88 
msgid "はい？　なんでしょうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:89 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:89 
msgid "あの、その……ロ―ルケ―キは腕の中じゃ\nなくて、ちゃんとした入れ物に……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:90 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:90 
msgid "またまた〜。腕にロ―ルケ―キが\n入るわけないじゃないですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:91 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:91 
msgid "ええ？　あ、でも、あの……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0085:92 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0085:92 
msgid "それじゃあまるで私が\nロボットみたいじゃないですか〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
