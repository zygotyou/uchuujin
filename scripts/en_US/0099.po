#  speaker 
#  warnings: 0 
#: sc/0099:0 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0099:0 
msgid "あ、部長。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:1 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:1 
msgid "おお、桜井くんか。\nこんなとこで奇遇だな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:2 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:2 
msgid "部室以外で会うのって珍しいですよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:3 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:3 
msgid "あ、部室といえば、\n今日の部活ってどうするんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:4 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:4 
msgid "どうって、別にいつも通りだけど？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:5 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:5 
msgid "あの……そろそろ\n囲碁サッカ―の練習しませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:6 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:6 
msgid "囲碁サッカ―か―。\nだってオレぜんぜん知らないしな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:7 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:7 
msgid "オレが教えますよ。\nほら、丁度ボ―ルと碁石も持ってますし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:8 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:8 
msgid "試しにリフティングしてみてください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:9 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:9 
msgid "普通のリフティングでいいのか？\nよ―し。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:10 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:10 
msgid "よっ、はっ……おおっと！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:11 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:11 
msgid "あっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0099:12 
msgid "キャンッ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:13 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:13 
msgid "うわわっ、大丈夫か！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0099:14 
msgid "うお、こっちに飛んできたぞ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0099:15 
msgid "これだから初心者は……次は上手くやれよ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0099:16 
msgid "ああ、空から祈っていようぜ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0099:17 
msgid "空から？\nま、俺たちは宇宙にいるけど……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0099:18 
msgid "って、そういうことか―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:19 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:19 
msgid "ちょっと見ててくださいね。\nまずはこうしてリフティングをして……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:20 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:20 
msgid "で、リフティングから碁石。\nよっ……はっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:21 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:21 
msgid "ここまでは前にも見せましたよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:22 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:22 
msgid "ああ、その時はえ―っと、\n一人ジャ―マンだっけ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:23 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:23 
msgid "そう、あの時やったのは\nナダレ定石三連星なので一人ジャ―マン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:24 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:24 
msgid "ですけど、別に他のことしてもいいんですよ。\nとにかく基本はリフティングから碁石。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:25 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:25 
msgid "で、そこから碁石とボ―ルの特性を考えて\nどこで大技を出すかを考えるんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:26 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:26 
msgid "ふ―む……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:27 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:27 
msgid "なるほど、\nそれで他にはどんな事に注意するんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:28 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:28 
msgid "あ、はい！\nあとは小技からの連続というのもあって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:29 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:29 
msgid "と、基本はこんな感じですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:30 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:30 
msgid "なるほど。ありがとう、勉強になったよ。\n囲碁サッカ―って面白そうだな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:31 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:31 
msgid "でしょ！？\nこんなに奥深いスポ―ツありませんよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:32 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:32 
msgid "オレもちょっと練習してみようかな。\n桜井くん、教えてくれるか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:33 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:33 
msgid "はいっ、もちろんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:34 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:34 
msgid "ちょっとした技ぐらいは\n出せるようになりたいし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:35 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:35 
msgid "部長、運動神経いいみたいですし\nすぐできますよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:36 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:36 
msgid "そうか？\nあ、休み時間終わりだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:37 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:37 
msgid "ホントですね。\nそれじゃ、また部活の時間に！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:38 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:38 
msgid "おう！　そうだ、桜井くん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:39 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:39 
msgid "なんですか？\n"
msgstr "What is it?\n"

#  speaker 
#  warnings: 0 
#: sc/0099:40 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:40 
msgid "今日は関口も入れてゲ―ムやろうぜ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:41 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:41 
msgid "ええ―――――っ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:42 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:42 
msgid "ちょっとオレもやってみていいかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:43 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:43 
msgid "はい、いいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:44 
msgid "まずはリフティングだっけ？\nよ……っと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:45 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:45 
msgid "そのままボ―ルをキ―プして、\nそこに碁石っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:46 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:46 
msgid "おぉっ！？\nよっ、はっ……あぁっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:47 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:47 
msgid "意外と難しいな、\n碁石が入るとリフティングが続かないし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:48 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:48 
msgid "最初はそんな感じですよ。\nここからゆっくり慣れていけばいいんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:49 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:49 
msgid "そうだなぁ、たまには悪くないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:50 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:50 
msgid "１年半くらい練習すれば\n試合に出られるようになると思いますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:51 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:51 
msgid "……１年半か。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:52 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:52 
msgid "はい、部長ならそれくらいあれば十分……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:53 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:53 
msgid "その頃にはオレ、卒業前だぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:54 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:54 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0099:55 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:56 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:56 
msgid "す、すみません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:57 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:57 
msgid "なあ、桜井くん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:58 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:58 
msgid "はい、なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:59 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:59 
msgid "これ、碁石のかわりに\nリバ―シでやっちゃダメなのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:60 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:60 
msgid "！！！！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:61 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:61 
msgid "ほら、似たような白黒だしさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:62 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:62 
msgid "ぶ、部長ッ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:63 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:63 
msgid "もがっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:64 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:64 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:65 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:65 
msgid "……良かった、誰にも聞かれてない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:66 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:66 
msgid "きゅ、急にどうしたんだ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:67 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:67 
msgid "いいですか？\nリバ―シの話はダメです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:68 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:68 
msgid "先輩はビギナ―だから仕方ないですけど、\n経験者に聞かれたら大変ですからね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:69 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:69 
msgid "（な、なんでだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:70 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:70 
msgid "誰も聞いてなくてよかった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0099:71 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0099:71 
msgid "（碁石は良くて、リバ―シはダメなのか……）\n"
msgstr ""
