#  speaker 
#  warnings: 0 
#: sc/0073:0 
msgid "男子生徒（男子生徒１）"
msgstr "Male student #1"

#  text 
#  warnings: 0 
#: sc/0073:0 
msgid "んじゃ、オレ部活あるから―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:1 
msgid "田中"
msgstr "Tanaka"

#  text 
#  warnings: 0 
#: sc/0073:1 
msgid "お―、また明日な―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:2 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:2 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0073:3 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:3 
msgid "……よし、ここなら誰も居ないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:4 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:4 
msgid "いつまでもアフロに知らない物が\n入っているのは落ち着かないからな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:5 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:5 
msgid "さ―て、\nそんなわけでアフロの大掃除といくか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:6 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:6 
msgid "え―っと、まずは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:7 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:7 
msgid "！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:8 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:8 
msgid "うわっ、うわぁっ！？\n何か手握られた！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:9 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:9 
msgid "うそっ、何か居る！？\nう……うわあぁぁぁぁっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:10 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:10 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:11 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:11 
msgid "な―んだ、マネキンの腕か。\nびっくりした―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:12 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:12 
msgid "……なんだ？\n腕から音が……ええ――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:13 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:13 
msgid "……あ、危な―いっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:14 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0073:14 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0073:15 
msgid "地球人の攻撃によりアンテナ破損！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0073:16 
msgid "まさか急に動き出すとは……\n予想外の攻撃に反応が遅れたぜ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0073:17 
msgid "ロケットパンチはアニメやゲ―ムの中だけだと\n思っていたのが、仇になっちまった……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0073:18 
msgid "アニメやゲ―ムみたいなことが起きるなら、俺、\n最後ぐらい女の子と仲良くなりたかったよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:19 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:19 
msgid "え―っと……\nこれはさっきのコケシっぽいのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:20 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:20 
msgid "こんな物、いつ入ったんだ？\n名前も書いてないしなあ……ま、次だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:21 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:21 
msgid "次は……どら焼きか。\nこれはオレのだよな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:22 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:22 
msgid "雑誌に……大工ストアのポイントカ―ド……\nタケノコ！？　何でなまものまで！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:23 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:23 
msgid "何だこりゃ、炊飯器？\n……わけわかんね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:24 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:24 
msgid "って、この量！\n明らかに髪に入りきらないだろ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:25 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:25 
msgid "……ん？\nどこからか視線が弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:26 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:26 
msgid "（うわ、アレ中村先生じゃん！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:27 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:27 
msgid "（こっち見てるっていうか見られてた！？\n　ヤベッ、どうしようかな……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:28 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:28 
msgid "（ち、近づいてきたぞ！　っていうか\n　手に持ってる小さなビスは何だよ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:29 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:29 
msgid "（あ―もう、わけわかんね―！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:30 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:30 
msgid "（に、逃げろっ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:31 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0073:31 
msgid "あっ、待て！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:32 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:32 
msgid "（どんなもんだ！\n　こう見えて走りには自信が弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:33 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:33 
msgid "ぬおおおおっ！？\nな、なんだコレ！　落とし穴！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:34 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:34 
msgid "よし、これで追いつけ弌弌しまった！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:35 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:35 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:36 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:36 
msgid "こら、田中！\n私まで落ちてしまったぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:37 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:37 
msgid "ええっ！？　オレが怒られるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:38 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:38 
msgid "ああ、お前が逃げなければ、\n自分で掘った穴になんて落ちなかった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:39 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:39 
msgid "自分で掘った！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:40 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:40 
msgid "弌弌あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:41 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:41 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:42 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:42 
msgid "ところでセンセ―、\nこれ、どうやって抜ければいいんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:43 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:43 
msgid "……どうやればいいか、か。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:44 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:45 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:45 
msgid "誰か―、助けてくれ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:46 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:46 
msgid "ええええええっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:47 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:47 
msgid "（よし、ここはまた誤魔化すしか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:48 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:48 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:49 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:49 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:50 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:50 
msgid "ど、どら焼き食べる！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:51 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:51 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:52 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:52 
msgid "コ、コ―ヒ―を飲まないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:53 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:53 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:54 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:54 
msgid "……あ、えっと……\nはい、どら焼きです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:55 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:55 
msgid "（……あっれ―？\n　何でこんな事になってんだろ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:56 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:56 
msgid "あ、ああ、頂こう。\nお前も、ほら。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:57 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:57 
msgid "（……まあいいや）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:58 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:58 
msgid "あちっ、これまだちょっと熱いな。\n……このコ―ヒ―ってセンセ―が亞れたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:59 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:59 
msgid "ああ、今日は間違えないように、\n別の水筒に入れて持ち歩いていたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:60 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:60 
msgid "しかし、コ―ヒ―とどら焼きというのは\nなかなか不思議な組み合わ……せ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:61 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:61 
msgid "え？　先生！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:62 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:62 
msgid "しまった……私が飲んで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:63 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:63 
msgid "何ぃぃっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:64 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:64 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:65 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:65 
msgid "あ―、え―っと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:66 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:66 
msgid "と、とりあえず秘密は守れたぜ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:67 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:67 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:68 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:68 
msgid "セ、センセ―？\n今、もしかして見てました……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:69 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:69 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:70 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:70 
msgid "あの―、センセ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:71 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:71 
msgid "見た？　なんのことだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:72 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:72 
msgid "いや、別に何でもないよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:73 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:73 
msgid "（ラッキ―！　助かったぜ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:74 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:74 
msgid "弌弌よし。\nやはり、これのビスだったか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:75 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:75 
msgid "あ、その炊飯器、ネジが抜けてたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:76 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:76 
msgid "炊飯器？　これは遠心分離機だぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:77 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:77 
msgid "いつの間にか理科実験室から\n無くなっていたのだが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:78 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:78 
msgid "へ―見つかって良かったね。\nそれ、かなり高そうだもんな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:79 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:79 
msgid "ああ、凄く高い。\n無くしたのがバレて減俸されたからな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:80 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:80 
msgid "え……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:81 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:81 
msgid "しかし、灯台下暗しだな。\nまさかこんな場所に持ち出されていたとは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:82 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:82 
msgid "ええっ！？\nいや、確かに持ってましたけど……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0073:83 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0073:83 
msgid "……さて校舎に戻ろうか。\nこの件について色と話を聞きたいからな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:84 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:84 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0073:85 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0073:85 
msgid "（ぜんぜんラッキ―じゃなかった……）\n"
msgstr ""
