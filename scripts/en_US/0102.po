#  speaker 
#  warnings: 0 
#: sc/0102:0 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0102:0 
msgid "大福―、大福―。\n大福はいらんかね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:1 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0102:1 
msgid "父さん。ただいま。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:2 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:2 
msgid "おお、帰ってきたか。\n悪いが店の手伝いをしてくれんか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:3 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:3 
msgid "うん。じゃあ、荷物置いてくるから\nちょっと待ってて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:4 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:4 
msgid "ああ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:5 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:5 
msgid "よ―し、がんばって手伝うぞ。\nお父さ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:6 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:6 
msgid "はぁ……大福の売り上げが悪いな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:7 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:7 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0102:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:8 
msgid "一体どうすれば\n売れるようになるんだろうか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:9 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:9 
msgid "（父さん……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:10 
msgid "……父さん。\n手伝い始めるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:11 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:11 
msgid "おっ、来たか。では早速、\n大福くんで接客を頼んだぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:12 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:12 
msgid "わ、わかった。がんばるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:13 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:13 
msgid "（本当は嫌だけど……\n　あんなこと聞いたら断れないや）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:14 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:14 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0102:15 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:15 
msgid "大福、売り上げが落ちてるのか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:16 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:16 
msgid "いつもはそんなこと感じさせないのに……\nそれだけ経営が危ないのかもしれないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:17 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:17 
msgid "どうしたらいいんだろう。\n今の僕にできることは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:18 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:18 
msgid "……とりあえず今は\n大福くんになることか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:19 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:19 
msgid "よし。恥ずかしいけど、\n顔が見えないのが救いだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:20 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0102:20 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:21 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:21 
msgid "あっ、校長先生。こんにちは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:22 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:22 
msgid "ぐあっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:23 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:23 
msgid "あっ！？　す、すみません！\n被り物が大きくて距離感が！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0102:24 
msgid "うわああ！\n今の頭突きでアンテナが折れたぁ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0102:25 
msgid "くそぉ！　かわいそうだから\n大福買ってあげようと思っただけなのに！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0102:26 
msgid "返せ！　俺の優しさを返せ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0102:27 
msgid "返って来てるじゃないか！\n仇となってな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:28 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0102:28 
msgid "私に良い考えがあるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:29 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:29 
msgid "わっ！　あ、相生さん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:30 
msgid "ごめんね。今の話、聞いちゃった。\nなんだか大変みたいだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:31 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:31 
msgid "う、うん。僕も今知ったんだけどね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:32 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:32 
msgid "ところで、さっき良い考えがあるって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:33 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:33 
msgid "フフフ、知りたい？\n知りたいでしょ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:34 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:34 
msgid "う、うん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:35 
msgid "いいでしょういいでしょう。\nじゃ、ちょっとその被り物貸して。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:36 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:36 
msgid "えっ？　これ？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0102:37 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0102:37 
msgid "そうそう。せっかくのマスコットキャラが\nこれじゃあ台無しだからね〜\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:38 
msgid "ここをこうやって……\nうんうん、良い感じ。勢いよくガ―ッと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:39 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:39 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:40 
msgid "ジャジャ―ン！\nこれを被れば客寄せ効果もバッチリ―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:41 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:41 
msgid "…………！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:42 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:42 
msgid "な、何コレ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:43 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:43 
msgid "大福マン！　カッコイイでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:44 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:44 
msgid "か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:45 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:45 
msgid "（カッコよくない！　なさすぎる！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:46 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:46 
msgid "（いや違う、カッコイイ悪いの前に……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:47 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:47 
msgid "（絵が下手すぎる！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:48 
msgid "（どうしてこれで客が増えると思えるんだ？\n　小さな子供が見たら泣いてしまうのでは？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:49 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:49 
msgid "（いや、でも相生さんはうちのことを思って\n　真剣に考えてくれたんだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:50 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:50 
msgid "（礼も言わず、ただ下手だと言うのは\n　あまりにも失礼じゃないのか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:51 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:51 
msgid "ね―ね―、どう？\n大福マン、カッコイイと思わない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:52 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:52 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:53 
msgid "……あ―、そうだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:54 
msgid "でしょ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:56 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:56 
msgid "（ここは相生さんを立てるんだ。\n　それが僕にできるせめてもの礼だ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:57 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:57 
msgid "ね―ね―、早く被ってみてよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:58 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:58 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:59 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:59 
msgid "（コ、コレを被って客引きをしろと！？\n　そんなことすれば客足は更に遠のく……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:60 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:60 
msgid "（だが、くっ……今の僕は\n　相生さんに従うしか……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:61 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:61 
msgid "おお―！　いいねいいね！\nそれで店の前に立ってみてよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:62 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:62 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:63 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:63 
msgid "……い、いらっしゃ―い。\n美味しい大福あるよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:64 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:64 
msgid "あはははは……あ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:65 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:65 
msgid "？　相生さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:66 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:67 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:67 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:68 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:68 
msgid "な、何これ……！！\nただの落書きじゃないか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:69 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:69 
msgid "落書きじゃないって。大福マン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:70 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:70 
msgid "だ、大福マン？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:71 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:71 
msgid "うん、大福マン。私が考えたの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:72 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:72 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:73 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:73 
msgid "ささ、それ被ってみてって。\nきっといっぱいお客さんくるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:74 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:74 
msgid "ダメだって。こんなんじゃ更に\nお客さんが減るに決まってるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:75 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:75 
msgid "いいから試しに被ってみなって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:76 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:76 
msgid "う―ん……ダメだと思うけどなぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:77 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:77 
msgid "おっ、いいね―！　似合ってるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:78 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:78 
msgid "そ、そうかな……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:79 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:79 
msgid "うん。早速お客さんを呼び込んでみてよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:80 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:80 
msgid "あ、うん。\n……大福、大福―。いかがですか―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:81 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:81 
msgid "この……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:82 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:82 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:83 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:83 
msgid "バカ息子がぁぁ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:84 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:84 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:85 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:85 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:86 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:86 
msgid "な、何コレ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:87 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:87 
msgid "大福マン！\n私の自信作だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:88 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:88 
msgid "自信作……？　これが……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:89 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:89 
msgid "むっ……なにさ、文句があるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:90 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:90 
msgid "だって、自信ありげに描いたわりには\n絵が下手だし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:91 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:91 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:92 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:92 
msgid "まあ絵が下手なのはいいとして、\nどうしてアッカンベ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:93 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0102:93 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0102:94 
msgid "中之条"
msgstr ""

#  text 
