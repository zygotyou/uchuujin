#  speaker 
#  warnings: 0 
#: sc/0078:0 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0078:0 
msgid "う―む。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:1 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:1 
msgid "（最近、客の数が落ち込んでいる。\n　大福フェアの効果も減り気味だ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:2 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:2 
msgid "（不景気のせいかと思ったが、\n　どうやらそうでもなさそうだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:3 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:3 
msgid "……くっ、大工カフェめ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:4 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:4 
msgid "あそこは相変わらず大盛況。\nむしろ、うちの客を取られてる気がする。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:5 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:5 
msgid "しかし、大福がコ―ヒ―とケ―キなんぞに\n負けるはずが無い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:6 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:6 
msgid "となれば、私の宣伝に問題があるに違いない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:7 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:7 
msgid "……よし、少しアンケ―トをとってみよう。\n丁度いいから、そこの人にするか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:8 
msgid "すいません！　ちょっといいですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:9 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0078:9 
msgid "はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:10 
msgid "アンケ―トに答えて欲しいんですが……\n大福はケ―キに負けていると思いますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:11 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:11 
msgid "大福ですか？　そうですねぇ。\n私は大福の方が好きですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:12 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:12 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:13 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:13 
msgid "ですが、昨日大工カフェで食べたケ―キも\n大福と同じくらい美味しかったですな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:14 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:14 
msgid "大福を仞めるな！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:15 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:15 
msgid "！？\n"
msgstr "!?"

#  text 
#  warnings: 1 
#: sc/0078:16 
msgid "えええ――！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0078:17 
msgid "オ、オッサンは大丈夫か！？\n急いで確認を弌弌\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0078:18 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0078:18 
msgid "えええ――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:19 
msgid "みお姉"
msgstr "Yoshino"

#  text 
#  warnings: 0 
#: sc/0078:19 
msgid "私ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:20 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:20 
msgid "はい。私、実はここで\n大福フェアをやってる者なんですが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:21 
msgid "率直に聞きますが、うちの大福フェアは\n大工カフェと比べてどうですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:22 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:22 
msgid "う―ん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:23 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0078:24 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:24 
msgid "地味？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:25 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:25 
msgid "！！！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:26 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:26 
msgid "（そんな、まさか大福の最大の武器が\n　最大の欠点になっているとは……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:27 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:27 
msgid "（だが……しかし！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:28 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:28 
msgid "じ、地味の何が悪い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:29 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:29 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:30 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:30 
msgid "そうだ、地味なんじゃない。\nシックで落ち着いた、日本的なデザインだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:31 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:31 
msgid "それに味は抜群なんだ！\nほら、あんたも食べてみてくれ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:32 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:32 
msgid "い、いただきます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:33 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:33 
msgid "……あ、ホントだ、おいしい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:34 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:34 
msgid "そうだろう！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:35 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:35 
msgid "そうさ、何を間違えていたんだろう。\n大福はこのままでいいんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:36 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:36 
msgid "私はこれからも\nこのままのスタイルを貫くぞ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:37 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:37 
msgid "でも……それって\n問題の解決になってませんよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:38 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:39 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:39 
msgid "くっ……やはり大福は大福。\n地味という宿命からは逃れられないのか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:40 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:40 
msgid "ん―、でも私大福好きですよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:41 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:41 
msgid "本当かね！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:42 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:42 
msgid "やっぱり緑茶には和菓子の方があいますし、\nあんこっておいしいですし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:43 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:43 
msgid "そ、そうか！\n大福の良さをわかってくれるか！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:44 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:44 
msgid "なんて良いお嬢さんだ！\nありがとう、何だか救われた気分だよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:45 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:45 
msgid "いえいえ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:46 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:46 
msgid "そうだ、良かったら寄ってってくれ！\n出来る限りサ―ビスさせてもらうよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:47 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:47 
msgid "あ、いいです。\n今から友達と大工カフェでお茶する所なんで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:48 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:48 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:49 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:49 
msgid "じゃあ、そろそろ時間なんで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:50 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:50 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:51 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:51 
msgid "い、良いお嬢さん、だった……はずだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:52 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:52 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:53 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:53 
msgid "そ、その。\nじゃあ次の質問を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:54 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:54 
msgid "はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:55 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:55 
msgid "反対に大福のいい所を\n思いつくだけ挙げてみてくれないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:56 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:56 
msgid "ん―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:57 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:57 
msgid "特に無いかな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:58 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:58 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:59 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:59 
msgid "そ、そうか……。\nありがとう、もういいよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:60 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:60 
msgid "は―い、それじゃ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:61 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:61 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0078:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0078:62 
msgid "……大福は無力なんだろうか。\n"
msgstr ""
