#  speaker 
#  warnings: 0 
#: sc/0129:0 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0129:0 
msgid "……む？\nあそこにいるのは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:1 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:1 
msgid "あそこにいるのは前に代金を踏み倒した客！？\nこ、これは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:2 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:2 
msgid "チャンス！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:3 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:3 
msgid "むむ、いかん。\n急いであとを追わなければ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:4 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:4 
msgid "ここは……時定、高校？\n学生服だったからここで間違いなさそうだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:5 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:5 
msgid "フフ、相手が高校生であろうと代金は代金。\nしっかりと払ってもらいましょうか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:6 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:6 
msgid "（しかし、どうする？\n　ここでの私はアウェイの立場）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:7 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:7 
msgid "（土地勘ある相手に逃走されたら……\n　二度と料金は回収できんかもしれぬっ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:8 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:8 
msgid "う〜む。\n何か良い手は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:9 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:9 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0129:10 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:10 
msgid "閃いた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:11 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:11 
msgid "逃げるなら捕えてやろうホトトギス。\n今度こそ逃がさん……フフフ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:12 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:12 
msgid "ごめんくださ―い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:13 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0129:13 
msgid "は―い？　……えっ？　えっ？\nあ、あの、スイマセン、ご父兄の方ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:14 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:14 
msgid "この学校に、あの、モヒカンの子がいますな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:15 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:15 
msgid "モヒカン……あっ、はい。\nあ……中之条くんのおばあさんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:16 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:16 
msgid "あの子を呼んできてもらえませんかね？\n逃げないように、こう、ギュッと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:17 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:17 
msgid "ギュ、ギュッと……ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:18 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0129:18 
msgid "桜井先生、どうしました？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:19 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:19 
msgid "あっ、高崎先生！\nこちら中之条くんのおばあさんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:20 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:20 
msgid "あ、そうでしたか。これはこれは。\n今日は何のご用でこちらまで？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:21 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:21 
msgid "それが……中之条くんを、ギュッ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:22 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:22 
msgid "……って、逃げないように\n連れてきてほしいそうですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:23 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:23 
msgid "ギュッ！　……ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:24 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:24 
msgid "（ははあ……中之条め、\n　何か叱られるようなことをやったな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:25 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:25 
msgid "わかりました！　任せてください！\nお婆さん、きっちり叱ってやってください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:26 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:26 
msgid "え？　あの……それって……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:27 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:27 
msgid "ありがちな話ですよ。\n俺、中之条を連れてきますから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:28 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:28 
msgid "ギュッと、よろしくお願いします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:29 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:29 
msgid "大丈夫です！\n逃げないように見張ってますから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:30 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:30 
msgid "良かったですね。\n多分、すぐに連れてきてくれますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:31 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:31 
msgid "ええ……これで踏み倒した料金を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:32 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:32 
msgid "え？　踏み倒す？　料金？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:33 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:33 
msgid "（し、しまった……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:34 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0129:34 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:35 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:35 
msgid "！？　そ、そう！　この猫が前に\nわたしの宝物を踏んで倒して壊したんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:36 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:36 
msgid "（えええ―――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:37 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:37 
msgid "えっ！？　そ、そうなんですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:38 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:38 
msgid "（ちょ、おい！　信じるのかよ！\n　明らかに嘘じゃね―か！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:39 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:39 
msgid "成敗してくれる！　キエェエエ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:40 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:40 
msgid "（うおおおお――！？　や、やめろ―っ！！）\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0129:41 
msgid "あ―っ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0129:42 
msgid "やばっ、壊れた！\n何してくれてんだ、あのばあさん！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0129:43 
msgid "くそっ！　システムが暴走を！\nこのままだと弌弌！\n"
msgstr ""

#  speaker 
#  warnings: 3 
#: sc/0129:44 
msgid "＆＆"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0129:44 
msgid "うあああああああああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:45 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:45 
msgid "（……あれ？\n　中之条くんのおばあさん……じゃないの？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:46 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:46 
msgid "（う……どうやって切り抜けるべきか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:47 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:47 
msgid "お待たせしました！\n中之条を連れてきましたよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:48 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:48 
msgid "（よし！　間に合った！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:49 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0129:49 
msgid "失礼します。\n……僕のお婆ちゃんが来てるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:50 
msgid "ああ、ほら、そこだ。\n何をしたか知らんが、ちゃんと謝るんだぞ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:51 
msgid "そう言われても……僕は何も……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:52 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:52 
msgid "してるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:53 
msgid "あ……イ、イタコのお婆さん……！\nどうしてここに……！？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0129:54 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0129:54 
msgid "お兄さん、５千円。\n出張料金と延滞料金を入れて２万円。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:55 
msgid "う、うう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:56 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:56 
msgid "出張料金？　延滞料？\n……えっ？　中之条のおばあさんでは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:57 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:57 
msgid "早く払ってください。\nあと５分で延滞料金が割り増されます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:58 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:58 
msgid "そんなこと……そんなこと弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:59 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:59 
msgid "できません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:60 
msgid "桜井先生＆高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:60 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0129:61 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:61 
msgid "そこな人！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:62 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:62 
msgid "お、俺ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:63 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:63 
msgid "そうじゃ！　早くアイツを捕まえなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:64 
msgid "俺がですか！？　けど、なんで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:65 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:65 
msgid "逃げられんように見張っとると言ったのは\nどこのどなたですかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:66 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:66 
msgid "うう、そ、それは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:67 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:67 
msgid "ほれ！　早うせんかぁっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:68 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:68 
msgid "は、はい！　わかりましたぁ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:69 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:69 
msgid "……まったく。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:70 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:70 
msgid "……行っちゃった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:71 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:71 
msgid "あのお婆さん……結局、誰だったんだろう？\n中之条くんのお婆さんじゃなさそうだし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:72 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:72 
msgid "だとしたら一体……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:73 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:73 
msgid "あっ……そっか。\nあのおばあさんは、きっと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:74 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:74 
msgid "中之条くんの、ひいおばあさんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:75 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:75 
msgid "あっ！？　こら！　何やっとるんじゃ！\n逃がさないように見張れと言ったじゃろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:76 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:76 
msgid "えっ！？　あ、すみません！\nで、ですが……しかし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:77 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:77 
msgid "……あの。\n本当に中之条くんのおばあさんなんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:78 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:78 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0129:79 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:79 
msgid "ん？　そう言えばさっき\n中之条はイタコって言ってませんでしたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:80 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:80 
msgid "あ……そういえば……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:81 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:81 
msgid "お婆さん……嘘、ついてます？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:82 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:82 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:83 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:83 
msgid "嘘……ついたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:84 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:84 
msgid "つきましたよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:85 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:85 
msgid "……………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0129:86 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:86 
msgid "……はい。\n"
msgstr "...yes.\n"

#  speaker 
#  warnings: 0 
#: sc/0129:87 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:87 
msgid "さらばっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:88 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:88 
msgid "ああっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:89 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0129:89 
msgid "ちょっ！？　待て！　止まれ―っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0129:90 
msgid "桜井先生＆高崎先生"
msgstr ""

#  text 
