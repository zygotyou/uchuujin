#  speaker 
#  warnings: 0 
#: sc/0093:0 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:0 
msgid "ウェボシ―！　笹原見なかった！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:1 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0093:1 
msgid "え、いや〜、ちょっとわかんないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:2 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:2 
msgid "そう。見かけたら教えてね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:3 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:3 
msgid "あいつ、一体どこ行ったのよ！？\n今日という今日は絶対許さないんだから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:4 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0093:4 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0093:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:5 
msgid "ちょっ……あの、た、立花さんっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:6 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:6 
msgid "は、はい。なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:7 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:7 
msgid "あ、あの……その、\n廊下は走っちゃダメですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:8 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:8 
msgid "あ、そ、そうですね。すみません……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:9 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:9 
msgid "そ、それと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:10 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:10 
msgid "え、まだ何か……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:11 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:11 
msgid "それ！　と言うか、え―っとですね。\nとても言いにくいことなんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:12 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:12 
msgid "その立花さんの手の弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:13 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:13 
msgid "……手の？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:14 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:14 
msgid "弌弌手の爪、ちょっと長いですよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:15 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:15 
msgid "えっ！？\nあ、そう言えば切ってくるの忘れてた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:16 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:16 
msgid "先生、教えてくれてありがとうございます。\n部活までには切っときますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:17 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:17 
msgid "そ、そうですね。\n割れたら危ないですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:18 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:18 
msgid "じゃあ先生、私、急いでるんで！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:19 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0093:19 
msgid "……おっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:20 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:20 
msgid "あっ……校長先生……\nす、すみません……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:21 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:21 
msgid "ほら、立花さんったら。\n廊下は走っちゃダメって言ったばかりなのに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:22 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:22 
msgid "す、すみません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:23 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:23 
msgid "はっはっは。\nこれからは気をつけてくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:24 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:24 
msgid "は―い。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:25 
msgid "なあ……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:26 
msgid "ん？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:27 
msgid "なにか変だと思わないか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:28 
msgid "何がだよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:29 
msgid "いや……何がって言われると困るが……\nとにかく変だよな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:30 
msgid "そうか？　オレにはさっぱりわからんな。\nいや、わかりたくない！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0093:31 
msgid "……なあ二人とも、そろそろ現実をみようぜ？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0093:32 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0093:32 
msgid "現実なんて見たくな――――いっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:33 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:33 
msgid "あ、あの！　も、もうひとついいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:34 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:34 
msgid "あ、はい。何か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:35 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:35 
msgid "その手の……て、手の銃です！\n学校への銃の持ち込みは禁止ですっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:36 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:36 
msgid "これは先生が預かっておきますから、\n放課後取りに来てくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:37 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:37 
msgid "……はい、すみません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:38 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:38 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:39 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:39 
msgid "笹原見つけたっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:40 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:40 
msgid "待ちなさい笹原！\n今日という今日は絶対許さないんだからね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:41 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:41 
msgid "まだ持ってた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:42 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:42 
msgid "あっ！\nす、すみません、銃は禁止でした。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:43 
msgid "いえ、わかってもらえればいいんですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:44 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:44 
msgid "あ、笹原が行っちゃう！\nこら、待ちなさ―い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:45 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:45 
msgid "よかった、銃はあぶないもんね。\n……私、少しは生徒指導らしくできたかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:46 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:46 
msgid "それにしても立花さん、あんなに一生懸命に。\n若いって良いですね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:47 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:47 
msgid "……あら？\nでも立花さん、なんで笹原くんとは逆方向へ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:48 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:48 
msgid "……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:49 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:49 
msgid "笹原―っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:50 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:50 
msgid "なんてこった！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:51 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:51 
msgid "ちょっと待ちなさい、笹原―っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:52 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:52 
msgid "あ……立花さん、廊下は走っちゃ……\nあぁ……行っちゃった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:53 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:53 
msgid "仕方ないですね……とりあえず、これを\n職員室へ持っていきましょう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:54 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:54 
msgid "あっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:55 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0093:55 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:56 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0093:56 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:57 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0093:57 
msgid "……な、なんでしょう？\nなんだか、みんなの視線が……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:58 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:58 
msgid "はぁ……笹原を見失っちゃった。\n武器もないし、どうしよう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:59 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:59 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:60 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:60 
msgid "そこの１年生！　悪いけど武器になってくれる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:61 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:61 
msgid "へっ！？　え？　あれ？\nわ、私、なんでこんな姿に―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:62 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:62 
msgid "さあ、頑張って笹原を倒すわよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:63 
msgid "え―っ！？\nなんで、どうしてこんなことに―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:64 
msgid "先生、それ返してください！\n今必要なんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:65 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:65 
msgid "ダ、ダメですよ！\nこれは没収です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:66 
msgid "そんな―。\nちょっとだけ、ちょっとだけですから―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:67 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:67 
msgid "ダメなんです―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:68 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:68 
msgid "あ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:69 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:69 
msgid "銃が！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:70 
msgid "教頭先生"
msgstr "Vice Principal"

#  text 
#  warnings: 0 
#: sc/0093:70 
msgid "ふふふ……今日のわら人形は亟身の一作。\n我ながら会心の出来だな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:71 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:71 
msgid "さて、今日はこれを燃やして、\n一段上の呪いを試してみようか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:72 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:72 
msgid "今までのやり方では効果が薄い……\n校長の髪が薄いだけに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:73 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:73 
msgid "……面白くないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:74 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:74 
msgid "それはさておき、ここまで見事な出来だと、\n燃やしてしまうのは惜しい気も……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:75 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:75 
msgid "おや？　何か、落ちてきたぞ。\n焼却炉に入ってしまったようだが……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:76 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:76 
msgid "立花さん、走っちゃダメですよ―っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:77 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:77 
msgid "ああ、火の中にっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:78 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:78 
msgid "それに何か騒がしい。\nいったい弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0093:79 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0093:79 
msgid "弌弌何が――――――っ！？\n"
msgstr ""
