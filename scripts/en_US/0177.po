#  speaker 
#  warnings: 0 
#: sc/0177:0 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0177:0 
msgid "あのモヒカンめ……どこに行きおった？\nこの中にいるのは間違いないんじゃが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:1 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:1 
msgid "……ん？　あれはこの学校の先生……？\nちょっと聞いてみようかの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:2 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:2 
msgid "あのぉ……もし、そこの人。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:3 
msgid "教頭先生"
msgstr "Vice Principal"

#  text 
#  warnings: 0 
#: sc/0177:3 
msgid "はい！？　だだ、誰ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:4 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:4 
msgid "いや……わたしは、\n通りすがりのイタコですが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:5 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:5 
msgid "（この反応……変じゃな。\n　何か見られたくないものでも……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:6 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:6 
msgid "失礼ですが、あなたはここの先生ですかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:7 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:7 
msgid "は、はい、私はこの学校の教頭ですが、\nな、何か問題でも？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:8 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:8 
msgid "な、何もないのなら……\nは、早くどこかに行ってください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:9 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:9 
msgid "（ふむ……何やら怯えとる。\n　なら、モヒカンから徴収する前に弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:10 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:10 
msgid "教頭先生……ここだけの話なんですがね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:11 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:11 
msgid "は、はい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:12 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:12 
msgid "この学校、呪われてますぞ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:13 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:13 
msgid "呪！？　なな、何を突然……\n私の学校に不吉なことを言わないでください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:14 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0177:14 
msgid "そうです！　私の学校のイメ―ジを\n下げるようなことは言わないで欲しい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:15 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:15 
msgid "いえ！　私の学校だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:16 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:16 
msgid "！？\nで、では、私たちの学校と言う事で……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:17 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:17 
msgid "ここは私の学校だ！\nあなたは別の学校の校長になりなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:18 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:18 
msgid "！？\n"
msgstr "!?"

#  speaker 
#  warnings: 0 
#: sc/0177:19 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:19 
msgid "もう回りくどいことは止めだ！\n力ずくで校長の席を譲ってもらう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:20 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:20 
msgid "きょ、教頭先生！？　ま、待ってくださ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:21 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:21 
msgid "ぎゃあああああ！！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0177:22 
msgid "あ―っ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0177:23 
msgid "やばい！　あのメガネ本気だ！\nこれじゃオッサンの命が危ない！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0177:24 
msgid "待て、危ないのは俺たちもだっ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0177:25 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0177:25 
msgid "……え？\n"
msgstr ""

#  speaker 
#  warnings: 3 
#: sc/0177:26 
msgid "＆＆"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0177:26 
msgid "うあああああああああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:27 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:27 
msgid "嘘じゃありません。ここに棲みついた悪霊が\n学校に呪いをかけているんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:28 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:28 
msgid "い、一体何を言っているんだ？\nバカバカしい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:29 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:29 
msgid "信じてください。\nそして……私に任せてください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:30 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:30 
msgid "任せる？　どういうことです？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:31 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:31 
msgid "除霊です。\n私がここから悪霊を……んん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:32 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:32 
msgid "ど、どうかしました？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:33 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:33 
msgid "い、いえ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:34 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:34 
msgid "（……って、この人が手に持ってるのは、\n　まさか……まさか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:35 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:35 
msgid "（藁人形！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:36 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:36 
msgid "（まさか……ヤバい人？\n　それとも本当に悪霊がいて……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:37 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:37 
msgid "どうしました？　顔色が悪いようですが……\n体調が悪いんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:38 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:38 
msgid "それとも……これが気になりますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:39 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:39 
msgid "（よ、予感的中……！\n　エラいのに声をかけてしもうた……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:40 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:40 
msgid "おやぁ、汗をかかれてますねぇ。\n保健室に行きますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:41 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:41 
msgid "（保健室と言って、\n　本当はどこに連れて行くつもりだ……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:42 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:42 
msgid "（油断すると命を持っていかれる……\n　料金どころの話じゃない……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:43 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:43 
msgid "（だが、やられる訳には……\n　やられる訳にはいかんのじゃ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:44 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:44 
msgid "かぁぁぁあ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:45 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:45 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:46 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:46 
msgid "この悪霊め！　来るなら来い！\n成敗してくれるわ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:47 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:47 
msgid "失敬な！　わ、私は悪霊じゃない、校長だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:48 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:48 
msgid "佶されんぞ！\nこれでもわたしはイタコの端く弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:49 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:49 
msgid "……えっ？　今さっき教頭って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:50 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:50 
msgid "あっ！？　い、いやだから教頭だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:51 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:51 
msgid "でも今、校長って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:52 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:52 
msgid "こ、校長でもいいんだ！\nそれで、お前が成敗したいのは校長か！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:53 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:53 
msgid "いや、別に校長は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:54 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:54 
msgid "だからどれなんだ！？\n校長か悪霊か藁人形か校長か！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:55 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:55 
msgid "……………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0177:56 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:56 
msgid "じゃあ、モヒカンで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:57 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:57 
msgid "いえ、結構ですから。\nそれじゃこれで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:58 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:58 
msgid "ちょっと待ってください。\n具合の悪いお婆さんをほっとけないですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:59 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:59 
msgid "結構です！　具合は悪くないですから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:60 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:60 
msgid "でしたら、なぜ顔色が……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:61 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:61 
msgid "そ、それは……んん！？　なんだこの匂い！？\nまさか悪霊の弌弌！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:62 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:62 
msgid "匂い？　……ああ、もしかしてこれですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:63 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:63 
msgid "ぐっ……やはり藁人形……\n私を呪うつもりか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:64 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:64 
msgid "は？　何を言っているのです。\nよく見なさい……ほら弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:65 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:65 
msgid "納豆！？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0177:66 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0177:66 
msgid "ええ。国産大豆１００、天然にがりのみ使用。\n……食べますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:67 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:67 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0177:68 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:68 
msgid "……タレとカラシは無いのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:69 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:69 
msgid "……あ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:70 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:70 
msgid "呪われろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:71 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:71 
msgid "いや、結構ですから。では、これにて弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:72 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:72 
msgid "ちょ、ちょっと待ってください！\nまだいいじゃないですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:73 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:73 
msgid "……いろいろと話したいことがあるんですよ。\nお付き合い願います。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:74 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:74 
msgid "（ぐっ……やはり悪霊か。\n　正に絶体絶命……どうする！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:75 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:75 
msgid "……………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:76 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:76 
msgid "ッ！？　なぜ鹿！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:77 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:77 
msgid "コ、コイツ……校長のみならず私まで！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:78 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:78 
msgid "ッ！　ッ！　ッ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:79 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:79 
msgid "これは天啓！　鹿、頼む！\nコイツを……悪霊を何とかしてくれい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:80 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:80 
msgid "ッッッッッッッ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:81 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:81 
msgid "おおおっ！　わかってくれたか！　恩に着るぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:82 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:82 
msgid "あうっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:83 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:83 
msgid "な、なんで……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:84 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:84 
msgid "し、鹿が人間の言葉をわかるはずない……\nそれに、この鹿は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:85 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:85 
msgid "ギャアッ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:86 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:86 
msgid "……人イコ―ル……敵……だ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:87 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0177:87 
msgid "……あれ？　あんなところに鹿？\nそれだけじゃないぞ、何か足もとに弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:88 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:88 
msgid "弌弌教頭先生！？\nあっちには、イタコのお婆さんも！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0177:89 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0177:89 
